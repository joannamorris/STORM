@book{taft_reading_1991-1,
  title = {Reading and the Mental Lexicon},
  author = {Taft, Marcus},
  year = {1991},
  series = {Essays in Cognitive Psychology},
  publisher = {{(1991). xi, 151pp.}},
  abstract = {(from the cover) Over the last twenty years a major area of cognitive psychology has developed centred upon research into the issue of how visually presented words are processed so that they can be read and understood. The focus has been on how words are stored in the mental lexicon and retrieved during the reading process. If we possessed no mental lexicon, we would be unable to read. This book dedicates itself to a critical evaluation of the ideas that have emerged from this body of research. The book outlines the major models of lexical processing that have been put forward in the literature, and how they explain the basic empirical findings that have been reported. It then goes on to consider the possible influence that sentential context has on lexical processing, the impact of the pronunciation of a word on its visual processing, and the role played by internal word structure (i.e., syllables and morphemes) in the recognition of a word. A connectionist style of model emerges during the course of the evaluation of these issues. The book is suitable for advanced students and researchers, and is intended to serve as a springboard for discussion and an inspiration for empirical research. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {[2340].,*Cognitive,*Cognitive Processes,*Lexical,*Lexical Access,*Reading,Access,Cognitive,Cognitive Processes [2340].,Human.,Morphology,Phonology,Processes,Pronunciation,Recognition,Semantics,Syntax,Word,word recognition},
  annotation = {References. English}
}

@incollection{taft_recognition_1979,
  title = {Developmental {{Dyslexia}}},
  booktitle = {The Science of Reading: {{A}} Handbook},
  author = {Vellutino, Frank R. and Fletcher, Jack M.},
  year = {2005},
  series = {Blackwell Handbooks of Developmental Psychology},
  pages = {362--378},
  publisher = {{Blackwell Publishing}},
  address = {{Malden}},
  doi = {10.1002/9780470757642.ch19},
  abstract = {In the present chapter, we provide a selective review of research conducted over the past two to three decades evaluating influential theories of the basic cause(s) of developmental dyslexia. Research in this area of inquiry has pursued causal explanations at the biological, cognitive, behavioral, and environmental levels of analysis (Vellutino, Fletcher, Snowling, \& Scanlon, 2004). Our primary concern in this chapter is to distinguish between manifest causes of early reading difficulties and underlying causes of such difficulties. We define manifest causes in terms of observed deficiencies in the knowledge and component skills the child must acquire in order to become a proficient reader and underlying causes in terms of biologically based cognitive deficits or environmental deficits that might impair the acquisition of those skills. Thus, we first define dyslexia and provide documentation of its primary behavioral manifestations. We proceed to discuss theories of dyslexia, specifying cognitive deficits presumed to underlie this disorder, and then go on to address the question of whether there are subtypes of dyslexia. We close with a brief discussion of research documenting the importance of distinguishing between reading problems caused primarily by biologically based cognitive deficits and reading problems caused primarily by experiential and instructional deficits. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
  isbn = {978-1-4051-1488-2},
  keywords = {Biology,Cognitive Ability,Cognitive Impairment,Dyslexia,Environment,Etiology,Reading Disabilities},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/5DT5YTRI/2005-06969-019.html}
}

@article{taft_recognition_1979-1,
