@article{wurm_conditional_2001-1,
  title = {Conditional Root Uniqueness Points: {{Psychological}} Validity and Perceptual Consequences},
  author = {Wurm, Lee H. and Ross, Shannon E.},
  year = {2001},
  journal = {Journal of Memory \& Language},
  volume = {45},
  number = {1},
  pages = {39--57},
  abstract = {L. H. Wurm (1997) introduced a construct called the Conditional Root Uniqueness Point (CRUP), which was defined as the uniqueness point of the free root of a prefixed word, given the prefix in question. Prefixed words with free roots can be divided into two mutually exclusive categories: those with the CRUP at the same phoneme as the full-form uniqueness point (UP), and those where the CRUP precedes the full-form UP by one or more phonemes. 89 college students participated in a lexical decision task and 40 college students participated in a naming task. Results of the reaction time (RT) experiments indicate a substantial processing advantage for the latter group of words. Regression analyses revealed effects of the number of auditory competitors a target word had, semantic transparency and prefix likelihood. Finally, semantic transparency, root frequency, and number of competitors had different effects, depending upon whether the CRUP of a word coincided with or preceded the full-form UP. Existing models of word recognition need to be modified to accommodate the current results. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),(Phonetic,[2326].,*Morphology,*Morphology (Language),*Phonemes,*Speech,*Speech Perception,*Word,*Word Recognition,*Words,*Words (Phonetic Units),\&,Adulthood,Auditory,Auditory \& Speech Perception [2326].,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older).,Male.,older).,Perception,Recognition,Semantics,Speech,Units),yrs},
  annotation = {Elsevier Science, US Conference Proceedings/Symposia. Empirical Study. Portions of this paper were presented at the aforementioned meeting Journal Article. English}
}

@article{xu_enlarging_2016,
  title = {Enlarging the Scope of Metaphor Studies},
  author = {Xu, Cihua and Zhang, Chuanrui and Wu, Yicheng},
  year = {2016},
  journal = {Intercultural Pragmatics},
  volume = {13},
  number = {3},
  pages = {439--447},
  issn = {1612-295X},
  doi = {10.1515/ip-2016-0018},
  urldate = {2017-10-05},
  abstract = {Many recent discussions about Deliberate Metaphor Theory (DMT) not only advance the research of metaphor theory, but also significantly contribute to a comprehensive understanding of metaphor, a ubiquitous yet complex phenomenon. In this paper we contend that DMT should not be regarded merely as a revitalization of traditional view of metaphor in rhetoric; rather, its contribution is undeniably significant and deserves its due attention. DMT is developed based on the integration of conceptual metaphor theory and discourse practice, and complements current studies of metaphor through its emphasis on consciousness and the communicative dimension, albeit there are also some inadequacies of DMT.},
  keywords = {communication,consciousness,deliberate metaphor theory,Metaphor}
}

@article{yao_as_2019,
  title = {As If by Magic: {{An}} Abrupt Change in Motion Direction Induces Change Blindness},
  shorttitle = {As If by Magic},
  author = {Yao, Richard and Wood, Katherine and Simons, Daniel J.},
  year = {2019},
  month = mar,
  journal = {Psychological Science},
  volume = {30},
  number = {3},
  pages = {436--443},
  issn = {0956-7976},
  doi = {10.1177/0956797618822969},
  abstract = {Magicians claim that an abrupt change in the direction of movement can attract attention, allowing them to hide their method for a trick in plain sight. In three experiments involving 43 total subjects, we tested this claim by examining whether a sudden directional change can induce change blindness. Subjects were asked to detect an instantaneous orientation change of a single item in an array of Gabor patches; this change occurred as the entire array moved across the display. Subjects consistently spotted the change if it occurred while the array moved along a straight path but missed it when it occurred as the array changed direction. This method of inducing change blindness leaves the object in full view during the change; requires no additional distractions, visual occlusion, or global transients; and worked in every subject tested here. This phenomenon joins a body of magic-inspired work that yields insights into perception and attention. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
  keywords = {attention,Awareness,change blindness,Distraction,failures of awareness,motion perception,Motion Perception,Visual Attention}
}

