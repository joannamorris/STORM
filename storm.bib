@article{schnyer_event-related_1997,
  title = {The {{Size}} of the {{Cross-Lingua Masked Phonological Priming Effect Does Not Depend}} on {{Second Language Proficiency}}},
  author = {Duyck, Wouter and Diependaele, Kevin and Drieghe, Denis and Brysbaert, Marc},
