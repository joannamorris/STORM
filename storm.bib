@article{taft_alternative_1982-1,
  title = {An Alternative to Grapheme-Phoneme Conversion Rules?},
  author = {Taft, Marcus},
  year = {1982},
  journal = {Memory \& Cognition},
  volume = {10},
  number = {5},
  pages = {465--474},
  abstract = {Reports results from 4 studies with 76 undergraduates that raise doubts about the existence of a phonological route to the lexicon via grapheme-phoneme rule conversion. When orthographic factors were tightly controlled in a lexical-decision task, it was observed that orthographic similarity rather than homophony with a word led to increased RTs to nonwords. This suggested that the pseudohomophone effect was not a phonological effect. Instead, a conversion of the graphemes of a stimulus item into different graphemes via a set of grapheme-grapheme conversion rules was supported. When phonological factors were tightly controlled and orthographic similarity varied, evidence for the existence of grapheme-grapheme rules was provided in both a lexical-decision task and a task in which Ss were required to say whether an item was pronounced in the same way as a word. Even in the latter task, in which the likelihood of phonological recoding was optimized, it appeared that grapheme-grapheme rules were rarely, if ever, used. (19 ref) (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(Phonetic,[2340].,*Human,*Human Information Storage,*Orthography,*Phonology,*Words,*Words (Phonetic Units),Cognitive,Cognitive Processes [2340].,Human.,Information,Processes,Storage,Units)},
  annotation = {Psychonomic Society, US Journal Article. English}
}

@article{taft_body_1992,
  title = {Semantic {{Productivity}} and {{Idiom Comprehension}}},
  author = {McGlone, Matthew S. and Glucksberg, Sam and Cacciari, Cristina},
  year = {1994},
  month = mar,
  journal = {Discourse Processes},
  volume = {17},
  number = {2},
  pages = {167},
  issn = {0163853X},
  doi = {10.1080/01638539409544865},
  abstract = {When people understand an idiom such as "spill the beans," two kinds of meaning are simultaneously apprehended: the meaning of the words themselves and the idiomatic meaning. Standard models of language comprehension assume that the literal meanings of figurative expressions are rejected in favor of a nonliteral meaning. We propose an alternative approach in which literal meanings are systematically used to the use and variation of conventional idioms and to generate novel idiom variants.},
  keywords = {Comprehension,Idioms}
}

@article{taft_body_1992-1,
  ids = {taft_body_1992-2},
  title = {The Body of the {{BOSS}}: {{Subsyllabic}} Units in the Lexical Processing of Polysyllabic Words},
  author = {Taft, Marcus},
  year = {1992},
  journal = {Journal of Experimental Psychology: Human Perception \& Performance},
  volume = {18},
  number = {4},
  pages = {1004--1014},
  publisher = {{American Psychological Association}},
  address = {{US}},
  abstract = {The body of a monosyllabic word (i.e., its vowel plus terminal consonants) has been shown to be an important unit in lexical processing (e.g., the EAD of HEAD). This article attempts to define what the body of a polysyllabic word might be, suggesting that it is the body of the 1st orthographically defined syllable of the word (the body of its basic orthographic syllable structure [BOSS], or BOB, e.g., the EAD of MEADOW). Three experiments, testing 60, 96, and 45 university students, respectively, demonstrated that the pronunciation of a nonword is influenced by the pronunciation of the BOB of a preceding word, but not by the pronunciation of its vowel alone or its 1st phonologically defined syllable. The results of the 3 studies are interpreted as supporting the idea that the BOB is an important unit of lexical processing. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Phonetic,[2720].,*Lexical,*Lexical Decision,*Pronunciation,*Words,*Words (Phonetic Units),\&,Adulthood,Decision,Human.,Human. Adulthood (18 yrs \& older).,Language,Lexical Decision,Linguistics,Linguistics \& Language \& Speech [2720].,older).,Pronunciation,Speech,Units),Words (Phonetic Units),yrs},
  annotation = {American Psychological Assn, US Empirical Study. Journal Article. English},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/HRNHTN3I/Taft. 1992.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/XVZEAU5R/1993-12968-001.html}
}

@article{taft_evidence_1984-1,
  title = {Evidence for an Abstract Lexical Representation of Word Structure},
  author = {Taft, Marcus},
  year = {1984},
  journal = {Memory \& Cognition},
  volume = {12},
  number = {3},
  pages = {264--269},
  abstract = {In 2 experiments, a total of 20 undergraduates were required to judge whether a particular visually presented word was a homophone or not. When instructed not to read the word aloud, Ss had difficulty with words whose orthography suggested a morphological structure different from that suggested by their homophones (e.g., FINED\textendash homophonic with FIND). There were no major problems encountered when the 2 members of the homophonic pair had the same morphological structure (e.g., KNEAD/NEED). When Ss were instructed to read the word aloud before making a decision, however, their performance on the morphologically different items improved markedly. This result suggests that inflected words are represented in the lexicon as stem plus affix and that silent reading involves a more abstract phonological representation than the actual phonetic representation produced by reading aloud. (18 ref) (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Phonetic,[2720].,*Orthography,*Phonology,*Silent,*Silent Reading,\&,Abstraction,Adulthood,Human.,Human. Adulthood (18 yrs \& older).,Language,Linguistics,Linguistics \& Language \& Speech [2720].,older).,Reading,Speech,Units),Words,Words (Phonetic Units),yrs},
  annotation = {Psychonomic Society, US Empirical Study. Journal Article. English}
}

@article{taft_exploring_1984,
  title = {Morphology, Orthography, and Phonology in Reading {{Chinese}} Compound Words},
  author = {Zhou, Xiaolin and {Marslen-Wilson}, William and Taft, Marcus and Shu, Hua},
  year = {1999},
  journal = {Language \& Cognitive Processes},
  volume = {14},
  number = {5-6},
  pages = {525--565},
  abstract = {The interaction between morphological, orthographic, and phonological information in reading Chinese compound words was investigated in five sets of experiments, using both masked priming and visual-visual priming lexical decision tasks. Words sharing common morphemes were consistently found to facilitate each other, although the priming effects were modulated by spatial overlap of orthographic forms in masked priming. Priming effects were also found for words having homographic-homophonic characters, but the effect tended to be inhibitory when the stimulus onset asynchrony between primes and targets was long and when the competing morphemes corresponding to the characters were at the initial constituent position of primes and targets. Priming effects between words having homographic but non-homophonic characters were more inhibitory, compared with effects between words having homographic-homophonic characters. Words having orthographically different homophonic morphemes did not prime each other throughout the experiments. The results were discussed in terms of how lexical representations incorporate morphological structure and how morphological, orthographic, and phonological information interacts in constraining semantic activation of constituent morphemes and compound words. (PsycINFO Database Record (c) 2002 APA, all rights reserved) (journal abstract).},
  keywords = {(18,(Language),[2340].,*Cognitive,*Cognitive Processes,*Lexical,*Lexical Decision,*Masking,*Semantic,*Semantic Priming,*Visual,*Visual Perception,\&,Adulthood,Cognitive,Cognitive Processes [2340].,Decision,Human.,Human. Adulthood (18 yrs \& older).,Morphology,Morphology (Language),older).,Orthography,Perception,Phonology,Priming,Processes,Reading,yrs},
  annotation = {Taylor \& Francis, United Kingdom Conference Proceedings/Symposia. Empirical Study. Most of the data published in this article were presented at the aforementioned conference Journal Article. English}
}

