@article{grainger_masked_1996,
  title = {The {{Career}} of {{Metaphor}}},
  author = {Bowdle, Brian F. and Gentner, Dedre},
  year = {2005},
  month = jan,
  journal = {Psychological Review},
  volume = {112},
  number = {1},
  pages = {193--216},
  issn = {0033-295X},
  doi = {10.1037/0033-295X.112.1.193},
  urldate = {2018-10-19},
  abstract = {A central question in metaphor research is how metaphors establish mappings between concepts from different domains. The authors propose an evolutionary path based on structure-mapping theory. This hypothesis--the career of metaphor--postulates a shift in mode of mapping from comparison to categorization as metaphors are conventionalized. Moreover, as demonstrated by 3 experiments, this processing shift is reflected in the very language that people use to make figurative assertions. The career of metaphor hypothesis offers a unified theoretical framework that can resolve the debate between comparison and categorization models of metaphor. This account further suggests that whether metaphors are processed directly or indirectly, and whether they operate at the level of individual concepts or entire conceptual domains, will depend both on their degree of conventionality and on their linguistic form. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {cognitive processes,Cognitive Processes,Humans,linguistics,Linguistics,Metaphor,metaphor research,structure-mapping theory},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/XJHA2M29/Bowdle and Gentner - 2005 - The Career of Metaphor.pdf}
}

@article{grainger_masked_1996-1,
  title = {Masked Orthographic and Phonological Priming in Visual Word Recognition and Naming: {{Cross-task}} Comparisons},
  shorttitle = {Masked Orthographic and Phonological Priming in Visual Word Recognition and Naming},
  author = {Grainger, Jonathan and Ferrand, Ludovic},
  year = {1996},
  month = oct,
  journal = {Journal of Memory and Language},
  volume = {35},
  number = {5},
  pages = {623--647},
  issn = {0749-596X},
  doi = {10.1006/jmla.1996.0033},
  abstract = {The effects of briefly presented, masked, and orthographically and/or phonologically related nonword primes on the recognition of subsequently presented target words were investigated     in different experimental tasks. Robust effects of orthographic and phonological priming were observed in both the lexical decision and the perceptual identification tasks, with no such effects appearing in the word naming task, except for orthographic priming effects at the shortest prime exposures. Further investigation of this marked dissociation across experimental tasks showed that word naming is particularly sensitive to shared onsets in the masked priming paradigm and that robust rhyme priming does occur when primes and targets have different onsets. The lexical decision task, on the other hand, showed priming effects independently of whether prime and targets shared onsets. These results are discussed within the framework of a bimodal interactive activation model of visual word recognition and naming. (PsycINFO Database Record (c) 2012 APA, all rights reserved) (journal abstract)},
  keywords = {{masked orthographic \& phonological priming, visual word recognition vs naming vs perceptual identification, college students, France},Masking,Naming,Orthography,Phonology,Priming,visual perception,WORD recognition}
}

@article{grainger_masked_2003,
  title = {Morphological Processing in a Second Language: {{Behavioral}} and {{ERP}} Evidence for Storage and Decomposition},
  author = {Hahne, Anja and M{\"u}ller, Jutta and Clahsen, Harald},
  year = {2006},
  journal = {Journal of Cognitive Neuroscience},
  volume = {18},
  number = {1},
  pages = {121--134},
  abstract = {This study reports the results of two behavioral and two event-related brain potential (ERP) experiments examining the processing of inflected words in second language (L2) learners with Russian as their native language (L1). Two different subsystems of German inflection were studied, participial inflection and noun plurals. For participial forms, L2 learners were found to widely generalize the -t suffixation rule in a nonce-word elicitation task, and in the ERP experiment they showed an anterior negativity followed by a P600 both results resembling previous findings from native speakers of German on the same materials. For plural formation, the L2 learners displayed different preference patterns for regular and irregular forms in an offline plural judgment task. Regular and irregular plural forms also differed clearly with regard to their brain responses. Whereas overapplications of the -s plural rule produced a P600 component, overapplications of irregular patterns elicited an N400. In contrast to native speakers of German, however, the L2 learners did not show an anterior negativity for -s plural overapplications. Taken together, the results show clear dissociations between regular and irregular inflection for both morphological subsystems. We argue that the two processing routes posited by dualmechanism models of inflection (lexical storage and morphological decomposition) are also employed by L2 learners.}
}

