@article{muller_solving_2003,
  title = {Reaction-Time Binning: {{A}} Simple Method for Increasing the Resolving Power of {{ERP}} Averages},
  shorttitle = {Reaction-Time Binning},
  author = {Poli, Riccardo and Cinel, Caterina and Citi, Luca and Sepulveda, Francisco},
  year = {2010},
  journal = {Psychophysiology},
  volume = {47},
  number = {3},
  pages = {467--485},
  issn = {1469-8986},
  doi = {10.1111/j.1469-8986.2009.00959.x},
  urldate = {2022-02-08},
  abstract = {Stimulus-locked, response-locked, and ERP-locked averaging are effective methods for reducing artifacts in ERP analysis. However, they suffer from a magnifying-glass effect: they increase the resolution of specific ERPs at the cost of blurring other ERPs. Here we propose an extremely simple technique\textemdash binning trials based on response times and then averaging\textemdash which can significantly alleviate the problems of other averaging methods. We have empirically evaluated the technique in an experiment where the task requires detecting a target in the presence of distractors. We have also studied the signal-to-noise ratio and the resolving power of averages with and without binning. Results indicate that the method produces clearer representations of ERPs than either stimulus-locked and response-locked averaging, revealing finer details of ERPs and helping in the evaluation of the amplitude and latency of ERP waves. The method is applicable to within-subject and between-subject averages.},
  langid = {english},
  keywords = {ERP averaging,ERP signal-to-noise ratio,Grand averages,High-resolution averages,Reaction-time distributions,Variable-latency ERPs},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/7EX5GLHH/Poli et al. - 2010 - Reaction-time binning A simple method for increas.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/K3BUXDGD/j.1469-8986.2009.00959.html}
}

@article{muller_solving_2003-1,
  title = {Solving the Bracketing Paradox: An Analysis of the Morphology of {{German}} Particle Verbs},
  author = {M{\"u}ller, Stefan},
  year = {2003},
  journal = {Journal of Linguistics},
  volume = {39},
  pages = {275--325},
  abstract = {Inflectional affixes are sensitive to morphological properties of the stems of the verbs they attach to. Therefore it is reasonable to assume that the inflectional material is combined with both the verbal stem of simplex verbs and the verbal stem of particle verbs. It has been argued that this leads to a bracketing paradox in the case of particle verbs since the semantic contribution of the inflectional information scopes over the complete particle verb. I will discuss nominalizations and adjective derivation, which are also problematic because of various bracketing paradoxes. I will suggest a solution to these paradoxes that assumes that inflectional and derivational prefixes and suffixes always attach to a form of a stem that contains the information about a possible particle already, but without containing a phonological realization of the particle. As is motivated by syntactic properties of particle verbs, the particle is treated as a dependent of the verb. The particle is combined with its head after inflection and derivation. With such an approach no special mechanisms for the analysis of particle verbs are necessary.}
}

@article{mullis_effects_1985,
