  number = {S1},
  pages = {78--92},
  issn = {0305-0009, 1469-7602},
  doi = {10.1017/S0305000914000336},
  urldate = {2018-01-24},
  abstract = {ABSTRACTThis paper lays out the components of a language acquisition model, the interconnections among the components, and the differing stances of nativism and empiricism about syntax. After demonstrating that parsimony cannot decide between the two stances, the paper analyzes nine examples of evidence that have been used to argue for or against nativism, concluding that most pieces of evidence are either irrelevant or suggest that language is special but need not invoke innate ideas. Two pieces of evidence~\textendash ~the development of home sign languages and the acquisition of Determiners~\textendash ~do show not just that language is special but that the child has innate syntactic content. The existential claim that nativism makes~\textendash ~there is at least one innate syntactic idea~\textendash ~is an easier claim to verify than the universal claim that empiricism makes~\textendash ~there are no innate syntactic ideas.},
  langid = {english},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/A42Y7NTC/BD5F9A9DE16D59EBCD3CF2C0CB06C6C9.html}
}

