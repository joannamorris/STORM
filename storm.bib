  shorttitle = {Humans {{Have Evolved Specialized Skills}} of {{Social Cognition}}},
  author = {Herrmann, Esther and Call, Josep and {Hern{\`a}ndez-Lloreda}, Mar{\'i}a Victoria and Hare, Brian and Tomasello, Michael},
