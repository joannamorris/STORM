@article{forster_intervenor_2009,
  title = {Differences in Working Memory Involvement in Analytical and Creative Tasks: {{An ERP}} Study},
  author = {Lavric, Aureliu and Forstmeier, Simon and Rippon, Gina},
  year = {2000},
  journal = {Neuroreport: For Rapid Communication of Neuroscience Research},
  volume = {11},
  number = {8},
  pages = {1613--1618},
  abstract = {Examined whether creative (insight) problem solving is less systematic and employs less planning than analytical problem solving, requiring substantially less working memory (WM). 19 18,\"A\`i25 yr old Ss simultaneously solved problems (deductive reasoning task) and counted auditory stimuli (concurrent WM task), in response to which event related potential (ERP) was recorded. Counting disrupted analytical, but not creative performance. Peak and time-window average P300 were more frontal during analytical problem solving as compared to insight or counting tones only (control). A PCA extracted 2 factors in the P3 range, one frontal and one broad left-lateralized, which distinguished analytical from creative problem solving. The findings indicate distinct processing pathways for the 2 types of tasks, with more WM involvement in analytical tasks. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
