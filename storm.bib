  author = {Burani, Cristina and Marcolini, Stefania and Stella, Giacomo},
  year = {2002},
  journal = {Brain \& Language},
  volume = {81},
  number = {1-3},
  pages = {568--586},
  abstract = {In this research, lexical and morpholexical reading in 90 Italian children (45 males and 45 females) aged 8-10 years were investigated. Children and control adults (27 males and 27 females) were administered two tasks on words and pseudowords: visual lexical decision and naming. Word frequency effects in both lexical decision and naming were found in both children and adults. For all age groups pseudowords made up of roots and derivational suffixes were decided more frequently as possible words and were named more quickly and accurately than matched pseudowords with no morphological constituency. These results show that morpholexical reading is available and efficient in young readers of a shallow orthography, with similar patterns in children and adults. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(6-12,(birth-12,(Language),[2820].,*Lexical,*Lexical Decision,*Morphology,*Morphology (Language),*Naming,*Orthography,*Reading,*Reading Development,\&,Adulthood,Age,Childhood,Cognitive,Cognitive \& Perceptual Development [2820].,Decision,Development,Elementary,Elementary School Students,Female.,Frequency,Human.,Human. Male. Female. Childhood (birth-12 yrs). Sch,Language,Male.,older).,Perceptual,School,Students,Word,Word Frequency,yrs,yrs).},
  annotation = {Academic Press, US Empirical Study. Journal Article. English}
}

@incollection{burani_morpholexical_1999,
  title = {Derivational Morphology in the {{German}} Mental Lexicon: {{A Dual Mechanism}} Account},
  booktitle = {Morphological Structure in Language Processing},
  author = {Clahsen, Harald and Sonnenstuhl, Ingrid and Blevins, James},
  editor = {Baayen, Harald and Schreuder, Robert},
  year = {2003},
  series = {Trends in Linguistics},
  number = {151},
  pages = {125--155},
  publisher = {{Mouton de Gruyter}},
  address = {{Berlin ; New York}},
  abstract = {The Dual Mechanism Model posits two different cognitive mechanisms for morphologically complex word forms: decomposition of regulars into stems and exponents, and full-form storage for irregulars. Most of the research in this framework has focused on contrasts between productive and non-productive inflection. In this paper, we extend the model to derivational morphology. Our studies indicate that productive derivation shows affinities with both productive and non-productive inflection. We argue that these results support the linguistic distinction between derivation and inflection, particularly as it is represented in realization-based models of morphology.},
  isbn = {3-11-017892-3}
}

