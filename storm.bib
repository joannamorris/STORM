}

@incollection{frauenfelder_constraining_1991,
  title = {How {{Language Facilitates}} the {{Acquisition}} of {{False-Belief Understanding}} in {{Children}} with {{Autism}}},
  booktitle = {Why Language Matters for Theory of Mind.},
  author = {{Tager-Flusberg}, Helen and Joseph, Robert M.},
  editor = {Astington, Janet Wilde and Baird, Jodie A.},
  year = {2005},
  pages = {298--318},
  publisher = {{Oxford University Press}},
  address = {{New York, NY US}},
  abstract = {This chapter discusses the facilitation of false-belief understanding through language in autistic children. Across all studies, the most consistent findings are that children with autism perform significantly worse than controls, even though in many studies participants with mental retardation or specific language impairment do not perform as well as normally developing preschoolers. In this chapter, the authors argue for a different hypothesis regarding the relationship between language and theory of mind in autism. They propose that the acquisition of sentential complements is the key to understanding why some children with autism are able to pass false-belief tasks. Children with autism are especially dependent on language, particularly knowledge of sentential complements, to bootstrap their meta-representational capacity. This hypothesis draws on the important theoretical and empirical work conducted by Jill de Villiers and Peter de Villiers and their colleagues with normally developing preschoolers and deaf children. This chapter includes a longitudinal study conducted by the authors which focuses on the developmental relationships between the acquisition of a representational theory of mind and language in children with autism. The findings from this study indicate that knowledge of sentential complements is a significant concurrent predictor of theory-of-mind performance in children with autism. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
  isbn = {0-19-515991-8},
  keywords = {AUTISM,autistic children,Childhood Development,Cognitive Development,false-belief understanding,Language,Language Development,sentential complements,theory of mind}
}

