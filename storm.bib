@article{fiedler_sampling_2000,
  title = {A Sampling Approach to Biases in Conditional Probability Judgments: {{Beyond}} Base Rate Neglect and Statistical Format},
  shorttitle = {A Sampling Approach to Biases in Conditional Probability Judgments},
  author = {Fiedler, Klaus and Brinkmann, Babette and Betsch, Tilmann and Wild, Beate},
  year = {2000},
  month = sep,
  journal = {Journal of Experimental Psychology: General},
  volume = {129},
  number = {3},
  pages = {399--418},
  issn = {0096-3445},
  doi = {10.1037/0096-3445.129.3.399},
  urldate = {2015-09-27},
  abstract = {Conditional probability judgments of rare events are often inflated. Early accounts assumed a general deficit in using statistical base rates. More recent approaches predict improvement when problems are presented in frequency format or refer to natural categories. The present theory focuses on sampling processes. Experiment 1 showed that a seeming advantage of frequency over probability formats is due to a confounded factor, the need to mentally transform stimulus samples. An information search paradigm was used in Experiment 2. When sampling by the predictor, the probability to be estimated, p(criterion/predictor), was conserved in the samples and judgments were quite accurate. However, when sampling by the criterion, the low base-rate event was strongly overrepresented, accounting for the entire bias. Judgments were quite sensitive to the sampled data, but failed to take sampling constraints into account, as shown in Experiments 3 and 4. (PsycINFO Database Record (c) 2013 APA, all rights reserved). (journal abstract)},
  keywords = {Biased Sampling,College Students,Probability Judgment,Sampling (Experimental),sampling approach to biases in conditional probability judgments},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/HI8B5XWQ/Fiedler et al. - 2000 - A sampling approach to biases in conditional proba.pdf}
}

@article{fiorentino_compound_2007,
  title = {Are Root Letters Compulsory for Lexical Access in {{Semitic}} Languages? {{The}} Case of Masked Form-Priming in {{Arabic}}},
  shorttitle = {Are Root Letters Compulsory for Lexical Access in {{Semitic}} Languages?},
  author = {Perea, Manuel and Mallouh, Reem Abu and Carreiras, Manuel},
  year = {2014},
  month = sep,
  journal = {Cognition},
  volume = {132},
  number = {3},
  pages = {491--500},
  issn = {0010-0277},
  doi = {10.1016/j.cognition.2014.05.008},
  abstract = {Do Semitic and Indo-European languages differ at a qualitative level? Recently, it has been claimed that lexical space in Semitic languages (e.g., Hebrew, Arabic) is mainly determined by morphological constraints, while lexical space in Indo-European languages is mainly determined by orthographic constraints (Frost, Kugler, Deutsch, \& Forster, 2005). One of the key findings supporting the qualitative difference between Semitic and Indo-European languages is the absence of masked form priming in Hebrew/Arabic with productive words. Here we examined whether masked form priming occurs in Arabic words when one of the letters from the productive root is replaced in the prime stimulus by another letter. Results showed a significant masked form priming effect with the lexical decision task in three experiments (including yes/no, go/no-go, and sandwich priming), to a similar degree to that reported in previous research with Indo-European languages. These data support the view that the processing of word forms in Semitic vs. Indo-European languages differs more at a quantitative than at a qualitative level. (PsycINFO Database Record (c) 2015 APA, all rights reserved). (journal abstract)},
  keywords = {decision making,Foreign Languages,Language,lexical access,Masked priming,Masking,Priming,WORD recognition}
}

@phdthesis{fiorentino_electrophysiological_2014,
  title = {Prosodic Sensitivity, Morphological Ability, and Reading Ability in Young Adults with and without Childhood Histories of Reading Difficulty},
  author = {Kitzen, Kathleen Ramona},
  year = {2001},
  abstract = {The purpose of this study was to examine the role of prosodic sensitivity in reading disability. The phonological processing deficit theory of dyslexia posits that deficiencies in processing oral language, particularly in segmenting spoken words into syllables and phonemes, underlie written language disabilities and may be related to deficits in speech perception. Phonological awareness, which is related to reading ability, is held to depend on complete and precise representations in the lexicon. Phonological representations may be imprecise, degraded, or incomplete in dyslexic individuals. Deficient speed in retrieving phonological codes may also contribute to impaired reading. Phonological processing studies have focused on the role of phonemic phonology in reading disability, but the role of prosodic phonology has not been well investigated. Given clinical evidence that dyslexic individuals are deficient in locating syllabic stress in words, it was hypothesized that deficient sensitivity to prosodic word structure, as well as deficient sensitivity to phonemic word structure, might play a role in reading disability. Questions were raised about differences in prosodic sensitivity between young adults with and without childhood histories of reading difficulty and about the contribution of prosodic sensitivity to proficiency in several types of reading ability. Results indicated that young adults with histories of reading difficulty are significantly lower in prosodic sensitivity compared to their peers without histories of reading difficulty. Regression analyses of reading ability found that prosodic sensitivity made an independent contribution to predicting nonword reading accuracy, oral text reading accuracy, and oral text reading comprehension, but it did not contribute to accuracy in real word reading or in proofreading for morphological errors. Word reading accuracy was predicted solely by rapid naming ability. Accuracy in proofreading for morphological errors was predicted by rapid naming ability and awareness of syllable structure. These results are consistent with models that emphasize the role of lexical encoding and rapid retrieval of lexical representations (Ellis \& Miles, 1981) and a role for phonological processing and rapid naming deficits in reading disability (Wolf \& Bowers, 1999). (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  school = {Columbia U., US, 1.},
  keywords = {(18,(18-29,(Language),[3500].,*Morphology,*Morphology (Language),*Prosody,*Reading,*Reading Ability,*Reading Disabilities,\&,Ability,Adulthood,Disabilities,Educational,Educational Psychology [3500].,Human.,Human. Adulthood (18 yrs \& older). Young Adulthood,older).,Psychology,Young,yrs,yrs).}
}

@article{fiorentino_electrophysiological_2014-1,
  title = {Electrophysiological Evidence for the Morpheme-Based Combinatoric Processing of {{English}} Compounds},
  author = {Fiorentino, Robert and {Naito-Billen}, Yuka and Bost, Jamie and {Fund-Reznicek}, Ella},
  year = {2014},
  journal = {Cognitive neuropsychology},
  volume = {31},
  number = {0},
  pages = {123--146},
  issn = {0264-3294},
  doi = {10.1080/02643294.2013.855633},
  urldate = {2015-06-15},
  abstract = {The extent to which the processing of compounds (e.g., ``catfish'') makes recourse to morphological-level representations remains a matter of debate. Moreover, positing a morpheme-level route to complex word recognition entails not only access to morphological constituents, but also combinatoric processes operating on the constituent representations; however, the neurophysiological mechanisms subserving decomposition, and in particular morpheme combination, have yet to be fully elucidated. The current study presents electrophysiological evidence for the morpheme-based processing of both lexicalized (e.g., ``teacup'') and novel (e.g., ``tombnote'') visually-presented English compounds; these brain responses appear prior to and are dissociable from the eventual overt lexical decision response. The electrophysiological results reveal increased negativities for conditions with compound structure, including effects shared by lexicalized and novel compounds, as well as effects unique to each compound type, which may be related to aspects of morpheme combination. These findings support models positing across-the-board morphological decomposition, counter to models proposing that putatively complex words are primarily or solely processed as undecomposed representations, and motivate further electrophysiological research toward a more precise characterization of the nature and neurophysiological instantiation of complex word recognition.},
  pmcid = {PMC4159414},
  pmid = {24279696},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/BGDQNAJR/Fiorentino et al. - 2014 - Electrophysiological evidence for the morpheme-bas.pdf}
}

