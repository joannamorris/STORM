@article{perner_what_2005,
  title = {Bihemispheric Foundations for Human Speech Comprehension.},
  author = {Bozic, Mirjana and Tyler, Lorraine K. and Ives, David T. and Randall, Billi and {Marslen-Wilson}, William D.},
  year = {2010},
  month = oct,
  journal = {PNAS Proceedings of the National Academy of Sciences of the United States of America},
  volume = {107},
  number = {40},
  pages = {17439--17444},
  issn = {0027-8424},
  doi = {http://dx.doi.org.ezproxy.library.tufts.edu/10.1073/pnas.1000531107},
  urldate = {2015-04-02},
  abstract = {Emerging evidence from neuroimaging and neuropsychology suggests that human speech comprehension engages two types of neurocognitive processes: a distributed bilateral system underpinning general perceptual and cognitive processing, viewed as neurobiologically primary, and a more specialized left hemisphere system supporting key grammatical language functions, likely to be specific to humans. To test these hypotheses directly we covaried increases in the nonlinguistic complexity of spoken words [presence or absence of an embedded stem, e.g., claim (clay)] with variations in their linguistic complexity (presence of inflectional affixes, e.g., play+ed). Nonlinguistic complexity, generated by the on-line competition between the full word and its onset-embedded stem, was found to activate both right and left fronto-temporal brain regions, including bilateral BA45 and -47. Linguistic complexity activated left-lateralized inferior frontal areas only, primarily in BA45. This contrast reflects a differentiation between the functional roles of a bilateral system, which supports the basic mapping from sound to lexical meaning, and a language-specific left-lateralized system that supports core decompositional and combinatorial processes invoked by linguistically complex inputs. These differences can be related to the neurobiological foundations of human language and underline the importance of bihemispheric systems in supporting the dynamic processing and interpretation of spoken inputs. (PsycINFO Database Record (c) 2012 APA, all rights reserved)(journal abstract)},
  langid = {english},
  keywords = {Left Hemisphere,Linguistics,Morphology (Language) (major),Oral Communication (major),Right Hemisphere,Speech Characteristics (major),Verbal Comprehension (major)},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/3UIFI8FI/6.html}
}

@incollection{perner_what_2005-1,
  title = {What {{Does}} '{{That}}' {{Have}} to {{Do}} with {{Point}} of {{View}}? {{Conflicting Desires}} and '{{Want}}' in {{German}}},
  shorttitle = {What {{Does}} '{{That}}' {{Have}} to {{Do}} with {{Point}} of {{View}}?},
  booktitle = {Why Language Matters for Theory of Mind.},
  author = {Perner, Josef and Zauner, Petra and Sprung, Manuel},
  editor = {Astington, Janet Wilde and Baird, Jodie A.},
  year = {2005},
  pages = {220--244},
  publisher = {{Oxford University Press}},
  address = {{New York, NY US}},
  abstract = {(from the chapter) This chapter discusses the relationship in children between theory of mind and linguistic development, which seems particularly strong for children's mastery of the false-belief test, their syntactic competence in producing complement constructions (using the Index of Productive Syntax), and their memory for complements in statements about mistakes. In particular, this chapter focuses on the concepts of "that" in relation to theory of mind and the conveyance of "want" and desires in the German language. The research presented in this chapter is based on two theories of the relation between language and the theory of mind: Perner, Brandl, and Garnham (2003) analyze perspective problems as problems where the difference between perspectives needs to be confronted and where some representational element must be taken into account in order to maintain a coherent view of the world. J. de Villiers (this volume, chapter 10) makes the same developmental claim by suggesting that around 4 years of age, children acquire Point of View (PoV) markers on particular syntactic elements, and it is the way these markers function that allows children to keep distinct differences in point of view. The findings suggest that for German children's understanding of desire when neither requires complicated finite that-complements (Chinese), both require that-complements (German), or only belief but not desire requires that-complements (English), children understand and talk about desire substantially earlier than belief. The authors' data on German children's production of that-complements showed evidence that "that" has to do with inferring and remembering think-complements. The finding that children understand differences of point of view in the context of conflicting desires at the same time as they understand it with belief raises the question how understanding of point of view can be linked to understanding the that-complements of belief. What "that" has to do with point of view remains a mystery. (PsycINFO Database Record (c) 2012 APA, all rights reserved) (chapter)},
  isbn = {0-19-515991-8},
  keywords = {Childhood Development,Children,Cognitive Development,Comprehension,Concept Formation,desires,false-belief test,German language,Language Development,linguistic development,Linguistics,Motivation,point of view,Syntax,that-complements,theory of mind,want}
}

