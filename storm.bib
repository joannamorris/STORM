@article{baum_automatic_1999-1,
  title = {Automatic versus Strategic Effects of Phonology and Orthography on Auditory Lexical Access in Brain-Damaged Patients as a Function of Inter-Stimulus Interval},
  author = {Baum, Shari R. and Leonard, Carol L.},
  year = {1999},
  journal = {Cortex},
  volume = {35},
  number = {5},
  pages = {647--660},
  abstract = {The influence of both phonological and orthographic information on auditory lexical access was examined in left- and right-hemisphere-damaged Ss using a lexical decision paradigm. Ss were divided into 3 groups: 10 nonfluent aphasics (NFA); 8 fluent aphasics (FA); and 11 with right-hemisphere-damage (RHD). Ss were presented with prime-target pairs that were either phonologically related (tooth-youth), orthographically related (touch-couch), both phonologically and orthographically related (blood-flood), or unrelated (bill-tent), at 2 interstimulus intervals (ISI)\textendash 100 ms and 750 ms\textendash to tap more automatic vs. more strategic processing. All groups demonstrated effects of orthography at both ISIs (facilitory at 100 ms ISI and inhibitory at 750 ms ISI), supporting the authors' previous findings that effects of orthography emerge independent of site of brain damage and suggesting that orthographic effects in auditory word recognition tend to be largely strategic. A facilitory effect of phonology was also found for all groups at both ISIs. The findings are discussed in relation to theories of lexical activation in brain-damaged Ss. (PsycINFO Database Record (c) 2004 APA, all rights reserved).},
  keywords = {(18,(40-64,(65,[3297].,*Brain,*Brain Damage,*Interstimulus,*Interstimulus Interval,*Lexical,*Lexical Access,*Orthography,*Phonology,\&,Access,Adulthood,Age,Aged,Aphasia,Brain,Damage,Disorders,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older). M,Interval,Male.,Middle,Neurological,Neurological Disorders \& Brain Damage [3297].,older).,Priming,Recognition,Word,word recognition,yrs,yrs).},
  annotation = {Masson Italia, Italy English Empirical Study Peer Reviewed Journal}
