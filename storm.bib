@article{neville_neurobiology_1993-1,
  title = {The Neurobiology of Sensory and Language Processing in Language-Impaired Children},
  author = {Neville, Helen J. and Coffey, Sharon A. and Holcomb, Phillip J. and Tallal, Paula},
  year = {1993},
  journal = {Journal of Cognitive Neuroscience},
  volume = {5},
  number = {2},
  pages = {235--253},
  abstract = {Used the event-related brain potential (ERP) technique in a series of studies to probe and compare different aspects of functioning in 22 children (aged 8-20 yrs) labeled as learning impaired (LI) and reading disabled (RD) and in 12 control children. Subjects read sentences ending with either an appropriate or anomalous word, and ERPs were recorded to each word. ERP components linked to processing within the superior temporal gyrus were abnormal in a subset of children that displayed abnormal performance. During sentence processing, abnormal hemispheric specialization was observed in a subset of children who scored poorly on tests of grammar. By contrast, the group as a whole displayed abnormally large responses to words requiring contextual integration. The results imply that multiple factors can contribute to the profile of language impairment and that different and specific deficits occur heterogeneously across populations of LI/RD children. ((c) 1997 APA/PsycINFO, all rights reserved)},
  keywords = {Age,Childhood,Children,Comprehension,Cortical,Cortical Evoked Potentials,Disabilities,Disorders,Evoked,Language,Language Disorders,Neurobiology,Potentials,Reading,Reading Disabilities,School,School Age Children,Sentence,Sentence Comprehension,Sentence Structure,Structure},
  annotation = {ARTICLE Salk Inst, San Diego, CA, USA 1993 Spr}
}

@article{neville_syntactically_1991,
  title = {Regular Plurals in {{Dutch}} Compounds: {{Linking}} Graphemes or Morphemes?},
  author = {Schreuder, Robert and Neijt, Anneke and {van der Weide}, Femke and Baayen, Harald},
  year = {1998},
  journal = {Language \& Cognitive Processes},
  volume = {13},
  number = {5},
  pages = {551--573},
  abstract = {Examined the functional role of the orthographic realisation of the linking schwa (LS) in Dutch nominal compounds. From a diachronic perspective, the LS is a relic of a now obsolete spelling system. Synchronically, however, it is homographic and homophonic with the high-frequency inflectional affixes -e and -en. Are plural semantics (PSs) activated in the mental lexicon when the LS is written as the plural suffix -en? This question is addressed by means of 4 experiments in which 102 undergraduates participated. Exp 1 (progressive demasking) showed that perceptual identification is not affected by whether the LS is realised as -en or -e. Exp 2 (number decision) showed that changing the orthographic realisation of the LS from e- to en- induces the activation of PSs. Exp 3 focused on compounds for which the LS is realised as -en both in the old and new spelling system. Exp 4 (plurality rating) showed that the 1st constituents of Dutch nominal compounds can have PSs as part of their central semantic representations. It is revealed that writing the LS in the orthographic form of the plural suffix does lead to the activation of the PSs, even when PSs are not intended. The results are interpreted within the framework of a parallel dual-route model of morphological processing. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),(Phonetic,[2720].,*Morphology,*Morphology (Language),*Orthography,*Psycholinguistics,*Semantics,\&,Adulthood,Human.,Human. Adulthood (18 yrs \& older).,Language,Linguistics,Linguistics \& Language \& Speech [2720].,older).,Speech,Units),Words,Words (Phonetic Units),yrs},
  annotation = {Taylor \& Francis, US Empirical Study. Journal Article. English}
}

@article{neville_syntactically_1991-1,
  title = {Syntactically {{Based Sentence Processing Classes}}: {{Evidence}} from {{Event-Related Brain Potentials}}},
