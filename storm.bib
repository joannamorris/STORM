  issn = {1469-1841},
  doi = {10.1017/S1366728906002847},
  urldate = {2015-04-16},
  abstract = {An eye tracking experiment examined how exposure to a second language (L2) influences sentence parsing in the first language. Forty-four monolingual Spanish speakers, 24 proficient Spanish\textendash English bilinguals with limited immersion experience in the L2 environment and 20 proficient Spanish\textendash English bilinguals with extensive L2 immersion experience read temporarily ambiguous constructions. The ambiguity concerned whether a relative clause (RC) that appeared after a complex noun phrase (NP) was interpreted as modifying the first or the second noun in the complex NP (El polic\'ia arrest\'o a la hermana del criado que estaba enferma desde hac\'ia tiempo). The results showed that whereas the Spanish monolingual speakers and the Spanish\textendash English bilinguals with limited exposure reliably attached the relative clause to the first noun, the Spanish\textendash English bilingual with extensive exposure attached the relative to the second noun. Results are discussed in terms of models of sentence parsing most consistent with the findings.},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/D5U9EAZK/displayAbstract.html}
}

