@article{steen_when_2008,
  title = {When Is {{Metaphor Deliberate}}?},
  author = {Steen, Gerard J.},
  year = {2008},
  abstract = {Contemporary metaphor research sees metaphor as conventional, automa-tic and unconscious. This raises the question when metaphors are used deliberately. It appears that all novel metaphors and some conventional metaphors are used deli-berately, and that this happens when they are intended to produce a change in the perspective of the addressee on the topic of a stretch of discourse. This typically happens for all metaphors that are expressed directly, such as extended non-literal comparisons and similes. Indirect metaphors can also be used deliberately, but this does not occur very frequently. The paper explains how these factors of metaphor interact with each other to produce deliberate or non-deliberate metaphor.}
}

