@article{taft_exploring_1986-1,
  title = {Exploring the {{Cohort Model}} of Spoken Word Recognition},
  author = {Taft, Marcus and Hambly, Gail},
  year = {1986},
  journal = {Cognition},
  volume = {22},
  number = {3},
  pages = {259--282},
  abstract = {Tested predictions derived from the cohort model of spoken word recognition (e.g., R. A. Cole and J. Jakimik, 1980) with 30 Ss in 4 experiments using an auditory lexical decision task. In Exp I, the point at which a word could be uniquely identified appeared to influence reaction times (RTs). Exps II and III demonstrated that the processing of a nonword phoneme string continued after the point at which there were no possible continuations that would make a word. Exp IV demonstrated a frequency effect when high- and low-frequency words were matched on their point of unique identity. Results from Exps II-IV are not consistent with the cohort model; thus, an alternative account is put forward. According to this account, the 1st few phonemes are used to activate all words beginning with those phonemes and then these candidates are checked back to the original stimulus. (French abstract) (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
