  annotation = {Academic Press, US Empirical Study. Journal Article. English}
}

@article{mori_effects_1998,
  title = {Morphological Analysis in Sentence Processing: {{An ERP}} Study},
  shorttitle = {Morphological Analysis in Sentence Processing},
  author = {Allen, Mark and Badecker, William and Osterhout, Lee},
  year = {2003},
  month = aug,
  journal = {Language and Cognitive Processes},
  volume = {18},
  number = {4},
  pages = {405--430},
  issn = {0169-0965},
  doi = {10.1080/01690960244000054},
  urldate = {2016-07-15},
  abstract = {We examined the effects of syntactic (tense) violations occurring on regularly versus irregularly inflected verbs using event-related brain potentials (ERPs). Participants read sentences in which the main verb varied in terms of regularity (regular vs. irregular), frequency (high vs. low), and grammaticality (tense violation vs. no tense violation). For regular verbs, we found a reliable N400 effect for verb frequency and a reliable P600 effect for grammaticality, with no interaction between lexical frequency and grammaticality. For irregular verbs, we found interactions between lexical frequency and grammaticality, with tense violations on high-frequency forms (*will stood) eliciting a much earlier P600 response than tense violations on low-frequency forms (*will knelt). We discuss the implications of these results with respect to morphological parsing, the time course of syntactic feature analysis, and their consequent effects on temporal properties of ERP components.},
  keywords = {(Language),[2340].,[2720].,*Errors,*Evoked,*Evoked Potentials,*Grammar,*Morphology,*Morphology (Language),*Verbs,\&,Cognitive,Cognitive Processes,Cognitive Processes [2340]. Linguistics \& Language,Frequency,Human.,Language,Linguistics,Potentials,Processes,Sentences,Speech,Syntax,Word,Word Frequency},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/4UDG2GN4/Allen, Badecker, Osterhout. 2003.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/P936WFR4/01690960244000054.html}
}

@article{mori_effects_1998-1,
  title = {Effects of First Language and Phonological Accessibility on Kanji Recognition},
  author = {Mori, Yoshiko},
  year = {1998},
  journal = {Modern Language Journal},
  volume = {82},
  number = {1},
  pages = {69--82},
  abstract = {Examines the transfer of learners' first language (L1) orthographic processing strategies to second language (L2) processing. Two groups of college students learning Japanese, 20 learners whose L1 writing system is phonographic and 20 Ss whose group whose L1 employs a morphographic writing system, were tested on their ability to represent novel kanji characters in short-term memory. For students in the phonographic language background group, phonologically accessible characters (characters whose inner radical was a pronounceable katakana character) were easier to remember than phonologically inaccessible characters (characters whose inner radical was not pronounceable). In contrast, the absence of the pronounceable elements in the stimuli did not hinder the memory performance of the morphographic language background group. This interaction between L1 background and the phonological inaccessibility of stimuli is interpreted as showing that, due to their L1 experience, L2 learners from a morphographic language background use more flexible strategies for phonological decoding for new characters than learners from a phonographic language background. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),(Learning),[2720].,[3530].,*Foreign,*Foreign Language Learning,*Learning,*Learning Strategies,*Morphology,*Morphology (Language),*Short,*Short Term Memory,\&,Adulthood,Curriculum,Human.,Human. Adulthood (18 yrs \& older).,Language,Learning,Linguistics,Linguistics \& Language \& Speech [2720]. Curriculum,Memory,Methods,older).,Orthography,Programs,Recognition,Speech,Strategies,Teaching,Term,Transfer,Transfer (Learning),Word,word recognition,Written,Written Language,yrs},
  annotation = {Blackwell Publishers, US Empirical Study. Journal Article. English}
}

@article{morris_early_2012,
  title = {Bigram {{Frequency}}, {{Number}} of {{Syllables}} and {{Morphemes}} and {{Their Effects}} on {{Lexical Decision}} and {{Word Naming}}},
  author = {Muncer, Steven and Knight, David and Adams, John},
