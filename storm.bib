@article{mohr_lexical_1994,
  title = {Lexical Decision after Left, Right and Bilateral Presentation of Function Words, Content Words and Non-Words: {{Evidence}} for Interhemispheric Interaction.},
  shorttitle = {Lexical Decision after Left, Right and Bilateral Presentation of Function Words, Content Words and Non-Words},
  author = {Mohr, Bettina and Pulverm{\"u}ller, Freidemann and Zaidel, Eran},
