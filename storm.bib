@article{schriefers_inflectional_1992-1,
  title = {Inflectional and Derivational Morphology in the Mental Lexicon: {{Symmetries}} and Asymmetries in Repetition Priming},
  author = {Schriefers, Herbert and Friederici, A. and Graetz, P.},
  year = {1992},
  journal = {Quarterly Journal of Experimental Psychology A},
  volume = {44A},
  number = {2},
  pages = {373--390},
  abstract = {Tested the bidirectional relations between morphologically related words in 2 experiments with 128 adults. Clear asymmetries in the pattern of repetition priming were observed for the inflectional and for the derivational forms. Exp 1 showed that a given inflectional variant of a stem can be an effective prime for inflectionally related targets, but it cannot be primed effectively by these inflectionally related forms. This pattern provides evidence against the position that all inflectional variants of a given stem share one entry in the mental lexicon. Data strongly suggest that it can be misleading to take full priming of a stem by its morphological relatives as sufficient evidence for there being lexical entries shared by all morphological relatives. In Exp 2, similar asymmetries were obtained for derived forms. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,[2340].,*Inflection,*Lexical,*Lexical Decision,*Morphemes,*Morphology,*Priming,\&,Adulthood,Cognitive,Cognitive Processes [2340].,Decision,Human.,Human. Adulthood (18 yrs \& older).,older).,Processes,yrs},
