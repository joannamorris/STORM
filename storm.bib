}

@phdthesis{mckinnon_electrophysiological_1996-1,
  title = {Electrophysiological Investigations of Morphological Phenomena},
  author = {McKinnon, Richard Scott},
  year = {1996},
  abstract = {Event-related potentials were recorded in seven experiments which examined various aspects of morphological processing. Experiments 1-3 examined the question of whether words are decomposed into constituent morphemes during recognition. Nonwords which contained morphological structure (e.g., *confusal (Exp. 1), *detrude Exp. 2)) failed to elicit an enhanced N400 component. The finding was especially surprising for the morphological nonwords used in Experiment 2, which were entirely meaningless. This was taken in support of a model of word recognition in which exploits morphological structure. Experiments 4-7 examined the question of whether the brain's response to violations of morphological constraints resembles the response to syntactic violations (P600). No such effect was observed. Rather, a reduction in the amplitude of the P200 component consistently co-occurred with the presentation morphological violations. The possibility that the P200 may serve as a reliable indicator of morphological anomaly is discussed. The existence of a specific ERP effect for morphological violations is also taken to support the notion that morphological structure is accessed during word recognition. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),[2300].,*Electrical,*Electrical Activity,*Morphology,*Morphology (Language),\&,Activity,Adulthood,Experimental,Human,Human Experimental Psychology [2300].,Human.,Human. Adulthood (18 yrs \& older).,older).,Psychology,Recognition,Syntax,Word,word recognition,yrs},
  annotation = {Dissertation Abstracts International. Vol 57(2-A), Aug 1996, 0663, US: University Microfilms International. Empirical Study. English}
}

