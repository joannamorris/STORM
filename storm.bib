  booktitle = {Flynn, {{Suzanne}} ({{Ed}}); {{Martohardjono}}, {{Gita}} ({{Ed}}); et al (1998) {{The}} Generative Study of Second Language Acquisition (Pp 187-214) Xiv, 366pp},
  author = {Bennett, Susan and Progovac, Ljiljana},
  year = {1998},
  abstract = {(from the chapter) Apply Relativized SUBJECT analysis to the problem of long-distance reflexives. In particular, our findings with respect to acquisition of the English binding pattern by native speakers of Serbo-Croatian follow from the theoretical framework we adopt. Current versions of the Binding Theory, including the Relativized SUBJECT analysis, cite morphological complexity as a defining characteristic of reflexives among languages. We argue that Serbo-Croatian speakers (mis)analyze morphologically complex English reflexives as simple, due to transfer from the first language (L1). Participants included 39 adolescent (aged 13-15 yrs) and 34 adult (aged 18-49 yrs) native speakers of Serbo-Croatian and 25 adolescent (aged 13-14 yrs) and 22 adult (aged 18-39 yrs) controls who were native speakers of English. Results indicate that transfer of L1 knowledge of the morphological structure of reflexives may crucially influence the pattern of reflexive interpretation in second language acquisition. This suggests that learners still have access to the principles of Universal Grammar, given that they are able to compute the binding domain as expected, although this domain differs from both the domain in their L1, and the domain in the target language. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(13-17,(18,(30-39,(40-64,(Language),[2720].,*English,*Foreign,*Grammar,*Language,*Morphology,\&,Adolescence,Adulthood,Age,As,Development,English as Second Language,Foreign Language Learning,Grammar,Human.,Language,Language Development,Learning,Linguistics,Middle,Morphology (Language),older).,{Relativized SUBJECT analysis of acquisition of long-distance binding pattern of morphologically complex English reflexives, 13\textendash 49 yr old native Serbo-Croatian speakers},Second,Speech,Thirties,yrs,yrs).},
  annotation = {Empirical Study. English}
}

@article{plaut_are_2000-1,
  title = {Are Non-Semantic Morphological Effects Incompatible with a Distributed Connectionist Approach to Lexical Processing?},
  author = {Plaut, David C and Gonnerman, Laura M},
  year = {2000},
  journal = {Language and Cognitive Processes},
  volume = {15},
  number = {4-5},
  pages = {445--485},
  issn = {0169-0965},
  abstract = {The current article reports on 2 simulations designed to evaluate whether the existence of morphological priming in the absence of semantic similarity in morphologically rich languages is inconsistent with a distributed connectionist approach to morphology. A common set of primes and targets, ranging in transparency, were embedded among a larger set of words that were either transparent or opaque, forming 2 artificial languages: a morphologically rich language analogous to Hebrew, and a morphologically impoverished language analogous to English. A network was then trained to map orthography to semantics for either language, and then tested for priming. Both languages yielded morphological priming for the more transparent targets, but priming extended to semantically opaque targets only in the rich language. The results suggest that non-semantic morphological priming, is, in fact, compatible with the connectionist approach but that its occurrence is predicted to depend on the degree of morphological richness of the language. (PsycINFO Database Record (c) 2005 APA, all rights reserved).},
  keywords = {(Language),(Phonetic,[2720].,*Connectionism,*Language,*Morphology,*Morphology (Language),*Semantic,*Semantic Priming,*Words,*Words (Phonetic Units),\&,Language,Linguistics,Linguistics \& Language \& Speech [2720].,Priming,Speech,Units)}
