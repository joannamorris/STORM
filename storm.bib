@article{tamaoka_is_1994,
  title = {On the {{Relation Between Metaphor}} and {{Simile}}: {{When Comparison Fails}}},
  shorttitle = {On the {{Relation Between Metaphor}} and {{Simile}}},
  author = {Glucksberg, Sam and Haught, Catrinel},
  year = {2006},
  month = jun,
  journal = {Mind \& Language},
  volume = {21},
  number = {3},
  pages = {360--378},
  publisher = {{Wiley-Blackwell}},
  issn = {02681064},
  doi = {10.1111/j.1468-0017.2006.00282.x},
  urldate = {2020-08-28},
  abstract = {Since Aristotle, many writers have treated metaphors and similes as equals: any metaphor can be paraphrased as a simile, and vice-versa. This property of metaphors is the basis for psycholinguistic comparison theories of metaphor comprehension. However, if metaphors cannot always be paraphrased as similes, then comparison theories must be abandoned. The different forms of a metaphor\textemdash the comparison and categorical forms\textemdash have different referents. In comparison form, the metaphor vehicle refers to the literal concept, e.g. `in my lawyer is like a shark', the term `shark' refers to the literal fish. In categorical form, `my lawyer is a shark', `shark' refers to an abstract (metaphorical) category of predatory creatures. This difference in reference makes it possible for a metaphor and its corresponding simile to differ (a) in interpretability and (b) in meaning. Because a metaphor cannot always be understood in terms of its corresponding simile, we conclude that comparison theories of metaphor are fundamentally flawed.},
  keywords = {Communication,Comprehension,Language \& languages,Metaphor,Psycholinguistics,Simile},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/AFR9AYII/Glucksberg and Haught - 2006 - On the Relation Between Metaphor and Simile When .pdf}
}

@article{tang_syntactic_2001,
