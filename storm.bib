@article{schwarz_cellular_2008,
  title = {Cellular {{Mechanisms}} of {{Estradiol-Mediated Masculinization}} of the {{Brain}}},
  author = {Schwarz, Jaclyn M. and McCarthy, Margaret M.},
  year = {2008},
  month = apr,
  journal = {The Journal of steroid biochemistry and molecular biology},
  volume = {109},
  number = {3-5},
  pages = {300--306},
  issn = {0960-0760},
  doi = {10.1016/j.jsbmb.2008.03.012},
  urldate = {2022-11-15},
  abstract = {The sexual differentiation of reproductive physiology and behavior in the rodent brain is largely determined by estradiol aromatized from testicular androgens. The cellular mechanisms by which estradiol masculinizes the brain are beginning to emerge and revealing novel features of brain development that are highly region specific. In the preoptic area, the major site controlling male sexual behavior, estradiol increases the level of the COX-2 enzyme and its product, prostaglandin E2 which promotes dendritic spine synaptogenesis. In the ventromedial nucleus of the hypothalamus, the major site controlling female reproductive behavior, estradiol promotes glutamate release from synaptic terminals, activating NMDA receptors and the MAP Kinase pathway. In the arcuate nucleus, a major regulator of anterior pituitary function, estradiol increases GABA synthesis, altering the morphology of neighboring astrocytes and reducing formation of dendritic spines synapses. Glutamate, GABA and the importance of neuronal-astrocytic cross talk are emerging as common aspects of masculinization. Advances are also being made in the mechanistic basis of female brain development, although the challenges are far greater.},
  pmcid = {PMC2493288},
  pmid = {18430566},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/JJTAKJGN/Schwarz and McCarthy - 2008 - Cellular Mechanisms of Estradiol-Mediated Masculin.pdf}
}

