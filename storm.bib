@article{osterhout_words_2002-1,
  title = {Words in the Brain: {{Lexical}} Determinants of Word-Induced Brain Activity},
  author = {Osterhout, Lee and Allen, Mark and McLaughlin, Judith},
  year = {2002},
  journal = {Journal of Neurolinguistics},
  volume = {15},
  number = {3-5},
  pages = {171--187},
  abstract = {Many studies have shown that open- and closed-class words elicit different patterns of brain activity, as manifested in the scalp-recorded event-related potential (ERP). One hypothesis is that these ERP differences reflect the different linguistic functions of the two vocabularies. This hypothesis was tested against the possibility that the word-class effects are attributable to quantitative differences in word length. ERPs from 13 scalp sites were recorded while 20 participants (18-45 yrs old) read a short essay. Some participants made sentence-acceptability judgments at the end of each sentence whereas others read for comprehension without an additional task. ERPs were averaged as a function of word class (open versus closed), grammatical category (articles, nouns, verbs, etc.), and word length. Although the two word classes did elicit distinct ERPs, all of these differences were highly correlated with word length. It is concluded that ERP differences between open- and closed-class words are primarily due to quantitative differences in word length rather than to qualitative differences in linguistic function. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,(30-39,(40-64,(Language),(Phonetic,[2520].,*Brain,*Evoked,*Evoked Potentials,*Form,*Form Classes (Language),*Grammar,*Words,*Words (Phonetic Units),\&,Adulthood,Age,Classes,Human.,Human. Adulthood (18 yrs \& older). Young Adulthood,Middle,Neurology,Neuropsychology,Neuropsychology \& Neurology [2520].,older).,Potentials,Thirties,Units),Young,yrs,yrs).},
  annotation = {Elsevier Science, England Empirical Study. Journal Article. English}
}

