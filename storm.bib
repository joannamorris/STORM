@incollection{carroll_chapter_1988,
  title = {Chapter 2 - {{Mental Models}} in {{Human-Computer Interaction11This}} Chapter Appeared in Its Entirety and Is Reprinted from {{Mental Models}} in {{Human Computer Interaction}}: {{Research Issues}} about {{What}} the {{User}} of {{Software Knows}}, {{J}}.{{M}}. {{Carroll}} and {{J}}.{{R}}. {{Olson}}, {{Editors}},-{{The}} Report of the Workshop on Software Human Factors: {{Users}} Mental Models, {{Nancy Anderson}}, Chair, Sponsored by the {{Committee}} on {{Human Factors}}, {{Commission}} on {{Behavioral}} and {{Social Sciences}} and {{Education}}, {{National Research Council}}, Published by the {{National Academy Press}}, 1987},
  shorttitle = {Chapter 2 - {{Mental Models}} in {{Human-Computer Interaction11This}} Chapter Appeared in Its Entirety and Is Reprinted from {{Mental Models}} in {{Human Computer Interaction}}},
  booktitle = {Handbook of {{Human-Computer Interaction}}},
  author = {Carroll, John M. and Olson, Judith Reitman},
  editor = {Helander, {\relax MARTIN}},
  year = {1988},
  month = jan,
  pages = {45--65},
  publisher = {{North-Holland}},
  address = {{Amsterdam}},
  doi = {10.1016/B978-0-444-70536-5.50007-5},
  urldate = {2020-01-08},
  abstract = {2.1 Abstract Users of software systems acquire knowledge about the system and how to use it through experience, training, and imitation. Currently, there is a great deal of debate about exactly what users know about software. This knowledge may include one or more of the following:\textbullet simple rules that prescribe a sequence of actions that apply in certain conditions,\textbullet general methods that fit certain general situations and goals,\textbullet ``mental models,'' knowledge of the components of a system, their interconnection, and the processes that change the components; knowledge that forms the basis for users being able to construct reasonable actions; and explanations about why a set of actions is appropriate. Discovering what users know and how these different forms of knowledge fit together in learning and performance is important. It applies to the problem of designing systems and training programs so that the systems are easy to use and the learning is efficient. Research on the effects of different representations on ultimate performance is mixed. Research on exactly what users know is scattered. Analytical methods and techniques for representing what the user knows are sparse but growing. This paper reviews current work and through the review, identifies several important research needs:\textbullet Detail what kinds of mental representations people have of systems that allow them to behave appropriately in using the software.\textbullet Detail what a mental model would consist of and how a person would use it to decide what action to take next.\textbullet Produce evidence that people have and use mental models.\textbullet Determine the behaviors that would demonstrate a mental model's form and the operations used on the model.\textbullet Explore alternative views of goal-directed representations (e.g., so-called ``sequence/method representations'') and detail the behavior predicted from them.\textbullet Expand the types of mental representations that may exist to include those that may not be mechanistic, such as algebraic systems and visual systems.\textbullet Determine how people intermix different representations in producing behavior.\textbullet Explore how knowledge about systems is acquired.\textbullet Determine how individual differences have an impact on learning of and performance on systems.\textbullet Explore the design of training sequences for systems.\textbullet Provide systems designers with tools to help them develop systems that evoke ``good'' representations in users.\textbullet Expand the task domain of this research to include more complex software.},
  isbn = {978-0-444-70536-5},
  langid = {english},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/I8NISH37/B9780444705365500075.html}
}

