@article{bentin_linguistic_2001-1,
  title = {Linguistic Theory and Psychological Reality: {{A}} Reply to {{Boudelaa}} \& {{Marslen-Wilson}}},
  author = {Bentin, Shlomo and Frost, Ram},
  year = {2001},
  journal = {Cognition},
  volume = {81},
  number = {1},
  pages = {113--118},
  abstract = {Comments on S. Boudelaa and W. D. Marslen-Wilson's (see record 2001-07158-003) paper that revives a 19th century theory of Semitic language morphology suggesting that the major morphological unit that conveys the core meaning of words (mainly verbs) in Semitic languages is a bi-consonantal structure labeled "etymon." The concept of the etymon as the cornerstone of Semitic morphology is in sharp contrast to the well-accepted view that considers the 3-consonantal root to be the kernel and major carrier of meaning in Hebrew and Arabic words. Boudelaa and Marslen-Wilson were not concerned with the linguistic status of the etymon theory, but focused on the psychological reality of the etymon. Boudelaa and Marslen-Wilson also report a set of experiments exploring cross-modality and masked priming effects between words sharing the etymon, with or without a semantic relation between primes and targets. In addition to the theoretical and empirical complications that emerge assuming that etymons are primary morphemic units in Arabic, the present authors are also puzzled by some aspects of the data reported by Boudelaa and Marslen-Wilson. (PsycINFO Database Record (c) 2005 APA, all rights reserved).},
  keywords = {(Language),[2720].,*Consonants,*Morphemes,*Morphology,*Morphology (Language),*Semantic,*Semantic Priming,\&,Access,Language,Lexical,Lexical Access,Linguistics,Linguistics \& Language \& Speech [2720].,Priming,Speech},
  annotation = {Peer Reviewed Journal http://www.elsevier.com/wps/find/journaldescription.cws\_home/505626/description\#description}
}

@article{bentin_morphological_1995,
  title = {Learning Orthographic and Phonological Representations in Models of Monosyllabic and Bisyllabic Naming},
  author = {Sibley, Daragh E. and Kello, Christopher T. and Seidenberg, Mark S.},
  year = {2010},
  month = aug,
  journal = {European Journal of Cognitive Psychology},
  volume = {22},
  number = {5},
  pages = {650--668},
  issn = {0954-1446},
  doi = {10.1080/09541440903080583},
  urldate = {2017-01-04},
  abstract = {Most current models of word naming are restricted to processing monosyllabic words and pseudowords. This limitation stems from difficulties in representing the orthographic and phonological codes for words varying substantially in length. Sibley, Kello, Plaut, and Elman (2008) described an extension of the simple recurrent network architecture, called the sequence encoder, that learned orthographic and phonological representations of variable-length words. The present research explored the use of sequence encoders in models of monosyllabic and bisyllabic word naming. Performance in these models is comparable to other models in terms of word and pseudoword naming accuracy, as well as accounting for naming latency phenomena. Although the models do not address all naming phenomena, the results suggest that sequence encoders can learn orthographic and phonological representations, making it easier to create models that scale up to larger vocabularies, while accounting for behavioural data.},
  keywords = {Large-scale connectionist modelling,lexical processing,Sequence encoder,Simple recurrent network,Word reading},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/UCXZMFBS/09541440903080583.html}
}

@article{bentin_priming_2002,
  title = {Cross-Language Speech Perception: {{Evidence}} for Perceptual Reorganization during the First Year of Life},
  shorttitle = {Cross-Language Speech Perception},
  author = {Werker, Janet F. and Tees, Richard C.},
  year = {1984},
  month = jan,
  journal = {Infant Behavior \& Development},
  volume = {7},
  number = {1},
  pages = {49--63},
  issn = {1879-0453},
  doi = {10.1016/S0163-6383(84)80022-3},
  abstract = {Previous work in which the authors and colleagues (see record [rid]1981-12550-001[/rid]) compared English adults on their ability to discriminate 2 pairs of Hindu (non-English) speech contrasts has indicated that infants discriminate speech sounds according to phonetic category without prior specific language experience. Previous work by the authors (in press) has indicated that adults and children as young as 4 yrs may lose this ability as a function of age and/or linguistic experience. The present study examined the generalizability of such a decline by comparing 10 22\textendash 35 yr old English adults, 5 30\textendash 65 yr old Salish adults, and 58 English infants (12 6\textendash 8 mo olds, 26 8\textendash 10 mo olds, and 20 10\textendash 22 mo olds) on their perception of a new non-English (Salish) speech contrast. The time course of the developmental decline in this ability was also investigated. Results of 3 experiments replicate previous findings by showing that infants can discriminate nonnative speech contrasts without relevant experience and that there is a decline in this ability during ontogeny. Further, data from both cross-sectional and longitudinal studies show that this decline occurs within the 1st yr of life and that it is a function of specific language experience. (21 ref) (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
  keywords = {1984,6\textendash 8 vs 8\textendash 10 vs 10\textendash 12 mo olds,discrimination of nonnative speech contrasts,Foreign Languages,Phonetics,Speech Development,Speech Perception}
}

@article{berent_default_1999-1,
  title = {Default Nominal Inflection in {{Hebrew}}: {{Evidence}} for Mental Variables},
  author = {Berent, Iris and Pinker, Steven and Shimron, Joseph},
  year = {1999},
  journal = {Cognition},
  volume = {72},
  number = {1},
  pages = {127--138},
  abstract = {Demonstrates that Hebrew-speakers use the regular masculine inflection as a default despite the overlap in the distribution of regular and irregular Hebrew masculine nouns. Specifically, Exp 1 (21 college student Ss) demonstrates that regular inflection is productively applied to novel nouns regardless of their similarity to existing regular nouns. In contrast, the inflection of irregular sounding nouns is strongly sensitive to their similarity to stored irregular tokens. Exp 2 (16 Ss) establishes the generality of the regular default for novel words that are phonologically idiosyncratic. Exp 3 (39 Ss) demonstrates that Hebrew speakers assign the default regular inflection to borrowings and names that are identical to existing irregular nouns. The existence of default inflection in Hebrew is incompatible with the distributional asymmetry hypothesis. Our findings also lend no support for a type-frequency account. The convergence of the circumstances triggering default inflection in Hebrew, German and English suggests that the capacity for default inflection may be general. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,[2720].,*Cognitive,*Cognitive Processes,*Human,*Human Information Storage,*Inflection,*Nouns,\&,Adulthood,Human.,Human. Adulthood (18 yrs \& older). Young Adulthood,Information,Language,Linguistics,Linguistics \& Language \& Speech [2720].,older).,Processes,Speech,Storage,Young,yrs,yrs).},
  annotation = {Elsevier Science, Netherlands Reprint. Journal Article. English}
}

@article{berent_identity_2002,
  title = {Morphemes in Their Place: {{Evidence}} for Position-Specific Identification of Suffixes},
  shorttitle = {Morphemes in Their Place},
  author = {Crepaldi, Davide and Rastle, Kathleen and Davis, Colin J.},
  year = {2010},
  month = apr,
  journal = {Memory \& Cognition},
  volume = {38},
  number = {3},
  pages = {312--321},
  issn = {0090-502X, 1532-5946},
  doi = {10.3758/MC.38.3.312},
  urldate = {2016-12-19},
  abstract = {Previous research strongly suggests that morphologically complex words are recognized in terms of their constituent morphemes. A question thus arises as to how the recognition system codes for morpheme position within words, given that it needs to distinguish morphological anagrams like overhang and hangover. The present study focused specifically on whether the recognition of suffixes occurs in a position-specific fashion. Experiments 1 and 2 revealed that morphologically complex nonwords (gasful ) are rejected more slowly than orthographic controls (gasfil ) but that the same interference effect is not present when the morphemic constituents are reversed ( fulgas vs. filgas). Experiment 3 went further in demonstrating that reversing the morphemes within words (e.g., nesskind) does not yield morpheme interference effects against orthographic controls (e.g., nusskind). These results strongly suggest that suffix identification is position specific, which imposes important constraints on the further development of models of morphological processing.},
  langid = {english},
  keywords = {(Language),(Phonetic,identification,Morphemes,Morphology,Morphology (Language),position-specific,position-specific identification,Units),Words,Words (Phonetic Units)},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/72QB6Z7D/Crepaldi et al. - 2010 - Morphemes in their place Evidence for position-sp.html}
}

@article{berent_identity_2002-1,
  title = {Identity Avoidance in the {{Hebrew}} Lexicon: {{Implications}} for Symbolic Accounts of Word Formation},
  author = {Berent, Iris},
  year = {2002},
  journal = {Brain \& Language},
  volume = {81},
  number = {1-3},
  pages = {326--341},
  abstract = {Hebrew frequently exhibits geminates in the root but strictly constrains their location: Root-initial gemination is rare, whereas root-final gemination is frequent. Four experiments (n = 84) demonstrate that Hebrew speakers generalize this constraint to novel roots. When speakers are encouraged to form a triliteral root from a biconsonantal input, they frequently reduplicate the root's final radical, but not its initial radical. Likewise, the rejection of novel root foils with root initial geminates is easier than roots with final geminates. In both cases, speakers' performance is inexplicable by the statistical structure of the Hebrew language. Speakers' ability to freely generalize the constraint on root structure suggests that their linguistic competence appeals to mental variables. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),(Phonetic,[2340].,*Lexical,*Lexical Access,*Morphology,*Morphology (Language),*Phonology,*Semantics,*Speech,*Speech Characteristics,\&,Access,Adulthood,Characteristics,Cognitive,Cognitive Processes [2340].,Consonants,Human.,Human. Adulthood (18 yrs \& older).,Language,older).,Processes,Units),Words,Words (Phonetic Units),yrs},
  annotation = {Academic Press, US Empirical Study. Journal Article. English},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/2NGKVRIX/Berent. 2002.pdf}
}

@article{berent_nature_2002,
  title = {Sensitivity to Distributional Properties of the Orthography in the Spelling of {{Italian}} Children with Dyslexia},
  shorttitle = {{{EXPRESS}}},
  author = {Marinelli, Chiara Valeria and Iaia, Marika and Burani, Cristina and Angelelli, Paola},
  year = {2021},
  month = feb,
  journal = {Quarterly Journal of Experimental Psychology},
  pages = {1747021821998925},
  publisher = {{SAGE Publications}},
  issn = {1747-0218},
  doi = {10.1177/1747021821998925},
  urldate = {2021-02-22},
  abstract = {The study examines statistical learning in the spelling of Italian children with dyslexia and typically developing readers by studying their sensitivity to probabilistic cues in phoneme-grapheme mappings. In a first experiment children spelled to dictation regular words and words with unpredictable spelling that contained either a high- or a low-frequency (i.e., typical or atypical) sound-spelling mappings. Children with dyslexia were found to rely on probabilistic cues in writing stimuli with unpredictable spelling to a greater extent than typically developing children. The difficulties of children with dyslexia on words with unpredictable spelling were limited to those containing atypical mappings. In a second experiment children spelled new stimuli, i.e., pseudowords, containing phonological segments with unpredictable mappings. The interaction between lexical knowledge and reliance on probabilistic cues was examined through a lexical priming paradigm in which pseudowords were primed by words containing related typical or atypical sound-to-spelling mappings. In spelling pseudowords children with dyslexia showed sensitivity to probabilistic cues in the phoneme-to-grapheme mapping but lexical priming effects were also found, although to a smaller extent than in typically developing readers. The results suggest that children with dyslexia have a limited orthographic lexicon but are able to extract regularities from the orthographic system and rely on probabilistic cues in spelling words and pseudowords.},
  langid = {english},
  keywords = {dyslexia,lexical procedure,sound-spelling mapping,spelling,statistical learning}
}

@article{berent_nature_2002-1,
  title = {The Nature of Regularity and Irregularity: {{Evidence}} from {{Hebrew}} Nominal Inflection},
  author = {Berent, Iris and Pinker, Steven and Shimron, Joseph},
  year = {2002},
  journal = {Journal of Psycholinguistic Research},
  volume = {31},
  number = {5},
  pages = {459--502},
  abstract = {The dissociation between the phonological faithfulness of the stem and morphological regularity of the suffix in Hebrew noun inflection is used to examine 2 hallmarks of default inflection: its lack of dependence on analogies from similar regular nouns, and its application to nonroots such as names. Subjects (Ss) in Exps 1 and 2 were 20 native Hebrew speakers. Ss in Exp 3 were 24 native Hebrews speakers. Results show that these hallmarks of regularity may be found whether or not the plural form preserves the stem faithfully: People apply the regular suffix to novel nouns that do not resemble existing nouns and to names that sound like irregular nouns, regardless of whether the stem is ordinarily preserved in the plural of that family of nouns. When they pluralize names, they do not apply the stem changes that are found in their homophonous nouns, replicating an effect found in English and German. These findings show that the distinction between regular and irregular phenomena cannot be reduced to differences in the kinds of phonological changes associated with those phenomena in English. Instead, regularity and irregularity must be distinguished in terms of the kinds of mental computations that effect them: symbolic operations versus memorized idiosyncrasies. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,[2720].,*Inflection,*Language,*Nouns,*Phonology,\&,Adulthood,Human.,Human. Adulthood (18 yrs \& older).,Language,Linguistics,Linguistics \& Language \& Speech [2720].,older).,Speech,yrs},
  annotation = {K. G. Saur Verlag GmbH \& Co KG Empirical Study. Journal Article. English}
}

