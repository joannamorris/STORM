@article{rueckl_interaction_2011,
  title = {Further Neurolinguistic Evidence for Morphological Fractionation within the Lexical System},
  author = {Cholewa, J. and De Bleser, R.},
  year = {1996},
  journal = {Journal of Neurolinguistics},
  volume = {9},
  number = {2},
  pages = {95--111},
  abstract = {In the cognitive neurolinguistic literature, several aphasic patients have been described who showed modality-specific difficulties in tasks requiring the processing of polymorphemic words and neologisms. In order to explain the observed error patterns in terms of a model of the lexicon, one has to argue in favour of specialized morphological components for the processing of polymorphemic stimuli, an assumption that is also proposed in some models of generative morphology as well as in the psycholinguistic literature. In experimental cognitive neurolinguistics it has been discussed how further developments within the logogen paradigm can explain functional disorders in processing mono- and polymorphemic words in a unified theory. This article presents the results of 4 single case studies with patients (25-, 37-, and 62-yr-old males and a 65-yr-old female) clinically classified as agrammatics who showed dissociations of theoretically distinguishable word formation processes. It is discussed which special form of the morpholexical system has to be postulated to fit the experimental findings. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,(30-39,(40-64,(65,(Language),(Phonetic,[3297].,*Cognitive,*Cognitive Processes,*Morphemes,*Words,*Words (Phonetic Units),\&,Adulthood,Age,Aged,Brain,Damage,Disorders,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older). Y,Male.,Middle,Morphology,Morphology (Language),Neurolinguistics,Neurological,Neurological Disorders \& Brain Damage [3297].,older).,Processes,Thirties,Units),Young,yrs,yrs).},
