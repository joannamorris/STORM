  author = {Holcomb, Phillip J.},
  year = {1993},
  journal = {Psychophysiology},
  volume = {30},
  number = {1},
  pages = {47--61},
  abstract = {Explored the effects of stimulus degradation on behavioral and event-related potential (ERP) measures of semantic priming in 2 experiments with a total of 24 university students to elucidate the psychological processes that underlie the N400 component. Ss made speeded lexical decisions to words and pseudowords preceded by either semantically related or unrelated prime words. Ss responded faster and more accurately to related targets than to unrelated targets (behavioral semantic priming effect), and this priming effect was greater when the target was degraded. The behavioral and ERP measures appear to be tapping into different components of the process(es) involved in semantic priming. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,[2530].,*Cortical,*Cortical Evoked Potentials,*Lexical,*Lexical Decision,*Priming,*Reaction,*Reaction Time,*Stimulus,*Stimulus Parameters,\&,Adulthood,Decision,Electrophysiology,Electrophysiology [2530].,Evoked,Human.,Human. Adulthood (18 yrs \& older).,older).,Parameters,Potentials,Semantics,Time,yrs},
