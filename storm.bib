@article{johnston_probability_1980-1,
  title = {Probability Learning and the {{P3}} Component of the Visual Evoked Potential in Man},
  author = {Johnston, Victor S and Holcomb, Phillip J},
  year = {1980},
  journal = {Psychophysiology},
  volume = {17},
  number = {4},
  pages = {396--400},
  abstract = {Evoked potentials were recorded to stimuli (S1) that were predictive of stimuli (S2) worth different monetary values to determine if these waveforms reflected the probability and/or the value of the predicted event as the S learned the relationship between S1 and S2. Both the monetary value of S2 and its conditional probability following S1 were systematically manipulated over a wide range of values. Seven undergraduate Ss were required to use the conditional probability information in order to make a correct behavioral response to receive the monetary payoff. The results indicate that the amplitude of the P3 component of the average evoked response to S1 increased as Ss learned the relationship between S1 and S2, and S2 was a high value event. (20 ref) (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
