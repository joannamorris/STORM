  abstract = {Replies to comments made by W. D. Marslen-Wilson and L. K. Tyler (see record 2003-04740-008) on the current authors' work (see record 2002-08093-006) which which was itself a comment on the article by S. Pinker and M. Ullman (see record 2002-08093-004). Pinker and Ullman defended the theory that irregular past-tense forms are stored in the lexicon, a division of declarative memory, whereas regular forms can be computed by a concatenation rule, which requires the procedural system. McClelland and Patterson commented on their article, presenting evidence to support a three modal connectionist account, a gradual acquisition of the past tense, graded sensitivity to phonological and semantic content and a single mechanism for regular and irregular forms. Marslen-Wilson and Tyler did not believe the McClelland and Patterson connectionist models to represent a promising direction, either for resolving the past-tense disputer, or for capturing the specific functional and neural architecture of the human language system. The current authors reply to this criticism, stating the arguments given in their comment do not refute their view. The authors clarify their position on Broca's aphasia and state that semantic information is important for inflection of irregular verbs. (PsycINFO Database Record (c) 2006 APA, all rights reserved).},
  keywords = {[2340].,*Cognitive,*Cognitive Processes,*Grammar,*Human,*Human Information Storage,*Language,*Memory,Cognitive,Cognitive Processes [2340].,Connectionism,Information,Processes,Storage,Verbs}
}

@article{mcclelland_integrating_2013,
  ids = {lopez-calderon_erplab_2014},
