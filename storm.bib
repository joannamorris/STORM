@article{forster_masked_2000,
  title = {Language Skills of Children and Adolescents with {{Down}} Syndrome: {{II}}. {{Production}} Deficits},
  author = {Chapman, Robin S. and Seung, Hye-Kyeung and Schwartz, Scott E. and Bird, Elizabeth Kay-Raining},
  year = {1998},
  journal = {Journal of Speech Language \& Hearing Research},
  volume = {41},
  number = {4},
  pages = {861--873},
  abstract = {Hypotheses that children and adolescents with Down syndrome (DS) show (1) aspecific expressive language impairment, (2) a "critical period" for languageacquisition, (3) a "simple sentence syntactic ceiling" in production, and (4) deficit in grammatical morphology were investigated cross-sectionally. Conversational and narrative language samples from 47 5-20 yr olds with DS were compared to those from 47 control Ss (aged 2-6 yrs) matched statistically for nonverbal mental age. Results show that Ss with DS have a specific language impairment, compared to control Ss, in number of different words and total words and in mean length of utterance (MLU). Total utterance attempts per minute were more frequent in the DS group. Narrative samples contained more word tokens, more word types, and longer MLU than conversation samples, for both groups. Intelligibility of narratives was significantly poorer for the DS group. Analyses of narrative language sample by age subgroup showed no evidence of a critical period for language development ending at adolescence, nor of a "syntactic ceiling" at MLUs corresponding to simple sentences for the DS group. Omissions of word tokens and types were more frequent in the older DS than the younger control sample, matched on MLU. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(13-17,(18,(18-29,(2-5,(6-12,(birth-12,(Language),[3256].,*Downs,*Downs Syndrome,*Grammar,*Language,*Language Disorders,*Morphology,*Morphology (Language),*Syntax,\&,Adolescence,Adulthood,Age,Childhood,Disorders,Human.,Human. Childhood (birth-12 yrs). Preschool Age (2-,Mental,Mental Retardation [3256].,older).,Preschool,Retardation,School,Syndrome,Young,yrs,yrs).},
  annotation = {American Speech-Language-Hearing Assn, US Empirical Study. Journal Article. English}
}

