@article{meier_semantic_1986,
  title = {Morphophonological Influences on the Comprehension of Subject\textendash Verb Agreement: {{An ERP}} Study},
  shorttitle = {Morphophonological Influences on the Comprehension of Subject\textendash Verb Agreement},
  author = {Severens, Els and Jansma, Bernadette M. and Hartsuiker, Robert J.},
  year = {2008},
  month = sep,
  journal = {Brain Research},
  volume = {1228},
  pages = {135--144},
  issn = {0006-8993},
  doi = {10.1016/j.brainres.2008.05.092},
  urldate = {2016-07-15},
  abstract = {Event-related potentials were used to study whether factors known to influence subject\textendash verb number agreement in production exert analogous effects on sentence comprehension. Participants read Dutch sentences containing subject\textendash verb number agreement errors while their brainwaves were measured. The determiner of the singular head noun could be ambiguous or unambiguous in number and the modifier (``local'') noun could be plural or singular. Both ambiguity and number match affect error rates in production. We expected evoked potentials in response to the verb to be modulated by number ambiguity; larger effects were expected when the sentences started with an unambiguous determiner. When the local noun was singular, we observed a more negative wave elicited by the incorrect verbs compared to the correct verbs in the 350\textendash 400~ms time-window. This effect was largest when the head noun phrase contained no number ambiguity. When the local noun was plural the waves were more positive for incorrect verbs than for correct verbs in the 600\textendash 650~ms time-window. Again this effect was largest when the head noun phrase contained no number ambiguity. Most importantly, the results show that agreement computation in comprehension is influenced by the same factors as in production. We further suggest that in the sentences with a singular local noun there is a shallow analysis of the sentence, but the sentences with a plural local noun require a deeper syntactic analysis.},
  keywords = {event-related potentials,Production,Sentence Comprehension,Subject\textendash verb agreement},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/86SSDPNR/S0006899308015655.html}
}

