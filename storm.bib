}

@article{light_electroencephalography_2010-1,
  title = {Electroencephalography ({{EEG}}) and {{Event-Related Potentials}} ({{ERP}}'s) with {{Human Participants}}},
  author = {Light, Gregory A. and Williams, Lisa E. and Minow, Falk and Sprock, Joyce and Rissling, Anthony and Sharp, Richard and Swerdlow, Neal R. and Braff, David L.},
  year = {2010},
  month = jul,
  journal = {Current protocols in neuroscience / editorial board, Jacqueline N. Crawley ... [et al.]},
  volume = {CHAPTER},
  pages = {Unit-6.2524},
  publisher = {{NIH Public Access}},
  issn = {1934-8584},
  doi = {10.1002/0471142301.ns0625s52},
  urldate = {2015-01-16},
  abstract = {Understanding the basic neural processes that underlie complex higher-order cognitive operations and functional domains is a fundamental goal of cognitive neuroscience. Electroencephalography (EEG) is a non-invasive and relatively inexpensive method for assessing neurophysiological function that can be used to achieve this goal. EEG measures the electrical activity of large, synchronously firing populations of neurons in the brain with electrodes placed on the scalp. This unit outlines the basics of setting up an EEG experiment with human participants, including equipment, and a step-by-step guide to applying and preparing an electrode cap. Also included are support protocols for two event-related potential (ERP) paradigms, P50 suppression and mismatch negativity (MMN), which are measures of early sensory processing. These paradigms can be used to assess the integrity of early sensory processing in normal individuals and clinical populations, such as individuals with schizophrenia.},
  isbn = {9780471142300},
