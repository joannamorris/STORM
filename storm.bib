@article{vanpe_mating_2008,
  title = {Mating System, Sexual Dimorphism, and the Opportunity for Sexual Selection in a Territorial Ungulate},
  author = {Vanp{\'e}, C{\'e}cile and Kjellander, Petter and Galan, Maxime and Cosson, Jean-Fran{\c c}ois and Aulagnier, St{\'e}phane and Liberg, Olof and Hewison, A. J. Mark},
  year = {2008},
  month = mar,
  journal = {Behavioral Ecology},
  volume = {19},
  number = {2},
  pages = {309--316},
  issn = {1045-2249},
  doi = {10.1093/beheco/arm132},
  urldate = {2022-12-10},
  abstract = {In mammals, species with high sexual size dimorphism tend to have highly polygynous mating systems associated with high variance in male lifetime reproductive success (LRS), leading to a high opportunity for sexual selection. However, little information is available for species with weak sexual size dimorphism. In a long-term study population, we used parentage analysis based on 21 microsatellite markers to describe, for the first time, variance in male lifetime breeding success (LBS) of roe deer, a territorial ungulate where males weigh less than 10\% more than females. LBS ranged from 0 to 14 (mean = 4.54, variance = 15.5), and its distribution was highly skewed, with only a few males obtaining high LBS and many males failing to breed or siring only one fawn. As predicted for polygynous species with low sexual size dimorphism, the standardized variance in male LBS was low (Im = 0.75) and was only slightly higher than the standardized variance in female LRS (If = 0.53), suggesting a low opportunity for sexual selection. The Im value reported here for roe deer is much lower than values reported for highly dimorphic ungulates such as red deer (Im \&gt; 3). We suggest that, along a continuum of opportunity for sexual selection, roe deer occupy a position closer to monogamous and monomorphic territorial ungulates than to highly polygynous, sexually dimorphic ungulates with dominance rank\textendash based mating systems such as harems or roving mating systems.},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/GCYNPSBS/Vanpé et al. - 2008 - Mating system, sexual dimorphism, and the opportun.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/GVDVU8KH/212845.html}
}

