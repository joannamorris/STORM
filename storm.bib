  author = {Weigelt, Sarah and Koldewyn, Kami and Kanwisher, Nancy},
  year = {2012},
  month = mar,
  journal = {Neuroscience \& Biobehavioral Reviews},
  volume = {36},
  number = {3},
  pages = {1060--1084},
  issn = {0149-7634},
  doi = {10.1016/j.neubiorev.2011.12.008},
  urldate = {2014-01-16},
  abstract = {Face recognition \textendash{} the ability to recognize a person from their facial appearance \textendash{} is essential for normal social interaction. Face recognition deficits have been implicated in the most common disorder of social interaction: autism. Here we ask: is face identity recognition in fact impaired in people with autism? Reviewing behavioral studies we find no strong evidence for a qualitative difference in how facial identity is processed between those with and without autism: markers of typical face identity recognition, such as the face inversion effect, seem to be present in people with autism. However, quantitatively \textendash{} i.e., how well facial identity is remembered or discriminated \textendash{} people with autism perform worse than typical individuals. This impairment is particularly clear in face memory and in face perception tasks in which a delay intervenes between sample and test, and less so in tasks with no memory demand. Although some evidence suggests that this deficit may be specific to faces, further evidence on this question is necessary.},
  keywords = {Asperger's syndrome,AUTISM,Autism spectrum disorders,Behavioral studies,Face inversion effect,Face perception,Face recognition,Identity perception,Neurodevelopmental disorders,Pervasive developmental disorders,Psychophysics},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/DJEKTRXX/S0149763411002156.html}
}

