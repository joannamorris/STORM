  journal = {Trends in Cognitive Sciences},
  volume = {7},
  number = {2},
  pages = {92--96},
  issn = {1364-6613},
  doi = {10.1016/S1364-6613(02)00040-2},
  urldate = {2023-04-10},
  abstract = {Can lawyers be sharks, can jobs literally be jails, and can dogs fly across lawns? Such metaphors create novel categories that enable us to characterize the topic of interest. These novel metaphorical categories are special in that they are based on outstanding exemplars of those categories, and they borrow the exemplar's name for use as the category names. Thus `shark' can be taken as a metaphor for any vicious and predatory being. Contemporary research reveals how people can create and understand such metaphors in ordinary conversation, and suggests that we understand metaphorical meanings as quickly and automatically as we understand literal meanings.},
  langid = {english},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/Y542PMHF/glucksberg_2003.pdf}
}

@article{glucksberg_understanding_1990,
  title = {Understanding Metaphorical Comparisons: {{Beyond}} Similarity},
  shorttitle = {Understanding Metaphorical Comparisons},
  author = {Glucksberg, Sam and Keysar, Boaz},
  year = {1990},
  month = jan,
  journal = {Psychological Review},
  volume = {97},
  number = {1},
  pages = {3--18},
  issn = {0033-295X},
  doi = {10.1037/0033-295X.97.1.3},
  urldate = {2018-04-20},
  abstract = {Traditionally, metaphors such as 'my job is a jail' have been treated as implicit similes (i.e., this metaphor would be treated as if it were a comparison statement, 'my job is like a jail'). A. Tversky's (1977) account of similarity is applied to such nonliteral similarity expressions, and is shown to apply as readily to nonliteral comparisons as to literal comparisons. But treating metaphors as comparison statements fails to account for certain important phenomena, including metaphoricity itself (the judgment that a comparison statement is nonliteral). We argue that metaphors are exactly what they appear to be: class-inclusion assertions, in which the topic of the metaphor (e.g., 'my job') is assigned to a diagnostic category (e.g., entities that confine one against one's will, are unpleasant, are difficult to escape from). In such assertions, the metaphor vehicle (e.g., 'jail') refers to that category, and at the same time is a prototypical exemplar of that category. This account of metaphor provides a basis for a theory of metaphor comprehension, and also clarifies why people use metaphors instead of similes. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {Figurative Language,Metaphor,metaphor vs literal comprehension \& use of metaphor vs simile as communication strategy,metaphors as class inclusion assertions vs implicit comparisons,Semantics,Verbal Comprehension},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/RQ5A2HUZ/Glucksberg and Keysar - 1990 - Understanding metaphorical comparisons Beyond sim.pdf}
}

@article{glucksberg_understanding_1998,
  title = {Understanding Metaphors},
  author = {Glucksberg, Sam},
  year = {1998},
  journal = {Current Directions in Psychological Science},
  volume = {7},
  pages = {39--43},
  issn = {1467-8721},
  doi = {10.1111/1467-8721.ep13175582},
  abstract = {Discusses the understanding of metaphors through the use of purely linguistic and pragmatic operations. Pragmatic theory is said to hold that people must first derive the literal meaning of an utterance, then that literal meaning is assessed as to whether it makes sense in the context. If it does not make sense in the context, only then are alternative nonliteral meanings considered. This view is said to have three testable implications. First, metaphor understanding will necessarily be more difficult and take longer than literal understanding; second, metaphor understanding is optional; and third, that the ground of a metaphor\textemdash the relation between the metaphor topic and vehicle\textemdash is arrived at via a comparison process. These implications are further delineated. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {Comprehension,Linguistics,Metaphor,Pragmatics}
}

