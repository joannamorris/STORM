@article{jarvella_recognition_1993,
  title = {Reading Development and Attention to Letters in Words},
  author = {Assink, Egbert M. and Knuijt, Paul P.},
  year = {2000},
  journal = {Contemporary Educational Psychology},
  volume = {25},
  number = {4},
  pages = {347--362},
  abstract = {Four groups of Dutch readers, (1) beginners, second grade elementary school students with 2 years of reading experience, (2) advanced readers, sixth grade elementary school students with 6 years of reading experience, (3) expert readers, university students with 14 years of reading experience, and (4) poor readers, aged 13 years with 2 years of delay in reading skill, were presented with short stories followed by multiple-choice text comprehension questions. Ss were asked to read the stories for meaning and to circle the letter 't' whenever it occurred in the text. Five types of detection locations were defined: (1) verb suffix, (2) noun final, (3) intra word, second letter, (4) intra word, prefinal letter, and (5) definite article, final 't'. Differential detection patterns as a function of reading development were obtained. Beginning readers did not differ in detecting the final 't' in verbs and nouns, whereas expert readers showed a strong attentional bias to verb suffixes as compared with noun endings. Expert readers missed more intra word letters. The overall developmental pattern can be characterized as a shift from attention for single letter information to attention for more complex morphological information in text. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(13-17,(18,(18-29,(6-12,(Alphabet),(birth-12,(Language),[2820].,[3253].,*Attention,*Letters,*Letters (Alphabet),*Reading,*Reading Development,*Word,*Word Recognition,\&,Adolescence,Adulthood,Age,Childhood,Cognitive,Cognitive \& Perceptual Development [2820]. Learnin,Development,Disabilities,Disorders,Frequency,Human.,Human. Childhood (birth-12 yrs). School Age (6-12,Learning,Morphology,Morphology (Language),older).,Perceptual,Reading,Reading Disabilities,Recognition,School,Word,Word Frequency,Young,yrs,yrs).},
  annotation = {Academic Press, US Conference Proceedings/Symposia. Empirical Study. Journal Article. English}
}

@article{jarvella_recognition_1993-1,
  title = {Recognition of Partial Regularity in Words and Sentences},
  author = {Jarvella, Robert J and Wennstedt, Ola},
  year = {1993},
  journal = {Scandinavian Journal of Psychology},
  volume = {34},
  number = {1},
  pages = {76--85},
  abstract = {The involvement of morphological representations in reading Swedish and Dutch was explored using a lexical decision task and a sentence continuation task. In Exp 1, 94 letter strings, composed of a root and derivational suffix, a nonroot and suffix, a root and nonsuffix, or a nonroot and nonsuffix were studied by 42 adult native speakers of Swedish. Latencies in identifying nonword items increased as the letter strings became morphologically regular. In Exp 2, 60 adult native speakers of Dutch chose among pairs of continuations to a sentence. One choice offered at a given point was acceptable in context, while the other was acceptable, or contained an inappropriate stem, an inappropriate inflection, or both together. The latency to choose a correct continuation increased as the foil offered became morphologically appropriate. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),[2340].,*Morphology,*Morphology (Language),*Reading,*Word,*Word Recognition,\&,Adulthood,Cognitive,Cognitive Processes [2340].,Decision,Human.,Human. Adulthood (18 yrs \& older).,Lexical,Lexical Decision,older).,Processes,Recognition,Sentences,yrs}
}

