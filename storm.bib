@article{taft_influence_1985-1,
  title = {The Influence of Orthography on Phonological Representations in the Lexicon},
  author = {Taft, Marcus and Hambly, Gail},
  year = {1985},
  journal = {Journal of Memory \& Language},
  volume = {24},
  number = {3},
  pages = {320--335},
  abstract = {Examined the nature of the phonological representation of words in the lexicon, using a syllable monitoring task. In Exp I, undergraduate Ss were required to determine whether a particular string of phonemes appeared in an aurally presented word. Results show that Ss tended to accept a match between the target syllable and the word when the vowel of the target syllable was of full value and the vowel of the word was a reduced vowel. Thus, the acceptance of the reduced vowel as a full vowel was determined by the orthography of the word. In Exp II, the orthographic syllabic structure (OGS) of the words was manipulated and response RT was measured. Results show that OGS influenced both reaction time and the phonological representation involved in performing the task. Combined results support a post-access level of processing that is orthographically affected. Findings are discussed in terms of the nature of phonological representation and the possible implications for the lexical representation of pronunciation and spelling. (28 ref) (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Phonetic,[2326].,*Orthography,*Phonology,*Speech,*Speech Perception,*Syllables,*Words,*Words (Phonetic Units),\&,Adulthood,Auditory,Auditory \& Speech Perception [2326].,Human,Human Information Storage,Human.,Human. Adulthood (18 yrs \& older).,Information,older).,Perception,Reaction,reaction time,Speech,Storage,Time,Units),yrs},
  annotation = {Elsevier Science, US Empirical Study. Journal Article. English}
}

