  doi = {10.1016/j.cognition.2006.05.012},
  urldate = {2019-05-26},
  abstract = {Although English verbs can be either regular (walk-walked) or irregular (sing-sang), ``denominal verbs'' that are derived from nouns, such as the use of the verb ring derived from the noun a ring, take the regular form even if they are homophonous with an existing irregular verb: The soldiers ringed the city rather than *The soldiers rang the city. Is this regularization due to a semantic difference from the usual verb, or is it due to the application of the default rule, namely VERB + -ed suffix? In Experiment 1, participants rated the semantic similarity of the extended senses of polysemous verbs and denominal verbs to their central senses. Experiment 2 examined the acceptability of the regular and irregular past-tenses of the different verbs. The results showed that all the denominal verbs were rated as more acceptable for the regular inflection than the same verbs used polysemously, even though the two were semantically equally similar to the central meaning. Thus, the derivation of the verb (nominal or verbal) determined the past-tense preference more than semantic variables, consistent with dual-route models of verb inflection.},
  pmcid = {PMC2631984},
  pmid = {16839538},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/7YT62VC6/Bandi-Rao and Murphy - 2007 - The Role of Meaning in Past-Tense Inflection.pdf}
}

@article{marslen-wilson_abstractness_1999-1,
  title = {Abstractness, Allomorphy, and Lexical Architecture},
  author = {{Marslen-Wilson}, William and Zhou, Xiaolin},
  year = {1999},
  journal = {Language \& Cognitive Processes},
  volume = {14},
  number = {4},
  pages = {321--352},
  abstract = {Two intra-modal immediate repetition priming experiments ask whether speech inputs can link directly to abstract underlying representations, or whether access is mediated via intervening "access representations" of each word's surface phonetic form. Experiment 1 with 30 Ss showed that auditory-auditory priming between morphologically related derived/stem pairs (such as excitement/excite) was not affected by allomorphic variation in the phonetic form of the stem in prime and target (as in sanity/sane). Experiment 2 with 18 Ss showed that interference effects between suffixed primes and targets sharing the same stem (as in excitement/excitable) were also unaffected by stem variation (as in sanity/sanely). These results, which cannot be attributed to either semantic or phonological factors, are problematic for mediated access theories and point to direct access from speech to abstract representations at the level of the lexical entry. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),(Phonetic,[2340].,*Lexical,*Lexical Access,*Morphology,*Morphology (Language),*Theories,*Words,*Words (Phonetic Units),\&,Access,Adulthood,Cognitive,Cognitive Processes [2340].,Human.,Human. Adulthood (18 yrs \& older).,older).,Processes,Units),yrs},
  annotation = {Taylor \& Francis, United Kingdom Conference Proceedings/Symposia. Empirical Study. Preliminary reports of some of this work were given at the aforementioned meeting and in London at the January 1994 meeting of the Experimental Psychology Society Journal Article. English}
}

