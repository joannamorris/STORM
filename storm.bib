  author = {Dufau, Stephane and Du{\~n}abeitia, Jon Andoni and {Moret-Tatay}, Carmen and McGonigal, Aileen and Peeters, David and Alario, F. Xavier and Balota, David A. and Brysbaert, Marc and Carreiras, Manuel and Ferrand, Ludovic and {Maria Ktori} and Perea, Manuel and Rastle, Kathy and Sasburg, Olivier and Yap, Melvin J. and Ziegler, Johannes C. and Grainger, Jonathan},
