@article{badecker_processing_2002,
  title = {Studies of Nervous System Sensitivity in Children with Learning and Attention Disorders},
  author = {Ackerman, Peggy T. and Holcomb, Phillip J. and McCray, David S. and Dykman, Roscoe A.},
  year = {1982},
  journal = {Pavlovian Journal of Biological Science},
  volume = {17},
  number = {1},
  pages = {30--41},
  abstract = {Studied 74 male and 39 female 7-10 yr olds diagnosed as having learning or attention disorders, with or without hyperactivity. They were found to vary widely on a measure of nervous system (NS) sensitivity recommended by A. N. Vasiler (1960). Ss were contrasted on their mean press and release RTs to 4 tones, ranging from soft (55 db) to loud (100 db), with the expectation that some would be able to maintain a parallel separation of press and release RT gradients across all intensity levels, and others would show convergence or overlap of the gradients at higher intensities. Contrary to expectation, girls did not have weaker or more sensitive NSs than boys, although girls rated themselves as less tolerant of intense stimuli. Significantly more of Ss diagnosed as hyperactive had weaker NSs. Boys also participated in a blind crossover study contrasting placebo and methylphenidate effects; the prescribing physician, who was not informed of the child's NS classification, adjusted the dosage levels so that Ss with weaker NS were titrated at higher dosage levels than those with stronger NSs. J. A. Gray (1964) suggested an explanation of the paradox (that the weak NS requires a more intense stimulus than the strong to reach the threshold of concentration, whereas for other thresholds the strong requires a more intense stimulus than the weak). Gray's theory was supported by the finding that Ss typed as weak did not show as great facilitation in RT with reward. (23 ref) (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(6-12,(birth-12,[3253].,*Attention,*Auditory,*Auditory Stimulation,*Learning,*Learning Disorders,*Perceptual,*Perceptual Motor Processes,*Reaction,*Reaction Time,Age,Childhood,Disorders,Human.,Human. Childhood (birth-12 yrs). School Age (6-12,Hyperkinesis,Learning,Learning Disorders [3253].,Methylphenidate,Motor,Processes,School,Stimulation,Time,yrs).},
  annotation = {Transaction Periodicals Consortium, US Journal Article. English}
}

@article{badecker_processing_2002-1,
  title = {The Processing Role of Structural Constraints on Interpretation of Pronouns and Anaphors},
  author = {Badecker, William and Straub, Kathleen},
  year = {2002},
  journal = {Journal of Experimental Psychology: Learning, Memory, \& Cognition},
  volume = {28},
  number = {4},
  pages = {748--769},
  abstract = {The authors report 6 self-paced word-by-word reading studies of how morphosyntactic agreement, focus status, and the structural constraints of binding theory apply and interact during the online interpretation of pronouns (e.g., him, her) and anaphors (e.g., himself, each other). Previous studies held that structural conditions on coreference work as interpretive filters that impose exceptionless limits on which antecedent candidates can be evaluated by subsequent, content-based processes. These experiments instead support an interactive-parallel-constraint model, in which multiple weighted constraints (including constraints on binding) simultaneously influence the net activation of a candidate during preselection stages of antecedent evaluation. Accordingly, structurally inaccessible candidates can interfere with antecedent selection if they are both prominent in focus structure and gender-number compatible with the pronoun or anaphor. (PsycINFO Database Record (c) 2002 APA, all rights reserved) (journal abstract).},
  keywords = {(18,(Language),[2340].,*Comprehension,*Morphology,*Morphology (Language),*Pronouns,*Syntax,\&,Adulthood,Cognitive,Cognitive Processes [2340].,Human.,Human. Adulthood (18 yrs \& older).,older).,Processes,yrs},
  annotation = {American Psychological Assn, US Empirical Study. Journal Article. English}
}

@article{baeskow_-ness_2012,
  title = {-{{Ness}} and -Ity: {{Phonological Exponents}} of n or {{Meaningful Nominalizers}} of {{Different Adjectival Domains}}?},
  shorttitle = {-{{Ness}} and -Ity},
  author = {Baeskow, Heike},
  year = {2012},
  month = mar,
  journal = {Journal of English Linguistics},
  volume = {40},
  number = {1},
  pages = {6--40},
  issn = {0075-4242},
  doi = {10.1177/0075424211405156},
  urldate = {2019-09-29},
  abstract = {Representatives of a relatively new theoretical direction\textemdash Distributed Morphology\textemdash claim that morphological building blocks traditionally referred to as derivational suffixes constitute phonological exponents (or allomorphs) of some abstract syntactic nodes n, a, or v. Following the principles of inflection, the distribution of these allomorphs is determined by competition. This view is in sharp contrast to the long-standing assumption that derivational suffixes convey not only morpho-syntactic, but also semantic information. In this article, a detailed analysis of two functionally similar English suffixes, -ness and -ity, will show that the distributionalist view is too vague as it stands because it misses subtle but significant semantic differences between these suffixes, which result from their historical development. On the basis of doublets such as ethnicness versus ethnicity, or sincereness versus sincerity it will be argued that -ness and -ity are not in complementary distribution, but select different domains of their adjectival bases. Evidence for this hypothesis is drawn from various morphological phenomena and a variety of data.},
  langid = {english}
}

@article{bagiella_mixed-effects_2000,
  title = {Mixed-Effects Models in Psychophysiology},
  author = {Bagiella, E. and Sloan, R. P. and Heitjan, D. F.},
  year = {2000},
  month = jan,
  journal = {Psychophysiology},
  volume = {37},
  number = {1},
  pages = {13--20},
  issn = {0048-5772},
  abstract = {The current methodological policy in Psychophysiology stipulates that repeated-measures designs be analyzed using either multivariate analysis of variance (ANOVA) or repeated-measures ANOVA with the Greenhouse-Geisser or Huynh-Feldt correction. Both techniques lead to appropriate type I error probabilities under general assumptions about the variance-covariance matrix of the data. This report introduces mixed-effects models as an alternative procedure for the analysis of repeated-measures data in Psychophysiology. Mixed-effects models have many advantages over the traditional methods: They handle missing data more effectively and are more efficient, parsimonious, and flexible. We described mixed-effects modeling and illustrated its applicability with a simple example.},
  langid = {english},
  pmid = {10705763},
  keywords = {Algorithms,Analysis of Variance,Mixed effects models,{Models, Psychological},Psychophysiology,Repeated measures designs,Variance-covariance matrix},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/SY4V6UT2/E3671AED5C812FB145B151206F8CDE46.html}
}

@article{baillet_electromagnetic_2001,
  title = {Electromagnetic Brain Mapping},
  author = {Baillet, S. and Mosher, J.C. and Leahy, R.M.},
  year = {2001},
  month = nov,
  journal = {IEEE Signal Processing Magazine},
  volume = {18},
  number = {6},
  pages = {14--30},
  issn = {1558-0792},
  doi = {10.1109/79.962275},
  abstract = {There has been tremendous advances in our ability to produce images of human brain function. Applications of functional brain imaging extend from improving our understanding of the basic mechanisms of cognitive processes to better characterization of pathologies that impair normal function. Magnetoencephalography (MEG) and electroencephalography (EEG) (MEG/EEG) localize neural electrical activity using noninvasive measurements of external electromagnetic signals. Among the available functional imaging techniques, MEG and EEG uniquely have temporal resolutions below 100 ms. This temporal precision allows us to explore the timing of basic neural processes at the level of cell assemblies. MEG/EEG source localization draws on a wide range of signal processing techniques including digital filtering, three-dimensional image analysis, array signal processing, image modeling and reconstruction, and, blind source separation and phase synchrony estimation. We describe the underlying models currently used in MEG/EEG source estimation and describe the various signal processing steps required to compute these sources. In particular we describe methods for computing the forward fields for known source distributions and parametric and imaging-based approaches to the inverse problem.},
  keywords = {Array signal processing,Brain mapping,Brain modeling,Digital signal processing,Electroencephalography,Humans,Magnetoencephalography,Noninvasive treatment,Pathology,Phased arrays}
}

@article{balaguer_different_2007-1,
  title = {Different {{Neurophysiological Mechanisms Underlying Word}} and {{Rule Extraction}} from {{Speech}}},
  author = {Balaguer, Ruth De Diego and Toro, Juan Manuel and {Rodriguez-Fornells}, Antoni and {Bachoud-L{\'e}vi}, Anne-Catherine},
  year = {2007},
  month = nov,
  journal = {PLOS ONE},
  volume = {2},
  number = {11},
  pages = {e1175},
  publisher = {{Public Library of Science}},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0001175},
  urldate = {2022-01-03},
  abstract = {The initial process of identifying words from spoken language and the detection of more subtle regularities underlying their structure are mandatory processes for language acquisition. Little is known about the cognitive mechanisms that allow us to extract these two types of information and their specific time-course of acquisition following initial contact with a new language. We report time-related electrophysiological changes that occurred while participants learned an artificial language. These changes strongly correlated with the discovery of the structural rules embedded in the words. These changes were clearly different from those related to word learning and occurred during the first minutes of exposition. There is a functional distinction in the nature of the electrophysiological signals during acquisition: an increase in negativity (N400) in the central electrodes is related to word-learning and development of a frontal positivity (P2) is related to rule-learning. In addition, the results of an online implicit and a post-learning test indicate that, once the rules of the language have been acquired, new words following the rule are processed as words of the language. By contrast, new words violating the rule induce syntax-related electrophysiological responses when inserted online in the stream (an early frontal negativity followed by a late posterior positivity) and clear lexical effects when presented in isolation (N400 modulation). The present study provides direct evidence suggesting that the mechanisms to extract words and structural dependencies from continuous speech are functionally segregated. When these mechanisms are engaged, the electrophysiological marker associated with rule-learning appears very quickly, during the earliest phases of exposition to a new language.},
  langid = {english},
  keywords = {Event-related potentials,Language,Language acquisition,Learning,Speech,Speech signal processing,Syllables,Syntax},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/G93JXW3W/Balaguer et al. - 2007 - Different Neurophysiological Mechanisms Underlying.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/22XKKGWL/article.html}
}

@article{baldi_orthographic_1998,
