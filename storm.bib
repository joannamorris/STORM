@article{senghas_children_2001-1,
  title = {Children Creating Language: {{How Nicaraguan}} Sign Language Acquired a Spatial Grammar},
  author = {Senghas, Ann and Coppola, Marie},
  year = {2001},
  journal = {Psychological Science},
  volume = {12},
  number = {4},
  pages = {323--328},
  abstract = {It has long been postulated that language is not purely learned, but arises from an interaction between environmental exposure and innate abilities. The innate component becomes more evident in rare situations in which the environment is markedly impoverished. The present study investigated the language production of a generation of deaf Nicaraguans (aged 7-32 yrs) who had not been exposed to a developed language. The authors examined the changing use of early linguistic structure (specifically, spatial modulations) in a sign language that has emerged since the Nicaraguan group first came together. In under 2 decades sequential cohorts of learners systematized the grammar of this new sign language. The authors examined whether the systematicity being added to the language stems from children or adults; results indicate that such changes originate in children aged 10 and younger. Thus, sequential cohorts of interacting young children collectively possess the capacity not only to learn, but also to create, language. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(13-17,(18,(18-29,(30-39,(6-12,(birth-12,(Language),[3299].,*Deaf,*Language,*Language Proficiency,*Linguistics,*Morphology,*Morphology (Language),*Sign,*Sign Language,\&,Adolescence,Adulthood,Age,Childhood,Disorders,Female.,Grammar,Hearing,Human.,Human. Male. Female. Childhood (birth-12 yrs). Sch,Language,Male.,older).,Organization,Proficiency,School,Sensory,Spatial,Spatial Organization,Thirties,Vision,Vision \& Hearing \& Sensory Disorders [3299].,Young,yrs,yrs).},
