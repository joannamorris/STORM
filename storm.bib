}

@article{wu_individual_1990-1,
  title = {Individual Differences in {{Chinese}} Character Matching},
  author = {Wu, Jei-tun},
  year = {1990},
  journal = {Chinese Journal of Psychology},
  volume = {32},
  pages = {63--74},
  abstract = {Studied individual cognitive processes and the roles of morphological and phonological codes in recognizing Chinese characters during a character matching computer task. Ss were 115 normal male and female undergraduates. Exp 1 included 256 sets of Chinese characters. Each set included an object character, a smaller comparison character, and a larger comparison character. The characters were classified into 4 combinations of morphological and phonological codes. Three processing models were proposed: (1) the object character was compared with the smaller character first, then with the larger character in linear or parallel processing; (2) the object character was compared with the larger character first, then with the smaller character in linear or parallel processing; and (3) mixtures of Model 1 and Model 2 or identical processing models were used. Exp 2 included 128 sets of Chinese characters. Ss had to complete the matching task within 15 min. The internal representation of Chinese characters was reexamined. (English abstract) (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
