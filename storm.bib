}

@article{taft_lexical_1976-1,
  title = {Lexical Storage and Retrieval of Polymorphemic and Polysyllabic Words},
  author = {Taft, Marcus and Forster, Kenneth I.},
  year = {1976},
  month = dec,
  journal = {Journal of Verbal Learning \& Verbal Behavior},
  volume = {15},
  number = {6},
  pages = {607--620},
  issn = {0022-5371},
  doi = {10.1016/0022-5371(76)90054-2},
  abstract = {Conducted 5 experiments with 75 Ss to investigate how polysyllabic words (e.g., DAYDREAM, ATHLETE) are stored and retrieved from lexical memory. The 1st 4 experiments examined interference effects caused by the accessing of inappropriate lexical entries. It was found that compound nonwords whose 1st constituent is a word (e.g., DUSTWORTH, FOOTMILGE) took longer to classify as nonwords than compound nonwords whose 1st constituent is not a word (e.g., TROWBREAK, MOWDFLISK). Moreover, the presence of a word in the 2nd constituent position appeared to be irrelevant. These effects were seen even when the boundary between constituents is unclear on an orthographic basis (e.g., TRUCERIN). It is also argued that 1st syllables, as opposed to last syllables, have independent status in the lexicon since nonword 1st syllables (e.g., ATH) show interference effects, while last syllables (e.g., CULE) do not. The 5th experiment revealed that the frequency of the 1st constituent of a compound word influenced classification times. It is concluded that polysyllabic words, regardless of whether they are polymorphemic or monomorphemic, are accessed via their 1st syllable. (21 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
  keywords = {(Phonetic,[2340].,*Cognitive,*Cognitive Processes,*Memory,*Syllables,*Word,*Word Meaning,*Words,*Words (Phonetic Units),Cognitive,Cognitive Processes,Cognitive Processes [2340].,Human,Human Information Storage,Human.,Information,Meaning,MEMORY,{polymorphemic \& polysyllabic words, lexical storage \& retrieval},Processes,Storage,Syllables,Units),Word Meaning,Words (Phonetic Units)},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/MP8BAK5C/taft1976.pdf}
}

