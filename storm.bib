@article{allen_inflectional_2002,
  title = {Effect Sizes and {{F}} Ratios {$<$} 1.0. {{Sense}} or Nonsense?},
  author = {Voelkle, Manuel C. and Ackerman, Phillip L. and Wittman, Werner W.},
  year = {2007},
  journal = {Methodology: European Journal of Research Methods for the Behavioral and Social Sciences},
  volume = {3},
  number = {1},
  pages = {35--46},
  publisher = {{Hogrefe \& Huber Publishers}},
  address = {{Germany}},
  issn = {1614-2241},
  doi = {10.1027/1614-2241.3.1.35},
  abstract = {Standard statistics texts indicate that the expected value of the F ratio is 1.0 (more precisely: N/(N-2)) in a completely balanced fixed-effects ANOVA, when the null hypothesis is true. Even though some authors suggest that the null hypothesis is rarely true in practice (e.g., Meehl, 1990), F ratios {$<$} 1.0 are reported quite frequently in the literature. However, standard effect size statistics (e.g., Cohen's f) often yield positive values when F {$<$} 1.0, which appears to create confusion about the meaningfulness of effect size statistics when the null hypothesis may be true. Given the repeated emphasis on reporting effect sizes, it is shown that in the face of F {$<$} 1.0 it is misleading to only report sample effect size estimates as often recommended. Causes of F ratios {$<$} 1.0 are reviewed, illustrated by a short simulation study. The calculation and interpretation of corrected and uncorrected effect size statistics under these conditions are discussed. Computing adjusted measures of association strength and incorporating effect size confidence intervals are helpful in an effort to reduce confusion surrounding results when sample sizes are small. Detailed recommendations are directed to authors, journal editors, and reviewers. (PsycINFO Database Record (c) 2020 APA, all rights reserved)},
  keywords = {Analysis of Variance,Effect Size (Statistical),F Test,Null Hypothesis Testing},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/T3LZHYZZ/2007-04214-004.html}
}

@article{allen_inflectional_2002-1,
  title = {Inflectional Regularity: {{Probing}} the Nature of Lexical Representation in a Cross-Modal Priming Task},
  author = {Allen, Mark and Badecker, William},
  year = {2002},
  journal = {Journal of Memory \& Language},
  volume = {46},
  number = {4},
  pages = {705--722},
  abstract = {Two cross-modal priming experiments with 42 undergraduates address issues concerning the representation \& processing of regular \& irregular morphology in English as well as methodological issues concerning the use of the cross-modal technique in lexical processing studies. Cross-modal inflectional priming facilitates regular verbs but not irregular verbs. Similar but morphologically unrelated words inhibit each other when presented cross-modally. Based on this effect for formbased inhibition, the authors interpret the regular/irregular verb priming asymmetry as a reflection of competition between orthographically similar verb forms which results in target inhibition. The authors found that orthographically dissimilar irregular verbs show strong priming facilitation. The authors account for this result in terms of a two-level lexicon in which irregular verb alternates have distinct form-level entries, but engage a common entry at an abstract morphological level of representation. In addition to serving as evidence concerning the representation of English verbal inflection, the findings from these experiments also set important limits on the claim that the cross-modal priming paradigm automatically circumvents form-related processing effects that are not morphological in character. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),[2343].,*Lexical,*Lexical Access,*Morphology,*Morphology (Language),*Priming,*Verbs,\&,Access,Adulthood,Human.,Human. Adulthood (18 yrs \& older).,Learning,Learning \& Memory [2343].,Memory,older).,yrs},
