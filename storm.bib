@article{davies_cross-cultural_1997,
  title = {Non-{{Symbolic Approaches}} to {{Artificial Intelligence}} and the {{Mind}} [and {{Discussion}}]},
  author = {Willshaw, David and Dennett, D. and Partridge, D.},
  year = {1994},
  month = oct,
  journal = {Philosophical Transactions of the Royal Society of London A: Mathematical, Physical and Engineering Sciences},
  volume = {349},
  number = {1689},
  pages = {87--102},
  issn = {1364-503X, 1471-2962},
  doi = {10.1098/rsta.1994.0115},
  urldate = {2015-12-19},
  abstract = {Current theories of artificial intelligence and the mind are dominated by the notion that thinking involves the manipulation of symbols. The symbols are intended to have a specific semantics in the sense that they represent concepts referring to objects in the external world and they conform to a syntax, being operated on by specific rules. I describe three alternative, non-symbolic approaches, each with a different emphasis but all using the same underlying computational model. This is a network of interacting computing units, a unit representing a nerve cell to a greater or lesser degree of fidelity in the different approaches. Computational neuroscience emphasizes the development and functioning of the nervous system; the approach of neural networks examines new algorithms for specific applications in, for example, pattern recognition and classification; according to the sub-symbolic approach, concepts are built up of entities called sub-symbols, which are the activities of individual processing units in a neural network. A frequently debated question is whether theories formulated at the subsymbolic level are `mere implementations' of symbolic ones. I describe recent work due to Foster, who proposes that it is valid to view a system at many different levels of description and that, whereas any theory may have many different implementations, in general sub-symbolic theories may not be implementations of symbolic ones.},
  langid = {english},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/9SP6J75B/87.html}
}

@article{davies_study_1998,
  title = {Fast Morphological Effects in First and Second Language Word Recognition},
  author = {Diependaele, Kevin and Du{\~n}abeitia, Jon Andoni and Morris, Joanna and Keuleers, Emmanuel},
  year = {2011},
  month = may,
  journal = {Journal of Memory and Language},
  volume = {64},
  number = {4},
  pages = {344--358},
  issn = {0749-596X},
  doi = {10.1016/j.jml.2011.01.003},
  urldate = {2015-12-09},
  abstract = {In three experiments we compared the performance of native English speakers to that of Spanish\textendash English and Dutch\textendash English bilinguals on a masked morphological priming lexical decision task. The results do not show significant differences across the three experiments. In line with recent meta-analyses, we observed a graded pattern of facilitation across stem priming with transparent suffixed primes (e.g., viewer\textendash view), opaque suffixed or pseudo-suffixed primes (e.g., corner\textendash corn) and form control primes (e.g., freeze\textendash free). Priming was largest in the transparent condition, smallest in the form condition and intermediate in the opaque condition. Our data confirm the hypothesis that bilinguals largely adopt the same processing strategies as native speakers (e.g., Lemh\"ofer et al., 2008), and constrain the hypothesis that bilinguals rely more heavily on whole-word processing in their second language (Clahsen, Felser, Neubauer, Sato, \&amp; Silva, 2010; Ullman, 2004, 2005). The observed pattern of morphological priming is in line with earlier monolingual studies, further highlighting the reality of semantic transparency effects in the initial stages of word recognition.},
  copyright = {All rights reserved},
  keywords = {Bilingual word recognition,Masked priming,morphological processing,semantic transparency},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/8W9H2UR7/Diependaele et al.. 2011.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/8XC33T8T/S0749596X11000040.html}
}

@article{davies_study_1998-1,
  title = {A Study of Colour Grouping in Three Languages: {{A}} Test of Linguistic Relativity Hypothesis},
  shorttitle = {A Study of Colour Grouping in Three Languages},
  author = {Davies, Ian R. L.},
  year = {1998},
  month = aug,
  journal = {British Journal of Psychology},
  volume = {89},
  number = {3},
  pages = {433--452},
  issn = {2044-8295},
  doi = {10.1111/j.2044-8295.1998.tb02695.x},
  urldate = {2013-12-16},
  abstract = {Conducted a cross-cultural study of the relationship between language and color cognition that tests the linguistic relativity hypothesis. We compared speakers of English, Russian and Setswana\textemdash languages with different numbers of basic color terms\textemdash on a color grouping task in order to see whether choice of color groups reflected the differences among the color lexicons of the 3 languages. Participants sorted a representative set of 65 colors into N groups (where N ranged from 2 to 12) based on their perceptual similarity. We assessed the similarities and differences among the choices of the 3 language samples, and focused in particular on 2 more specific questions. First, we tested the conjecture that levels of consensus over which tiles to group together should peak when N equalled the number of basic color terms in the language. Second, we focused on possible linguistic influences on color grouping in the GREEN-BLUE region of color space. Setswana uses a single term for this region, whereas English uses 2 terms and Russian uses 3 terms. The most striking feature of the results was the marked similarity of the groups chosen across the 3 language samples. In addition there were small but reliable differences in grouping associated with linguistic differences. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
  keywords = {Cognitive Processes,Color,Color Perception,Cross Cultural Differences,Ethnolinguistics,{grouping of color terms in English vs Russian vs Setswana languages, 18\textendash 82 yr olds from England vs Russia vs Botswana}}
}

