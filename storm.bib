  journal = {Journal of Experimental Psychology: General},
  volume = {106},
  number = {3},
  pages = {226--254},
  issn = {0096-3445},
  doi = {10.1037/0096-3445.106.3.226},
  urldate = {2017-01-10},
  abstract = {Prior to each target letter string presented visually to 120 university students in a speeded word\textendash nonword classification task, either \{bird, body, building,\} or \{xxx\} appeared as a priming event. Five types of word-prime/word-target trials were used: bird-robin, bird-arm, body-door, body-sparrow, and body-heart. The stimulus onset asynchrony (SOA) between prime and target letter string varied between 250 and 2,000 msec. At 2,000-msec SOA, reaction times (RTs) on bird-robin type trials were faster than on xxx-prime trials (facilitation), whereas RTs on bird-arm type trials were slower than on xxx-prime (inhibition). As SOA decreased, the facilitation effect on bird-robin trials remained constant, but the inhibition effect on bird-arm decreased until, at 250-msec SOA, there was no inhibition. For Shift conditions at 2,000-msec SOA, facilitation was obtained on body-door type trials and inhibition was obtained on body-sparrow type. These effects decreased as SOA decreased until there was no facilitation or inhibition. On body-heart type trials, there was an inhibition effect at 2,000 msec SOA, which decreased as SOA decreased until, at 250-msec SOA, it became a facilitation effect. Results support the theory of M. I. Posner and S. R. Snyder (1975) that postulated 2 distinct components of attention: a fast automatic inhibitionless spreading-activation process and a slow limited-capacity conscious-attention mechanism. (27 ref) (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
  keywords = {1977,ATTENTION,Classification (Cognitive Process),College Students,Human Channel Capacity,Human Information Storage,inhibitionless spreading activation \& limited-capacity attention,MEMORY,semantic priming \& retrieval from lexical memory in word-nonword classification task,Word Meaning},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/AAMDKS8Q/Neely - 1977 - Semantic priming and retrieval from lexical memory.pdf}
}

@article{kootstra_syntactic_2010-1,
  title = {Syntactic Alignment and Shared Word Order in Code-Switched Sentence Production: {{Evidence}} from Bilingual Monologue and Dialogue},
  shorttitle = {Syntactic Alignment and Shared Word Order in Code-Switched Sentence Production},
  author = {Kootstra, Gerrit Jan and {van Hell}, Janet G. and Dijkstra, Ton},
  year = {2010},
