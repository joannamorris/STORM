@article{butterworth_language_1983,
  title = {Auditory {{ERP}} Augmentation-Reduction and Methylphenidate Dosage Needs in Attention and Reading Disordered Children},
  author = {Dykman, Roscoe A. and Holcomb, Phillip J. and Ackerman, Peggy T. and McCray, David S.},
  year = {1983},
  journal = {Psychiatry Research},
  volume = {9},
  number = {3},
  pages = {255--269},
  abstract = {From their event-related potentials (ERPs) to tones of 4 intensity levels, 32 attention-disordered and/or reading-disabled males (mean age 8.5 yrs), who were recommended for a trial on methylphenidate, were classified as auditory augmenters or reducers. The augmenters were blindly titrated at significantly lower dosage levels than the reducers. The ameliorative effects of the drug, as assessed by teacher ratings, were more evident in hyperactive augmenters. Ss diagnosed either as hyperactive or hyperactive and reading disabled had steeper (or more augmenting) gradients than the nonhyperactive reading-disabled and attention-disordered Ss. Auditory ERP augmentation appears to be related to the clinical diagnosis of childhood hyperactivity, but auditory ERP augmenters have neither more augmenting RT gradients nor stronger or weaker nervous systems than reducers. Results suggest that reducers either require or tolerate higher dosages of methylphenidate than augmenters and that hyperactive augmenters derive greater clinical benefit from the stimulant than hyperactive reducers. (20 ref) (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(6-12,(birth-12,[3340].,*Attention,*Auditory,*Auditory Evoked Potentials,*Drug,*Drug Therapy,*Methylphenidate,*Reading,*Reading Disabilities,Age,Childhood,Clinical,Clinical Psychopharmacology [3340].,Disabilities,Disorders,Evoked,Human.,Human. Childhood (birth-12 yrs). School Age (6-12,Hyperkinesis,Potentials,Psychopharmacology,School,Therapy,yrs).},
  annotation = {Elsevier Scientific, US Empirical Study. Journal Article. English}
}

