@article{assink_prefixes_2000,
  title = {Dissociations in Processing Derivational Morphology: {{The}} Right Basal Ganglia Involvement.},
  author = {Marangolo, Paola and Piras, Fabrizio},
  year = {2008},
  month = jan,
  journal = {Neuropsychologia},
  volume = {46},
  number = {1},
  pages = {196--205},
  issn = {0028-3932},
  abstract = {In the neuropsychological literature, there is converging evidence for a dominant role of the left hemisphere in morphological processing. However, two right hemisphere patients were described with a clear dissociation between impaired derivational morphology and preserved inflectional processing. A recent fMRI experiment confirmed the involvement of right hemispheric areas in derivational processing and also suggested that the right basal ganglia contribute to deriving nouns from verbs. The present investigation was aimed at further demonstrating the role of the right hemisphere in derivational processing. Nine right brain damaged subjects were asked to perform different derivational tasks. Five out of nine subjects confirmed the previous data. They selectively failed only in deriving nouns from verbs (i.e. to observe \textrightarrow{} observation), mostly substituting the derived noun with a frequent inflectional suffix of the verb paradigm (i.e. observed instead of observation). Lesion subtraction analysis revealed that the caudate nucleus and the corona radiata, are the subcortical structures associated with the morphological deficit. Anatomical commonalities were found between lesion site in these patients and activations in healthy subjects. An account of these results in terms of a distributed bi-hemispheric neural network in complex language tasks is offered. (PsycINFO Database Record (c) 2012 APA, all rights reserved) (journal abstract)},
  keywords = {Basal Ganglia,basal ganglia involvement,Brain Damage,derivational morphology,dissociation,dissociations,language tasks,morphological processing,Morphology (Language),Right Hemisphere,right hemisphere patients}
}

@article{assink_prefixes_2000-1,
  title = {Prefixes as Access Units in Visual Word Recognition: {{A}} Comparison of {{Italian}} and {{Dutch}} Data},
  author = {Assink, Egbert M. and Vooijs, Caroline and Knuijt, Paul P.},
  year = {2000},
  journal = {Reading \& Writing},
  volume = {12},
  number = {3-4},
  pages = {149--168},
  abstract = {Two experiments were conducted to compare morphological processing of skilled (adults) and less skilled (children) Dutch readers (20-24 vs 11-12 yr olds, respectively). Prefixes and suffixes provide morphological information concerning meaning and/or function of the word stem. There is some empirical support for the hypothesis that these sublexical access units are functionally involved in the process of word recognition. These studies focus on the role of prefixes as access units in visual word recognition. Adult readers and children were presented a lexical decision task using polysyllabic word and nonword items. Prefix Frequency (high/low) and Quality (true/pseudo prefix ratio) were manipulated. Evidence was found for differential use of prefix information in both groups. Characteristic difference patterns between skilled and less skilled readers are discussed. It is concluded that the information accessed by prefixes is semantically combined with the subsequent word stem (root). Characteristics of the prefix plus stem combination largely determine speed of access to the complex word item processed. The results are compared to those of A. Laudanna et al (1994) with the Italian language. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,(6-12,(birth-12,(Language),[2820].,*Age,*Age Differences,*Lexical,*Lexical Decision,*Morphology,*Morphology (Language),*Reading,*Reading Skills,*Word,*Word Recognition,\&,Adulthood,Age,Childhood,Cognitive,Cognitive \& Perceptual Development [2820].,Decision,Development,Differences,Human.,Human. Childhood (birth-12 yrs). School Age (6-12,older).,Perceptual,Recognition,School,Skills,Young,yrs,yrs).},
  annotation = {Kluwer Academic, Netherlands Empirical Study. Journal Article. English}
}

@article{assink_reading_2000,
  title = {Relations versus {{Properties}} in {{Conceptual Combination}}},
  author = {Wisniewski, Edward J. and Love, Bradley C.},
  year = {1998},
  month = feb,
  journal = {Journal of Memory and Language},
  volume = {38},
  number = {2},
  pages = {177--202},
  issn = {0749-596X},
  doi = {10.1006/jmla.1997.2550},
  urldate = {2020-08-28},
  abstract = {We evaluate the hypothesis that people interpret noun-noun combinations by first attempting to find a plausible relation in which the constituents play different functional roles (as in ``a hawk thatpreyson robins'' forrobin hawk). If they do not succeed, they then attempt as a last resort to derive an interpretation based on properties of the modifier (``hawk with ared breast''). In one study, combinations with plausible relations were more often interpreted with properties when their constituents were highly similar. In a second study, property and relation interpretations of the same combination were selectively primed. In a final study, we show that property interpretations characterize the meanings of a number of familiar combinations in the English language. Taken together, the results of these studies run counter to the view that concepts are combined by first attempting to link them by a relation. We describe other reasons to doubt this view and discuss the implications of our findings for models of conceptual combination.},
  langid = {english},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/56ZYJMEY/Wisniewski and Love - 1998 - Relations versus Properties in Conceptual Combinat.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/HANPFZ67/S0749596X9792550X.html}
}

@incollection{astington_introduction:_2005,
  title = {The {{Role}} of {{Language}} in {{Theory-of-Mind Development}}: {{What Deaf Children Tell Us}}},
  shorttitle = {The {{Role}} of {{Language}} in {{Theory-of-Mind Development}}},
  booktitle = {Why Language Matters for Theory of Mind},
  author = {{de Villiers}, Peter A.},
  editor = {Astington, J. W. and Baird, J. A.},
  year = {2005},
  pages = {266--297},
  publisher = {{Oxford University Press}},
  address = {{New York,  NY,  US}},
  abstract = {This chapter discusses language and theory of mind (ToM) development from the perspective of deaf children. Children provide a strong test of a causal role for language in ToM development. Many deaf children have significantly delayed language acquisition, but they have age-appropriate nonverbal intelligence and active sociability. In this chapter the author first summarizes the findings of previous studies of ToM development in deaf children. These provide suggestive evidence that language acquisition is a fundamental contributor to children's mastery of reasoning about false beliefs. However, there are a number of flaws in the earlier studies, varying from small sample sizes to weaknesses in the measurement of ToM reasoning and especially of language acquisition in the deaf children. The author therefore describes the findings of a more comprehensive study of the relationship between language and false-belief reasoning in deaf children that was designed to eliminate these weaknesses. Two studies in particular are detailed in this chapter: one which studies the relationship between language and false-belief understanding in oral and ASL signing deaf children, and the second which studies teasing apart the relative effects of executive functioning and language on the mastery of false-belief reasoning by oral deaf children. The results of the first study argue for a strong causal role of language in the development of a representational ToM. The second study confirms the results of Study 1 that processing of false-complement clauses with communication verbs is the primary language predictor of the mastery of false-belief reasoning in the standard ToM tasks for deaf children.},
  copyright = {(c) 2012 APA, all rights reserved},
  isbn = {0-19-515991-8 (Hardcover); 978-0-19-515991-2 (Hardcover)},
  keywords = {*Cognitive Development,*Deaf,*Language Development,*Sign Language,*Theory of Mind,American Sign Language signing,Childhood Development,Cognitive Ability,Cognitive Development,Deaf,deaf children,executive functioning,false-belief reasoning,Language,Language Development,Sign Language,theory of mind,Verbal Communication}
}

@article{astington_representational_2005,
  title = {Maturational {{Constraints}} on {{Functional Specializations}} for {{Language Processing}}: {{ERP}} and {{Behavioral Evidence}} in {{Bilingual Speakers}}},
  shorttitle = {Maturational {{Constraints}} on {{Functional Specializations}} for {{Language Processing}}},
  author = {{Weber-Fox}, C and Neville, H},
  year = {1996},
  month = jul,
  journal = {Journal of Cognitive Neuroscience},
  volume = {8},
  number = {3},
  pages = {231--256},
  issn = {0898-929X},
  doi = {10.1162/jocn.1996.8.3.231},
  abstract = {Changes in several postnatal maturational processes during neural development have been implicated as potential mechanisms underlying critical period phenomena. Lenneberg hypothesized that maturational processes similar to those that govern sensory and motor development may also constrain capabilities for normal language acquisition. Our goal, using a bilingual model, was to investigate the hypothesis that maturational constraints may have different effects upon the development of the functional specializations of distinct sub within language. Subjects were 61 adult Chinese/English bilinguals who were exposed to English at different points in development: 1\textendash 3, 4\textendash 6, 7\textendash 10, 11\textendash 13, and after 16 years of age. Event-related brain potentials (ERPs) and behavioral responses were obtained as subjects read sentences that included semantic anomalies, three types of syntactic violations (phrase structure, specificity constraint, and subjacency constraint), and their controls. The accuracy in judging the grammaticality for the different types of syntactic rules and their associated ERPs was affected by delays in second language exposure as short as 1\textendash 3 years. By comparison the N400 response and the judgment accuracies in detecting semantic anomalies were altered only in subjects who were exposed to English after 11\textendash 13 and 16 years of age, respectively. Further, the type of changes occurring in ERPs with delays in exposure were qualitatively different for semantic and syntactic processing. All groups displayed a significant N400 effect in response to semantic anomalies, however, the peak latencies of the N400 elicited in bilinguals who were exposed to English between 11\textendash 13 and {$>$}16 years occurred later, suggesting a slight slowing in processing. For syntactic processing. the ERP differences associated with delays in exposure to English were observed in the morphology and distribution of components. Our findings are consistent w- th the view that maturational changes significantly constrain the development of the neural systems that are relevant for language and, further, that subsystems specialized for processing different aspects of language display different sensitive periods.},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/6SJN3CA6/login.html}
}

@incollection{astington_representational_2005-1,
  title = {Representational {{Development}} and {{False-Belief Understanding}}},
  booktitle = {Why Language Matters for Theory of Mind.},
  author = {Astington, Janet Wilde and Baird, Jodie A.},
  editor = {Astington, Janet Wilde and Baird, Jodie A.},
  year = {2005},
  pages = {163--185},
  publisher = {{Oxford University Press}},
  address = {{New York, NY US}},
  abstract = {(from the chapter) The importance of language for theory of mind, and more specifically for belief ascription, has become increasingly apparent during recent years. Language is important in many ways for many reasons, as the chapters in this volume clearly show. In this chapter the authors discuss the question of why language matters for the development of false-belief understanding in particular. "Language" is a broad term-different structural aspects as well as different functions fall under the scope of the term, and therefore it is important to first be precise about the aspects of language to which this chapter is referring. In the introduction (Astington \& Baird, this volume, chapter 1), the authors make two orthogonal distinctions. The first is a functional distinction, between language as an intra-individual representational system, on the one hand, and language as an inter-individual communication system, on the other. The second distinction is made at a structural level between language in general and mentalistic language in particular. With reference to the first distinction, the focus in this chapter is on language as an internal, representational system that can support the development of false-belief understanding. With reference to the second, the authors focus on the role of language in general until the final section of the chapter, when attention is shifted to mentalistic language in particular. (PsycINFO Database Record (c) 2012 APA, all rights reserved) (chapter)},
  isbn = {0-19-515991-8},
  keywords = {Attitudes,belief ascription,Cognitive Development,Concept Formation,false-belief understanding,Language,mentalistic language,representational development,theory of mind}
}

@article{astington_why_2005,
  title = {Brain Embodiment of Syntax and Grammar: {{Discrete}} Combinatorial Mechanisms Spelt out in Neuronal Circuits},
  shorttitle = {Brain Embodiment of Syntax and Grammar},
  author = {Pulverm{\"u}ller, Friedemann},
  year = {2010},
  month = mar,
  journal = {Brain and Language},
  series = {Embodied {{Language Processing}}: {{Neuroimaging}}, {{Behavioural}}, and {{Neurocomputational Perspectives}}},
  volume = {112},
  number = {3},
  pages = {167--179},
  issn = {0093-934X},
  doi = {10.1016/j.bandl.2009.08.002},
  urldate = {2015-03-17},
  abstract = {Neuroscience has greatly improved our understanding of the brain basis of abstract lexical and semantic processes. The neuronal devices underlying words and concepts are distributed neuronal assemblies reaching into sensory and motor systems of the cortex and, at the cognitive level, information binding in such widely dispersed circuits is mirrored by the sensorimotor grounding of form and meaning of symbols. Recent years have seen the emergence of evidence for similar brain embodiment of syntax. Neurophysiological studies have accumulated support for the linguistic notion of abstract combinatorial rules manifest as functionally discrete neuronal assemblies. Concepts immanent to the theory of abstract automata could be grounded in observations from modern neuroscience, so that it became possible to model abstract pushdown storage \textendash{} which is critical for building linguistic tree structure representations \textendash{} as ordered dynamics of memory circuits in the brain. At the same time, neurocomputational research showed how sequence detectors already known from animal brains can be neuronally linked so that they merge into larger functionally discrete units, thereby underpinning abstract rule representations that syntactically bind lexicosemantic classes of morphemes and words into larger meaningful constituents. Specific predictions of brain-based grammar models could be confirmed by neurophysiological and brain imaging experiments using MEG, EEG and fMRI. Neuroscience and neurocomputational research offering perspectives on understanding abstract linguistic mechanisms in terms of neuronal circuits and their interactions therefore point programmatic new ways to future theory-guided experimental investigation of the brain basis of grammar.},
  keywords = {associative memory,Brain,Discrete combinatorial rule,Grammar,Neuronal assembly,Pushdown store,Sequence detector,Syntax,Tree structure},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/CNE7KVQ3/S0093934X09000893.html}
}

@book{astington_why_2005-1,
  title = {Why Language Matters for Theory of Mind},
  author = {Astington, Janet Wilde and Baird, Jodie A.},
  editor = {Astington, Janet Wilde and Baird, Jodie A.},
  year = {2005},
  series = {Why Language Matters for Theory of Mind.},
  publisher = {{Oxford University Press}},
  address = {{New York, NY US}},
  abstract = {(from the jacket) Recently, work from leading researchers has demonstrated a strong relationship between theory of mind and language development. In this volume, these researchers give their answers to the fundamental question: Why does language matter for theory of mind? Theory of mind requires awareness of other peoples mental states-beliefs, desires, intentions, and emotions-that are revealed largely through conversation. As their language skills develop, children are able to take part in conversation, and it is this experience that allows them to acquire concepts of mental states and to master the syntax for representing them. For theory of mind to develop, children need both the social environment that provides the input and their own cognitive resources that make use of it. This volume is a state-of-the-art account of the latest conceptual and empirical advances in children's theory of mind. The contributors consider the links between theory of mind and language from both communicational and representational perspectives because language serves both of these different functions-that is, social communication in conversation and internal, verbal representation. They also present research from a variety of populations-typically developing children, children with autism, and deaf children. The result is a kaleidoscopic view of the complexity of the relationship between language and theory of mind that provides a variety of answers to the question of why language matters for theory of mind. (PsycINFO Database Record (c) 2012 APA, all rights reserved) (jacket)},
  isbn = {0-19-515991-8},
  keywords = {Childhood Development,Children,Cognitive Development,communicational perspectives,Interpersonal Communication,Language,Language Development,Psychosocial Development,representational perspectives,social communication,theory of mind,Verbal Communication,verbal representation}
}

