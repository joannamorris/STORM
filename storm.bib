  abstract = {Four experiments with 84 college students investigated the memory status of inflectional forms of verbs (i.e., s, ed, and ing), irregular past tense words, and adjective and nominal derivatives of verbs. The central question was whether inflections, irregular past tense forms, and derivatives have memory representations separate from their base verbs. Results indicate that inflections do not have memory representations separate from their base verbs, but irregular past tense words and both types of derivatives do. While the latter 3 types of items seem to have separate memory entries for base verbs and their variations, the entries do not appear to be independent. There is evidence that accessing the variation results in activation of the representation for the base verb and that this activation is a necessary step in the processing of the variation. (15 ref) (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(Language),[2343].,*Cognitive,*Cognitive Processes,*Human,*Human Information Storage,*Memory,*Morphology,*Morphology (Language),*Verbs,\&,Human.,Inflection,Information,Learning,Learning \& Memory [2343].,Memory,Processes,Storage},
  annotation = {Elsevier Science, US Journal Article. English}
}

@article{stanners_memory_1979-3,
  title = {Memory Representation for Prefixed Words},
  author = {Stanners, Robert F. and Neiser, James J. and Painton, Scott},
  year = {1979},
  journal = {Journal of Verbal Learning \& Verbal Behavior},
  volume = {18},
  number = {6},
  pages = {733--743},
  abstract = {Four experiments with 79 undergraduates examined the nature of memory representation for prefixed words. The investigation focused on whether a prefixed word is represented in memory as 2 separate morphemes, prefix and stem, or whether the representation is unitary. The experiments used 3 types of prefixed words: 2 had bound morphemes as stems (e.g., rejuvenate, progress), and the other type had free morpheme stems (e.g., untrue). All experiments support the idea that at least 1 of the representations of a prefixed word is unitary. Other results indicate that the stem and prefix of a prefixed word are partitioned in the course of reading the word. One effect of the partitioning process was that prefixed words with free morpheme stems (untrue) access both the unitary representation and that for the stem (true). Another effect of partitioning was that words having bound morpheme stems ( progress) access both their unitary memory representation and the memory representations of words with which they share a prefix. (8 ref) (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(Phonetic,[2343].,*Memory,*Words,*Words (Phonetic Units),\&,Human.,Learning,Learning \& Memory [2343].,Memory,Units)},
  annotation = {Elsevier Science, US Journal Article. English}
}

