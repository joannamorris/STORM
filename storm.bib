}

@article{ford_derivational_2010-1,
  title = {Derivational Morphology and Base Morpheme Frequency},
  author = {Ford, M. A. and Davis, M. H. and {Marslen-Wilson}, W. D.},
  year = {2010},
  month = jul,
  journal = {Journal of Memory and Language},
  volume = {63},
  number = {1},
  pages = {117--130},
  issn = {0749-596X},
  doi = {10.1016/j.jml.2009.01.003},
  urldate = {2022-01-08},
  abstract = {Morpheme frequency effects for derived words (e.g. an influence of the frequency of the base ``dark'' on responses to ``darkness'') have been interpreted as evidence of morphemic representation. However, it has been suggested that most derived words would not show these effects if family size (a type frequency count claimed to reflect semantic relationships between whole forms) were controlled. This study used visual lexical decision experiments with correlational designs to compare the influences of base morpheme frequency and family size on response times to derived words in English and to test for interactions of these variables with suffix productivity. Multiple regression showed that base morpheme frequency and family size were independent predictors of response times to derived words. Base morpheme frequency facilitated responses but only to productively suffixed derived words, whereas family size facilitated responses irrespective of productivity. This suggests that base morpheme frequency effects are independent of morpheme family size, depend on suffix productivity and indicate that productively suffixed words are represented as morphemes.},
  langid = {english},
  keywords = {Frequency effects,Lexical decision,Lexical representation,Morphology},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/QAE6DSYJ/S0749596X09000126.html}
}

