@article{stojanovik_williams_2004-1,
  title = {Williams Syndrome and Specific Language Impairment Do Not Support Claims for Developmental Double Dissociations and Innate Modularity},
  author = {Stojanovik, Vesna and Perkins, Mick and Howard, Sara},
  year = {2004},
  month = nov,
  journal = {Journal of Neurolinguistics},
  volume = {17},
  number = {6},
  pages = {403--424},
  issn = {0911-6044},
  doi = {10.1016/j.jneuroling.2004.01.002},
  urldate = {2013-12-05},
  abstract = {It has been argued that Williams Syndrome (WS) and Specific Language Impairment (SLI) show developmental double dissociations. This has been put forward as an argument that language is a module which is biologically programmed and develops independently of other cognitive abilities. However there have been very few studies which have directly compared the two populations. In this article we compare individuals with WS with individuals with SLI on a number of verbal and non-verbal standardised tests as well as on their language abilities in a narrative task. The participants with WS performed similarly to the participants with SLI on the verbal standardised tests and in the narrative task (there were no significant differences between the two groups of participants) although they performed significantly lower on the non-verbal standardised tests in comparison to the participants with SLI. The results suggest that although there are dissociations between WS and SLI in the non-verbal domain, the two populations are very similar in the domain of morpho-syntax, hence there is no support for developmental double dissociations between these two populations.},
  keywords = {Double dissociations,Modularity,Specific language impairment,Williams syndrome},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/88WTMR9U/Stojanovik et al. - 2004 - Williams syndrome and specific language impairment.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/WJBJTB93/Stojanovik et al. - 2004 - Williams syndrome and specific language impairment.html}
}

@article{stolz_levels_1998,
  title = {Effects of Priming in Word Retrieval},
  author = {Bowles, Nancy L. and Poon, Leonard W.},
  year = {1985},
  journal = {Journal of Experimental Psychology: Learning, Memory, \& Cognition},
  volume = {11},
  number = {2},
  pages = {272--283},
  abstract = {Conducted 3 studies with 69 undergraduates to investigate orthographic and semantic priming effects in word retrieval. In this paradigm, a prime is presented prior to the definition of a target word, and the task is to produce the target word. The authors view the process of word retrieval as requiring access from a conceptually organized semantic network to an orthographically and phonemically organized lexical network. Primes that were orthographically (and phonemically) related to the target words facilitated word retrieval. Both semantically related and unrelated primes inhibited word retrieval. Inhibition from both of these sources is attributed in part to the orthographic unrelatedness of these primes. This explanation is consistent with data from several other word retrieval studies (e.g., A. S. Brown [see PA, Vol 64:4730]). The word retrieval task is contrasted with the lexical decision task. It is suggested that the differential effects of orthographic and semantic priming in word retrieval and lexical decision are due to differences in retrieval processing between the 2 tasks. (33 ref) (PsycINFO Database Record (c) 2004 APA, all rights reserved).},
  keywords = {(18,(Learning),(Phonetic,[2343].,*Cues,*Orthography,*Recall,*Recall (Learning),*Semantics,*Words,*Words (Phonetic Units),\&,Adulthood,Human,Human Information Storage,Human.,Human. Adulthood (18 yrs \& older).,Information,Learning,Learning \& Memory [2343].,Memory,older).,Storage,Units),yrs},
  annotation = {American Psychological Assn, US English Empirical Study Peer Reviewed Journal}
}

@article{stolz_levels_1998-1,
