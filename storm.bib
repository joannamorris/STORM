@article{lambert_-line_2001-1,
  title = {On-Line Processing of Morphological Agreement in {{French}} Sentences},
  author = {Lambert, Ludovic and Kail, Michele},
  year = {2001},
  journal = {Annee Psychologique},
  volume = {101},
  number = {4},
  pages = {561--592},
  abstract = {This study examined on-line processing of morphological (verbal and nominal) agreement in French sentences. In a grammaticality judgment experiment, Ss were asked to detect as quickly as possible agreement violations: number and/or person in verbal agreement and gender and/or number in nominal agreement. Two factors were manipulated: 1) predictability (number of cues predicting an agreement); 2) violation size (number of dimensions violated). As predicted by structural constraints, nominal violations (intraphrasal) were detected more rapidly than verbal ones (interphrasal). Predictability and violation size did have an effect on judgments of grammaticality but their interactions were more complex than predicted. As hypothesized, for verbal agreement, high predictability and small violation size lengthened detection times and increased errors. For nominal agreement, the pattern is reversed: low predictability and large violation size lengthened detection times and increased errors. These contrastive results obtained in French are discussed in the light of some structural and perceptive factors. They underline the necessity to conduct further studies on each type of agreement independently. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),[2720].,*Grammar,*Morphology,*Morphology (Language),\&,Adulthood,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older).,Language,Linguistics,Linguistics \& Language \& Speech [2720].,Male.,older).,Speech,yrs},
  annotation = {French . Le traitement en temps reel des marques morphologiques d'accord dans des phrases en Francais.. Presses Universitaires de France, France Empirical Study. Journal Article. French}
}

