  title = {Speech Segmentation and Word Discovery: A Computational Perspective},
  shorttitle = {Speech Segmentation and Word Discovery},
  author = {Brent, null},
  year = {1999},
  month = aug,
  journal = {Trends in Cognitive Sciences},
  volume = {3},
  number = {8},
  pages = {294--301},
  issn = {1879-307X},
  abstract = {The segmentation and word discovery problem arises because speech does not contain any reliable acoustic analog of the blank spaces between words of printed English. As a result, children must segment the utterances they hear in order to discover the sound patterns of individual words in their language. A number of computational models have been proposed to explain how children segment speech and discover words, including ten new models in the last five years. This paper reviews all proposed models and organizes them according to their fundamental segmentation strategies, their processing characteristics, and the ways in which they use memory. All proposed models are found to use one of three fundamental strategies: the utterance-boundary strategy, the predictability strategy, or the word-recognition strategy. Selected predictions of the models are explained, their performance in computer simulations is summarized, and behavioral evidence bearing on them is discussed. Finally, ideas about how these diverse models might be synthesized into one comprehensive model are offered.},
  langid = {english},
  pmid = {10431183}
}

@article{frost_advances_2008-1,
  title = {Advances in Morphological Processing: {{An}} Introduction.},
  author = {Frost, Ram and Grainger, Jonathan and Carreiras, Manuel},
  year = {2008},
  month = nov,
  journal = {Language and Cognitive Processes},
  volume = {23},
  number = {7-8},
  pages = {933--941},
  issn = {1464-0732},
  abstract = {The current series of papers presents research conducted in a variety of languages: English, Danish, Dutch, Finnish, Chinese, and Spanish. It examines visual word recognition, speech perception, and speech production. Finally, it offers a wide array of experimental methods, including masked priming, long-term priming, the monitoring of eye movements, and recording of electrophysiological measures. The cross-linguistic perspective that characterizes the research effort of the present volume, as well as the previous ones, addresses the question whether there are qualitative differences in the principles of lexical organization and lexical processing in different alphabetic orthographies which arise from qualitative differences in morphological structure. To what extent can our findings in a given language be extended to another one? Any model of reading that considers the processing of morphological information should be concerned with its applicability to various writing systems and linguistic structures. The parallel monitoring of morphological processing in reading, speech perception and production provides converging evidence regarding the nature of morphemic representations. Finally, the richness of experimental methods highlights with impressive clarity whether our theoretical conclusions reflect the idiosyncratic features of a given task or not, pinpointing with increasing precision the various stages of morphological processing and their time courses. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
  keywords = {Language,Lexical Access,morphological processing,Morphology (Language),Orthography,Reading,speech perception,speech production,word recognition},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/WRWIN9R3/Frost et al. - 2008 - Advances in morphological processing An introduct.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/N46AXMD6/01690960802321925.html}
}

