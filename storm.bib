@article{feldman_repetition_1987,
  title = {Analogy in Morphology: {{Modeling}} the Choice of Linking Morphemes in {{Dutch}}},
  author = {Krott, Andrea and Baayen, Harald and Schreuder, Robert},
  year = {2001},
  journal = {Linguistics},
  volume = {39},
  number = {1},
  pages = {51--93},
  abstract = {Argues that a productive but not fully regular morphological phenomenon, the choice of linking morphemes in Dutch nominal compounds, is based on analogy. In Dutch, a linking -s- or -en can appear between the constituents of a nominal compound. The authors present production experiments, with native Dutch-speaking Ss, that show that the choice of linking morphemes in novel compounds is analogically determined by the distribution of linking morphemes in "constituent families". A "constituent family" is the set of existing compounds that share the first (or second) constituent with the novel compound. Results also show that in the case of derived pseudo-words as first constituents, it is the family of the suffix that influences the choice of the following linking morpheme. Simulation studies show that choices made by Ss are predicted with a high degree of accuracy using a machine-learning algorithm for analogy. These studies support the status of the constituent family as the primary basis for analogical prediction. A psycholinguistic model for analogy in the mental lexicon is outlined that does not give up symbolic representations and captures nondeterministic variation. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),[2720].,*Analogy,*Lexical,*Lexical Decision,*Morphemes,*Morphology,*Morphology (Language),*Psycholinguistics,\&,Adulthood,Decision,Human.,Human. Adulthood (18 yrs \& older).,Language,Linguistics,Linguistics \& Language \& Speech [2720].,older).,Speech,yrs}
}

@article{feldman_repetition_1987-1,
  title = {Repetition Priming Is Not Purely Episodic in Origin},
  author = {Feldman, Laurie Beth and Moskovljevic, Jasmina},
  year = {1987},
  journal = {Journal of Experimental Psychology: Learning, Memory, \& Cognition},
  volume = {13},
  number = {4},
  pages = {573--581},
  abstract = {The sufficiency of similarity among surface attributes of prime-target pairs to account for the pattern of facilitation obtained in the repetition priming paradigm was evaluated. In Experiment 1, morphological primes were singular, inflected case forms of Serbo-Croatian words, and visual similarity of prime and target was manipulated by alternating the two alphabets in which the Serbo-Croatian language is written. Results indicated that the magnitude of facilitation in the alphabetically alternating condition was not reduced relative to the nonalternating condition (rupi\^rupi vs. p\^rupi) which suggested that visual similarity is not a necessary condition for facilitation in the present task. In Experiment 2, related pairs included (a) base forms with diminutives, a class of highly productive and semantically predictable derivations marked in Serbo-Croatian by suffixes and (b) base words with morphologically unrelated monomorphemic words whose orthographic pattern encompassed the target in initial position and a sequence of letters in final position that elsewhere functions as a diminutive suffix. Collectively, the experiments suggested that structural similarity of prime and target is not a sufficient condition for facilitation in the repetition priming paradigm. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),[2340].,*Alphabets,*Lexical,*Lexical Decision,*Morphology,*Morphology (Language),*Priming,*Stimulus,*Stimulus Similarity,\&,Adulthood,Cognitive,Cognitive Processes [2340].,Decision,Human.,Human. Adulthood (18 yrs \& older).,older).,Processes,Reaction,reaction time,Similarity,Time,yrs},
  annotation = {American Psychological Assn, US Empirical Study. Journal Article. English}
}

@article{feldman_semantic_2002,
