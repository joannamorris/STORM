@article{feldman_beyond_1994-1,
  title = {Beyond Orthography and Phonology: {{Differences}} between Inflections and Derivations},
  author = {Feldman, Laurie Beth},
  year = {1994},
  journal = {Journal of Memory \& Language},
  volume = {33},
  number = {4},
  pages = {442--470},
  abstract = {Investigated the influence of morphological structure in 2 types of word recognition tasks with Serbian materials. In Exps 1, 2, and 3 (a total of 99 undergraduates), the pattern of facilitation to target decision latencies was examined following morphologically-related primes in a repetition priming task. Although all morphologically related primes facilitated targets relative to an unprimed condition, inflectionally related primes produced significantly greater effects than did derivationally related primes. In Exps 4, 5, and 6, 76 undergraduates were required to segment and shift an underlined portion from 1 word onto a 2nd word and to name the result aloud. The shifted letter sequence was sometimes morphemic. Results indicate that the constituent morphemic structure of a word contributes to word recognition, and morphemic structure is more transparent for inflectional than for derivational formations. (PsycINFO Database Record (c) 2003 APA, all rights reserved).},
