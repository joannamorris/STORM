@article{forster_lexical_1973,
  title = {Event-Related Potential ({{ERP}}) Responses to Violations of Inflectional and Derivational Rules of {{Finnish}}},
  author = {Leinonen, Alina and Brattico, Pauli and J{\"a}rvenp{\"a}{\"a}, Miika and Krause, Christina M.},
  year = {2008},
  month = jul,
  journal = {Brain Research},
  volume = {1218},
  pages = {181--193},
  issn = {0006-8993},
  doi = {10.1016/j.brainres.2008.04.049},
  urldate = {2013-03-20},
  abstract = {Event-related potentials (ERP) were used to investigate the electrophysiological correlates of inflectional and derivational morphology. The participants were presented with visual sentences containing critical words in which either inflectional, derivational or both rules (combined violation) of Finnish were violated. Inflectional anomalies violated a number agreement of a noun with a previous auxiliary word. Derivational violations included a word-internal selectional restriction violation, i.e., a root and suffix category violation. Combined violations contained both a number and a category violation. The phonemic length of the critical words was controlled. Inflectional violations elicited a bilateral negative effect in the 450\textendash 550~ms time window, which was interpreted as an anterior negativity (AN) effect. Inflectional violations also elicited a late positivity (P600) effect. Derivational violations elicited an N400-like negativity effect, followed by the P600 effect. The P600 effects in the derivational and inflectional violation conditions summated linearly in the combined violation condition. The results are discussed with respect to the hypothesis that inflectional and derivational processes are independent and elicited in parallel in the online language comprehension.},
  keywords = {Adult,Analysis of Variance,Brain Mapping,Comprehension,Comprehension: physiology,Contingent Negative Variation,derivation,Electroencephalography,event-related potential,Evoked Potentials,Evoked Potentials: physiology,Female,Finland,Finnish morphology,Humans,Inflection,Linear summation,Male,Middle Aged,Morphology,Photic Stimulation,Psycholinguistics,reaction time,Semantics},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/FS6AMHQN/Leinonen et al.. 2008.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/SXR8JRQE/S0006899308009840.html}
}

@article{forster_lexical_1973-1,
  title = {Lexical Access and Naming Time},
  author = {Forster, Kenneth I. and Chambers, Susan M.},
  year = {1973},
  month = dec,
  journal = {Journal of Verbal Learning and Verbal Behavior},
  volume = {12},
