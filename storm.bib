  shorttitle = {Visual Statistical Learning in Infancy},
  author = {Kirkham, Natasha Z and Slemmer, Jonathan A and Johnson, Scott P},
  year = {2002},
  month = mar,
  journal = {Cognition},
  volume = {83},
  number = {2},
  pages = {B35-B42},
  issn = {0010-0277},
  doi = {10.1016/S0010-0277(02)00004-5},
  urldate = {2020-10-11},
  abstract = {The rapidity with which infants come to understand language and events in their surroundings has prompted speculation concerning innate knowledge structures that guide language acquisition and object knowledge. Recently, however, evidence has emerged that by 8 months, infants can extract statistical patterns in auditory input that are based on transitional probabilities defining the sequencing of the input's components (Science 274 (1996) 1926). This finding suggests powerful learning mechanisms that are functional in infancy, and raises questions about the domain generality of such mechanisms. We habituated 2-, 5-, and 8-month-old infants to sequences of discrete visual stimuli whose ordering followed a statistically predictable pattern. The infants subsequently viewed the familiar pattern alternating with a novel sequence of identical stimulus components, and exhibited significantly greater interest in the novel sequence at all ages. These results provide support for the likelihood of domain general statistical learning in infancy, and imply that mechanisms designed to detect structure inherent in the environment may play an important role in cognitive development.},
  langid = {english},
  keywords = {Domain general learning mechanism,Infancy,Visual statistical learning},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/X92KA5W5/Kirkham et al. - 2002 - Visual statistical learning in infancy evidence f.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/IH7Z37YH/S0010027702000045.html}
}

@article{service_recall_2002-1,
  title = {Recall of Morphologically Complex Forms Is Affected by Memory Task but Not Dyslexia},
  author = {Service, Elisabet and Tujulin, Anna-Mari},
  year = {2002},
  journal = {Brain \& Language},
  volume = {81},
  number = {1-3},
  pages = {42--54},
  abstract = {The authors studied the effect of morphological complexity on working memory in list recall tasks with base words, inflected words and derived words in a morphologically rich language: Finnish. Simple serial recall was compared to complex working memory tasks, combining word recall with sentence verification in normally reading participants (15 boys and 14 girls, aged 7 yrs 11 mo-9 yrs 1 mo), dyslexic children (6 boys and 6 girls, aged 8 yrs 1 mo-9 yrs), and adults (19 Ss, aged 18-46 yrs). The normally reading children performed better than dyslexic children on both memory tasks and a test of morphology. Base words were better recalled than morphologically complex words. Memory was better for derived than inflected words in simple but not complex span tasks. There was no interaction between word type and reading group and thus no suggestion of dyslexia being associated with specific problems to represent complex morphology in working memory. Morphological processing in working memory appeared to depend on the task. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,(30-39,(40-64,(6-12,(birth-12,(Language),(Learning),(Phonetic,[3253].,*Cognitive,*Cognitive Processes,*Dyslexia,*Morphology,*Morphology (Language),*Recall,*Recall (Learning),*Short,*Short Term Memory,\&,Adulthood,Age,Childhood,Childhood Development,Complexity,Development,Disorders,Female.,Human.,Human. Male. Female. Childhood (birth-12 yrs). Sch,Learning,Learning Disorders [3253].,Male.,Memory,Middle,older).,Processes,Recall,School,Serial,Serial Recall,Stimulus,Stimulus Complexity,Term,Thirties,Units),Words,Words (Phonetic Units),Young,yrs,yrs).},
  annotation = {Academic Press, US Conference Proceedings/Symposia. Empirical Study. The data were presented at the aforementioned conference Journal Article. English}
}

