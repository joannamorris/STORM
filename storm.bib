  doi = {http://dx.doi.org.ezproxy.library.tufts.edu/10.1016/0028-3932(94)90073-6},
  urldate = {2013-05-02},
  abstract = {Tested 43 undergraduates on a lexical decision task in 2 experiments. 22 Ss participated in Exp 1, and 21 Ss participated in Exp 2. Function words, content words, and pronounceable nonwords (pseudowords) were presented tachistoscopically either in the left or the right visual field (RVF) or with identical copies flashed simultaneously to both visual half-fields. Function words showed an RVF advantage; for content words the RVF advantage was absent. Compared to either of the unilateral modes of presentation, bilateral (BI) presentation of identical word stimuli improved accuracy and latency significantly. The BI advantage was largest for content words, and was highly significant for function words in latency and accuracy. The BI gain was absent for nonwords. Results suggest that the lexicons of the left and right hemisphere can "collaborate" when processing the same linguistic stimuli. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
  langid = {english},
  keywords = {Interhemispheric Interaction (major),Left Hemisphere (major),Lexical Decision (major),Neologisms,Right Hemisphere (major),Word Meaning,Words (Phonetic Units)},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/RJTSVMFI/mohr1994.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/S4HIZWQ5/1.html}
}

