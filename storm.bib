@article{harris_young_1989,
  title = {Young {{Children}}'s {{Theory}} of {{Mind}} and {{Emotion}}},
  author = {Harris, Paul L. and Johnson, Carl N. and Hutton, Deborah and Andrews, Giles and Cooke, Tim},
  year = {1989},
