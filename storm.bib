@article{laws_linguistic_1995-1,
  title = {Linguistic Structure and Non-Linguistic Cognition: {{English}} and {{Russian}} Blues Compared},
  shorttitle = {Linguistic Structure and Non-Linguistic Cognition},
  author = {Laws, Glynis and Davies, Ian and Andrews, Catherine},
  year = {1995},
  month = feb,
  journal = {Language and Cognitive Processes},
  volume = {10},
  number = {1},
  pages = {59--94},
  issn = {1464-0732},
  doi = {10.1080/01690969508407088},
  abstract = {Investigated the influence of linguistic structure on nonlinguistic cognition by comparing behavior of Russian students (all in their early twenties) visiting England and of English university students in 4 experiments involving the blue region of color space. Exp 1 tested Russian naming behavior. Five Russian Ss named color chips spanning the blue-violet to blue-green range in 1 word. Then, 9 Russian and 9 English Ss were asked to choose which of 3 color stimuli was most different from the other 2. Exp 2 measured similarity judgments in 8 Russian and 6 English Ss who compared color chips covering 3 hues and 3 levels of lightness. Exp 3 measured categorization behavior in 9 Russian and 9 English Ss. In Exp 4, 30 English Ss learned to differentiate the blue region of color space along a light\textendash dark dimension more readily than the comparable green region. Overall, results show no evidence that language influences perception. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
  keywords = {Classification (Cognitive Process),Cognition,Color Perception,{linguistic structure, nonlinguistic cognition \& categorization involving blue region of color space, Russian vs English college students, England},Linguistics}
}

@article{ledwidge_recommendations_2018-1,
  title = {Recommendations for {{Developing}} an {{EEG Laboratory}} at a {{Primarily Undergraduate Institution}}},
  author = {Ledwidge, Patrick and Foust, Jeremy and Ramsey, Adam},
  year = {2018},
  month = dec,
  journal = {Journal of Undergraduate Neuroscience Education},
  volume = {17},
  number = {1},
  pages = {A10-A19},
  issn = {1544-2896},
  urldate = {2022-01-03},
  abstract = {Given its relatively low cost and minimal required space, an EEG laboratory provides the most feasible human cognitive neuroscience technique to implement at primarily undergraduate institutions (PUI). However, neuroscience programs at PUIs may be deterred from incorporating EEG methods into their research programs and/or classrooms due to limited funds and resources. This article provides recommended guidelines for faculty researchers looking to set up an EEG lab at their host PUIs with an emphasis on feasibility. We offer considerations regarding infrastructure, equipment, personnel, and potential sources of funding. A case study is also provided, describing the successful implementation and development of an EEG/ERP lab at a Midwest PUI, Baldwin Wallace University. Our goal is to offer diverse options for starting a new, or revitalizing an existing, EEG lab. We contend that such a laboratory at a PUI will advance undergraduate students' access to interdisciplinary neuroscience research and curricular opportunities.},
  pmcid = {PMC6312138},
  pmid = {30618494},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/QMA6Y6EQ/Ledwidge et al. - 2018 - Recommendations for Developing an EEG Laboratory a.pdf}
}

