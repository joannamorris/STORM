}

@article{feldman_contribution_1991-1,
  title = {The Contribution of Morphology to Word Recognition},
  author = {Feldman, Laurie Beth},
  year = {1991},
  journal = {Psychological Research},
  volume = {53},
  number = {1},
  pages = {33--41},
  abstract = {Investigated evidence of morphological processing in 3 word recognition tasks. In Exp 1, an interaction of base morpheme ambiguity and affix characteristics was obtained, and was interpreted as evidence that all morphological constituents of a word participate in lexical access. In Exp 2, facilitation due to morphological relatedness of prime and target was observed with Serbo-Croatian materials in the lexical decision and naming versions of the repetition priming task, and results were interpreted as evidence of a morphological principle of organization among whole-word forms in the lexicon. In Exp 3, morphological affixes of both English and Serbo-Croatian words were segmented from a source word and affixed to a target word more rapidly than phonemically matched controls. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),[2340].,*Lexical,*Lexical Access,*Morphology,*Morphology (Language),*Word,*Word Recognition,\&,Access,Adulthood,Cognitive,Cognitive Processes [2340].,Human.,Human. Adulthood (18 yrs \& older).,older).,Processes,Recognition,yrs},
