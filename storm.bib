@article{rousselet_animal_2004-1,
  title = {Animal and Human Faces in Natural Scenes: {{How}} Specific to Human Faces Is the {{N170 ERP}} Component?},
  shorttitle = {Animal and Human Faces in Natural Scenes},
  author = {Rousselet, Guillaume A. and Mac{\'e}, Marc J.-M. and {Fabre-Thorpe}, Mich{\`e}le},
  year = {2004},
  month = jan,
  journal = {Journal of Vision},
  volume = {4},
  number = {1},
  pages = {13--21},
  issn = {, 1534-7362},
  doi = {10.1167/4.1.2},
  urldate = {2014-08-27},
  abstract = {The N170 is an event-related potential component reported to be very sensitive to human face stimuli. This study investigated the specificity of the N170, as well as its sensitivity to inversion and task status when subjects had to categorize either human or animal faces in the context of upright and inverted natural scenes. A conspicuous N170 was recorded for both face categories. Pictures of animal faces were associated with a N170 of similar amplitude compared to pictures of human faces, but with delayed peak latency. Picture inversion enhanced N170 amplitude for human faces and delayed its peak for both human and animal faces. Finally, whether processed as targets or non-targets, depending on the task, both human and animal face N170 were identical. Thus, human faces in natural scenes elicit a clear but non-specific N170 that is not modulated by task status. What appears to be specific to human faces is the strength of the inversion effect.},
  langid = {english},
  keywords = {animal faces,event-related potentials,human faces,N170,natural scenes,rapid visual categorization},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/HJ435RNR/Rousselet et al. - 2004 - Animal and human faces in natural scenes How spec.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/ZIZQFGFV/Rousselet et al. - 2004 - Animal and human faces in natural scenes How spec.html}
}

@article{royle_frequency_2002,
  title = {Colour through the Thalamus},
  author = {Martin, Paul R.},
