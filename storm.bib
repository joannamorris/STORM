@article{wang_training_1999,
  title = {On-{{Line Individual Differences}} in {{Statistical Learning Predict Language Processing}}},
  author = {Misyak, Jennifer B. and Christiansen, Morten H. and Tomblin, J. Bruce},
  year = {2010},
  journal = {Frontiers in Psychology},
  volume = {1},
  issn = {1664-1078},
  doi = {10.3389/fpsyg.2010.00031},
  urldate = {2019-05-15},
  abstract = {Considerable individual differences in language ability exist among normally developing children and adults. Whereas past research have attributed such differences to variations in verbal working memory or experience with language, we test the hypothesis that individual differences in statistical learning may be associated with differential language performance. We employ a novel paradigm for studying statistical learning on-line, combining a serial-reaction time task with artificial grammar learning. This task offers insights into both the timecourse of and individual differences in statistical learning. Experiment 1 charts the micro-level trajectory for statistical learning of nonadjacent dependencies and provides an on-line index of individual differences therein. In Experiment 2, these differences are then shown to predict variations in participants\&\#8217; on-line processing of long-distance dependencies involving center-embedded relative clauses. The findings suggest that individual differences in the ability to learn from experience through statistical learning may contribute to variations in linguistic performance.},
  langid = {english},
  keywords = {artificial grammar,individual differences,language processing,relative clauses,serial-reaction time,statistical learning},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/Z5QNNMJZ/Misyak et al. - 2010 - On-Line Individual Differences in Statistical Lear.pdf}
}

@article{wang_training_1999-1,
  title = {Training {{American}} Listeners to Perceive {{Mandarin}} Tones},
  author = {Wang, Yue and Spence, Michelle M. and Jongman, Allard and Sereno, Joan A.},
  year = {1999},
  journal = {Journal of the Acoustical Society of America},
  volume = {106},
  number = {6},
  pages = {3649--3658},
  abstract = {Since the acquisition of Mandarin tones has been found to be difficult for native nontonal learners, it provides an ideal case for the study of suprasegmental training. By training 16 native English-speaking American college students to perceive the Mandarin tones, the goal of the present study was to examine whether auditory training, which has been shown to be effective at the segmental level, can be applied to the acquisition of non-native suprasegmental contrasts. All Ss were 19-29 yr-olds who had 1 or 2 semesters of Mandarin Chinese language courses. The study demonstrated that the perception of Mandarin tones can be improved using a simple training task, indicating that the procedure which has been adopted in training the acquisition of non-native segmental contrasts can also be applied at the suprasegmental level. The improvement gained in training was generalized to new stimuli (18\% increase), and new talkers and stimuli (25\% increase), and was retained by listeners six months later (21\% increase). (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,(Language),[2326].,*Foreign,*Foreign Language Learning,*Morphology,*Morphology (Language),*Speech,*Speech Perception,*Speech Pitch,\&,Adulthood,Auditory,Auditory \& Speech Perception [2326].,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older). Y,Language,Learning,Male.,older).,Perception,Pitch,Speech,Young,yrs,yrs).},
  annotation = {Acoustical Society of American, US Empirical Study. Journal Article. English}
}

