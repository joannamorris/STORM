@article{endsley_situation_2000,
  title = {Situation {{Models}}: {{An Avenue}} to the {{Modeling}} of {{Mental Models}}},
  shorttitle = {Situation {{Models}}},
  author = {Endsley, Mica R.},
  year = {2000},
  month = jul,
  journal = {Proceedings of the Human Factors and Ergonomics Society Annual Meeting},
  volume = {44},
  number = {1},
  pages = {61--64},
  issn = {2169-5067},
  doi = {10.1177/154193120004400117},
  urldate = {2020-01-08},
  abstract = {The concept of a mental model is well ensconced in the human factors literature. Mental models are used to explain a wide variety of human behavior and designers are directed to design to fit the mental model of the user. Its real utility in practical application remains beyond our grasp, however, due to significant difficulties in creating usable representations of these mental models that can provide descriptive arid predictive capabilities. This paper explores the concept of mental models and a highly related concept, the situation model. Through the situation model, a method for abstracting user knowledge and creating usable models of their mental model is explored. This technique shows promise as a means of more effectively capturing this elusive construct.},
  langid = {english}
}

@article{engelhardt_executive_2017,
  title = {Executive Function and Intelligence in the Resolution of Temporary Syntactic Ambiguity: {{An}} Individual Differences Investigation},
  shorttitle = {Executive Function and Intelligence in the Resolution of Temporary Syntactic Ambiguity},
  author = {Engelhardt, Paul E. and Nigg, Joel T. and Ferreira, Fernanda},
  year = {2017},
  month = jul,
  journal = {The Quarterly Journal of Experimental Psychology},
  volume = {70},
  number = {7},
  pages = {1263--1281},
  issn = {1747-0218},
  doi = {10.1080/17470218.2016.1178785},
  abstract = {In the current study, we examined the role of intelligence and executive functions in the resolution of temporary syntactic ambiguity using an individual differences approach. Data were collected from 174 adolescents and adults who completed a battery of cognitive tests as well as a sentence comprehension task. The critical items for the comprehension task consisted of object/subject garden paths (e.g., While Anna dressed the baby that was small and cute played in the crib), and participants answered a comprehension question (e.g., Did Anna dress the baby?) following each one. Previous studies have shown that garden-path misinterpretations tend to persist into final interpretations. Results showed that both intelligence and processing speed interacted with ambiguity. Individuals with higher intelligence and faster processing were more likely to answer the comprehension questions correctly and, specifically, following ambiguous as opposed to unambiguous sentences. Inhibition produced a marginal effect, but the variance in inhibition was largely shared with intelligence. Conclusions focus on the role of individual differences in cognitive ability and their impact on syntactic ambiguity resolution. (PsycINFO Database Record (c) 2018 APA, all rights reserved)},
  keywords = {Adolescent,Adult,Age Differences,Cognitive Ability,Comprehension,Executive function,Executive Function,{Factor Analysis, Statistical},Female,Garden-path sentence,Humans,Individual differences,Individual Differences,Individuality,Intelligence,Linear Models,Male,Neuropsychological Tests,Reproducibility of Results,Spatial Behavior,Syntactic ambiguity resolution,Young Adult}
}

@article{engelhardt_shock_2013-1,
  title = {Shock, Diaschisis and von {{Monakow}}.},
  author = {Engelhardt, E. and Gomes, M. D.},
  year = {2013},
  journal = {Arquivos de neuro-psiquiatria},
  doi = {10.1590/0004-282X20130067},
  abstract = {The concept of shock apparently emerged in the middle of the 18th century as an occurrence observed experimentally after spinal cord transection, and identified as "shock" phenomenon one century later. The concept of shock apparently emerged in the middle of the 18th century (Whyett) as an occurrence observed experimentally after spinal cord transection, and identified as "shock" phenomenon one century later (Hall). The concept was extended (Brown-S\'equard) and it was suggested that brain lesions caused functional rupture in regions distant from the injured one ("action \`a distance"). The term "diaschisis" (von Monakow), proposed as a new modality of shock, had its concept broadened, underpinned by observations of patients, aiming at distinguishing between symptoms of focal brain lesions and transitory effects they produced, attributable to depression of distant parts of the brain connected to the injured area. Presently, diaschisis is related mainly to cerebrovascular lesions and classified according to the connection fibers involved, as proposed by von Monakow. Depression of metabolism and blood flow in regions anatomically separated, but related by connections with the lesion, allows observing diaschisis with neuroimaging.},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/PSBZ6WKK/Engelhardt and Gomes - 2013 - Shock, diaschisis and von Monakow..pdf}
}

@article{engelmann_how_2019,
  title = {How the Input Shapes the Acquisition of Verb Morphology: {{Elicited}} Production and Computational Modelling in Two Highly Inflected Languages},
  shorttitle = {How the Input Shapes the Acquisition of Verb Morphology},
  author = {Engelmann, Felix and Granlund, Sonia and Kolak, Joanna and Szreder, Marta and Ambridge, Ben and Pine, Julian and Theakston, Anna and Lieven, Elena},
  year = {2019},
  month = may,
  journal = {Cognitive Psychology},
  volume = {110},
  pages = {30--69},
  issn = {0010-0285},
  doi = {10.1016/j.cogpsych.2019.02.001},
  urldate = {2019-05-24},
  abstract = {The aim of the present work was to develop a computational model of how children acquire inflectional morphology for marking person and number; one of the central challenges in language development. First, in order to establish which putative learning phenomena are sufficiently robust to constitute a target for modelling, we ran large-scale elicited production studies with native learners of Finnish (N\,=\,77; 35\textendash 63\,months) and Polish (N\,=\,81; 35\textendash 59\,months), using a novel method that, unlike previous studies, allows for elicitation of all six person/number forms in the paradigm (first, second and third person; singular and plural). We then proceeded to build and test a connectionist model of the acquisition of person/number marking which not only acquires near adult-like mastery of the system (including generalisation to unseen items), but also yields all of the key phenomena observed in the elicited-production studies; specifically, effects of token frequency and phonological neighbourhood density of the target form, and a pattern whereby errors generally reflect the replacement of low frequency targets by higher-frequency forms of the same verb, or forms with the same person/number as the target, but with a suffix from an inappropriate conjugation class. The findings demonstrate that acquisition of even highly complex systems of inflectional morphology can be accounted for by a theoretical model that assumes rote storage and phonological analogy, as opposed to formal symbolic rules.},
  keywords = {Computational modelling,Cross-linguistic,Elicited production,Language acquisition,Morphology,Neural networks,Verb marking},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/97CDH27L/Engelmann et al. - 2019 - How the input shapes the acquisition of verb morph.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/YIK82LND/S0010028518301105.html}
}

