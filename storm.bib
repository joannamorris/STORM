@article{pastizzo_does_2002-1,
  title = {Does Prime Modality Influence Morphological Processing?},
  author = {Pastizzo, Matthew John and Feldman, Laurie Beth},
  year = {2002},
  journal = {Brain \& Language},
  volume = {81},
  number = {1-3},
  pages = {28--41},
  abstract = {Facilitation among morphologically related words generally is impervious to the prefixed or suffixed structure of primes and targets. A notable exception arises, however, when both primes and targets are suffixed. More specifically, when primes are auditory and targets are visual, facilitation for a suffixed target (e.g., payment) is absent when it follows a prime (e.g., payable) that is morphologically related and suffixed (W. Marslen-Wilson, L. K. Tyler, R. Waksler, and L. Older; 1994). To account for null facilitation, Marslen-Wilson and his colleagues posit inhibitory links between suffixes of morphological relatives. The present study assesses the generality of the "suffix-suffix' effect in 245 college students. When morphological facilitation is assessed relative to an orthographically related baseline, suffixed primes facilitate derivationally as well as inflectionally related morphological targets when primes are visual as well as auditory in both the lexical decision and naming tasks. The present findings call into question lexical models that posit inhibitory links between suffixes of morphological relatives. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),(Phonetic,[2340].,*Cognitive,*Cognitive Processes,*Lexical,*Lexical Decision,*Morphology,*Morphology (Language),*Naming,*Syntax,\&,Adulthood,Cognitive,Cognitive Processes [2340].,Decision,Human.,Human. Adulthood (18 yrs \& older).,older).,Processes,Units),Words,Words (Phonetic Units),yrs},
