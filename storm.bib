@article{seidenberg_explaining_2000,
  title = {Explaining Derivational Morphology as the Convergence of Codes},
  author = {Seidenberg, Mark S. and Gonnerman, Laura M.},
  year = {2000},
  month = sep,
  journal = {Trends in Cognitive Sciences},
  volume = {4},
  number = {9},
  pages = {353--361},
  issn = {1364-6613},
  doi = {10.1016/S1364-6613(00)01515-1},
  abstract = {Language users have a remarkable ability to create, produce and comprehend complex words. Words such as undercut and bakery appear to be composed of units, traditionally called morphemes, that recombine in rule-like ways to form other words, such as underline and cannery. However, morphological systems are quasiregular: they are systematic and productive but admit many seemingly irregular forms. Thus, bakery is related to bake and cannery to can but what is the groce in grocery? There is no bread in sweetbreads, liver in deliver, corn in corner or ginger in gingerly. Such words exhibit partial regularities concerning the correspondences between form and meaning, the treatment of which has important implications for linguistic and psycholinguistic theories. This article describes an approach to morphological phenomena called the convergence theory, in which morphology is a graded, inter-level representation that reflects correlations among orthography, phonology and semantics. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {complex words,convergence theory,derivational morphology,Language,linguistics,Linguistics,morphemes,Morphemes,morphological systems,Morphology (Language),orthography,Orthography,phonology,Phonology,psycholinguistics,Psycholinguistics,semantics,Semantics,Theories,Words (Phonetic Units)},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/3DIBMGXI/Seidenberg, Gonnerman. 2000.pdf}
}

@inproceedings{seidenberg_reading_1983,
  title = {On {{Hapax Legomena}} and {{Morphological Productivity}}},
