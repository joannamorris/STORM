  author = {Rugg, Michael D},
  year = {1990},
  journal = {Memory \& Cognition},
  volume = {18},
  number = {4},
  pages = {367--379},
  issn = {0090-502X},
  abstract = {Recorded event-related brain potentials (ERPs) while 16 young adult Ss detected nonwords interspersed among sequences of words of high or low frequency of occurrence. In Phase 1, a proportion of the words was repeated after 6 intervening items. In Phase 2, the words were repeats of items or were new. Unrepeated low-frequency words evoked larger N400 components than did high-frequency items. The interactive effects of frequency and repetition suggest that these variables act jointly at multiple loci during the processing of a word. The specificity of the post-500-msec repetition effect for low-frequency words may reflect a process responsive to a discrepancy between the intra- and extraexperimental familiarity of words. (PsycINFO Database Record (c) 2006 APA, all rights reserved).},
  keywords = {(18,[2530].,*Evoked,*Evoked Potentials,*Stimulus,*Stimulus Discrimination,*Word,*Word Frequency,\&,Adulthood,Discrimination,Electrophysiology,Electrophysiology [2530].,Frequency,Human.,Human. Adulthood (18 yrs \& older),older),Parameters,Potentials,Stimulus,Stimulus Parameters,yrs}
