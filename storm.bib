@article{holcomb_cognitive_1985-1,
  title = {Cognitive Event-Related Brain Potentials in Children with Attention and Reading Deficits},
  author = {Holcomb, Phillip J. and Ackerman, Peggy T. and Dykman, Roscoe A.},
  year = {1985},
  journal = {Psychophysiology},
  volume = {22},
  number = {6},
  pages = {656--667},
  abstract = {Event-related potentials (ERPs) were recorded from 4 groups of male children (aged 8 yrs to 11 yrs 11 mo)\textendash 24 reading disabled, 45 attentional deficit disorder with and without hyperactivity, and 24 normal controls. Ss pressed a button to a low probability nonsense syllable (target, p = .168) and ignored all other events, which included a high probability nonsense syllable (nontarget, p = .664) and either low probability (category, p = .168) symbols (Block 1) or 3-letter words (Block 2). The amplitudes of several late ERP components and the latency of the late positive (P3) component were examined. Results indicate that the overall amplitude of P3 was significantly smaller in all clinical groups than in controls, but the difference in P3 amplitude between targets and nontargets was smaller only in the 2 attentional deficit groups. Reading disabled Ss had smaller P3 and Pc components to words than to symbols, while controls had equivalent values. The N2 component had a different scalp distribution for words and symbols but did not differentiate reading disabled Ss from controls. P3 latency was significantly longer in the 3 clinical groups than in controls, but only the attentional deficit groups showed an increase in P3 latency across blocks of the task. (28 ref) (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(6-12,(birth-12,[3250].,*Attention,*Attention Deficit Disorder,*Cognitive,*Cognitive Discrimination,*Cortical,*Cortical Evoked Potentials,*Hyperkinesis,*Reading,*Reading Disabilities,\&,Age,Autism,Childhood,Deficit,Developmental,Developmental Disorders \& Autism [3250].,Disabilities,Discrimination,Disorder,Disorders,Evoked,Human.,Human. Childhood (birth-12 yrs). School Age (6-12,Potentials,School,Stimuli,Verbal,Verbal Stimuli,yrs).},
