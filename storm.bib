@phdthesis{coltheart_drc:_2001,
  title = {Aging, Orthographic Overlap, and Implicit versus Explicit Word Retrieval},
  author = {Abrams, Lise},
  year = {1998},
  abstract = {Two experiments tested the hypothesis that orthographic overlap can affect word retrieval through inhibition and facilitation, analogous to effects observed in visual word recognition. Young and older adult participants rated a list of words for pleasantness, where the initial three letters of each word were orthographically identical for three words in the list (shared orthography) or were orthographically unique, shared by no other words in the list (unshared orthography). Word retrieval was assessed either explicitly via cued recall or implicitly via a word-stem completion test. Response times (RTs) were recorded in addition to the degree of correct recall and repetition priming, the standard retrieval measures in these tests. In explicit retrieval, orthographic overlap had no significant effect on RT, whereas correct cued recall was greater for words with shared orthography than for words with unshared orthography, and this increase in recall was the same for both age groups. There were age differences on both dependent measures, with older adults having slower RTs to recall words and less recall than young adults. In implicit retrieval, orthographic overlap significantly facilitated completion of word stems for both measures, evidenced by faster RTs and more repetition priming of shared orthography words than unshared orthography words, and these facilitation effects did not vary by age. Further, there were no overall age differences in RTs or in amount of repetition priming. Results suggest that orthographic overlap can facilitate word retrieval depending on the nature and measure of the retrieval. Both age and orthographic overlap dissociate implicit and explicit word retrieval, and these dissociations may be useful in clarifying theories of implicit and explicit memory and their relationships to language. (PsycINFO Database Record (c) 2004 APA, all rights reserved).},
  keywords = {(18,[2340];,[2800].,*Age,*Age Differences,*Orthography,*Reaction,*Reaction Time,*Word,*Word Recognition,\&,Adulthood,Cognitive,Cognitive Processes [2340],Developmental,Developmental Psychology [2800].,Differences,Human.,Human. Adulthood (18 yrs \& older).,older).,Processes,Psychology,Recognition,Time,yrs},
  annotation = {Dissertation Abstracts International: Section B: The Sciences \& Engineering. Vol 58(8-B), Feb 1998, 4480. US: Univ Microfilms International. English Empirical Study}
}

@article{coltheart_drc:_2001-1,
  ids = {coltheart_drc_2001},
  title = {{{DRC}}: {{A}} Dual Route Cascaded Model of Visual Word Recognition and Reading Aloud},
  shorttitle = {{{DRC}}},
  author = {Coltheart, Max and Rastle, Kathleen and Perry, Conrad and Langdon, Robyn and Ziegler, Johannes},
  year = {2001},
  journal = {Psychological Review},
  volume = {108},
  number = {1},
  pages = {204--256},
  issn = {1939-1471(Electronic);0033-295X(Print)},
  doi = {10.1037/0033-295X.108.1.204},
  abstract = {This article describes the Dual Route Cascaded (DRC) model, a computational model of visual word recognition and reading aloud. The DRC is a computational realization of the dual-route theory of reading, and is the only computational model of reading that can perform the 2 tasks most commonly used to study reading: lexical decision and reading aloud. For both tasks, the authors show that a wide variety of variables that influence human latencies influence the DRC model's latencies in exactly the same way. The DRC model simulates a number of such effects that other computational models of reading do not, but there appear to be no effects that any other current computational model of reading can simulate but that the DRC model cannot. The authors conclude that the DRC model is the most successful of the existing computational models of reading.},
  copyright = {(c) 2012 APA, all rights reserved},
  keywords = {dual route cascaded model of visual word recognition \& reading aloud}
}

