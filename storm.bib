@article{osterhout_superficial_1999-1,
  title = {A Superficial Resemblance Does Not Necessarily Mean You Are Part of the Family: {{Counterarguments}} to {{Coulson}}, {{King}} and {{Kutas}} (1998) in the {{P600}}/{{SPS-P300}} Debate},
