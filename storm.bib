@article{dasgupta_girls_2014,
  title = {Girls and {{Women}} in {{Science}}, {{Technology}}, {{Engineering}}, and {{Mathematics}}: {{STEMing}} the {{Tide}} and {{Broadening Participation}} in {{STEM Careers}}},
  shorttitle = {Girls and {{Women}} in {{Science}}, {{Technology}}, {{Engineering}}, and {{Mathematics}}},
  author = {Dasgupta, Nilanjana and Stout, Jane G.},
  year = {2014},
