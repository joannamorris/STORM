  volume = {38},
  number = {2},
  pages = {203--224},
  abstract = {Addressed the question of whether it is possible to read a word for meaning without phonological mediation. 56 undergraduates participated in a task involving semantic categorization. Ss decided whether or not each target word belonged to the category "words with definable meanings" (e.g., PLANK, PINT) or the category "given names" (e.g., TRENT, PAM). To test phonological mediation, latencies to respond to regular definable words (e.g., PLANK) and irregular definable words (e.g., PINT) were compared. No regularity effect was observed, despite these same words showing a difference in a naming task. Thus the semantic task was shown to be insensitive to a phonological effect. The possibility that the long response times of the semantic categorization task washed out any regularity effect was dismissed on the grounds that both a word frequency effect and a letter transposition effect (e.g., CLUE vs CALM, cf. CLAM) were observed using this task. It is concluded that phonological mediation is not a prerequisite for semantic access. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Cognitive,[2340].,*Classification,*Classification (Cognitive Process),*Lexical,*Lexical Access,*Phonology,*Semantics,*Word,*Word Recognition,\&,Access,Adulthood,Cognitive,Cognitive Processes [2340].,Human.,Human. Adulthood (18 yrs \& older).,older).,Process),Processes,Recognition,yrs},
  annotation = {Elsevier Science, US Empirical Study. Journal Article. English}
}

@article{taft_letter_2017,
  title = {Letter Coding in Visual Word Recognition: {{The}} Impact of Embedded Words},
  shorttitle = {Letter Coding in Visual Word Recognition},
  author = {Taft, Marcus and Xu, Joe and Li, Sonny},
  year = {2017},
  month = feb,
  journal = {Journal of Memory and Language},
  volume = {92},
  pages = {14--25},
  issn = {0749-596X},
  doi = {10.1016/j.jml.2016.05.002},
  urldate = {2017-01-10},
  abstract = {Nonword classification responses are examined in this study to establish the amount of interference arising from the presence of an embedded word. In Experiment 1, greater interference is found from an initial embedding (e.g., furb vs lurb, cf. fur) than a final embedding (e.g., clid vs clig, cf. lid). In addition, an ``outer'' embedding (e.g., jomb vs vomb, cf. job) generates interference that is no greater than for an initial embedding. These results are inconsistent with the idea of left-to-right parsing, while accounts of word recognition that center on open bigrams or the spatial coding of letters require additional processes. Instead, the results are interpreted within a model of word parsing and lexical access that incorporates subsyllabic structures; an account that is supported in Experiments 2 and 3 by the critical finding that an initially embedded word interferes more when it ends in a consonant (e.g., the fur of furb, the shadow of shadowl) than with a vowel (e.g., the tea of teaf, the coffee of coffeep).},
  keywords = {Embedded words,Letter coding,lexical access,Subsyllabic structure,Word parsing,WORD recognition},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/ZXPPN3EW/S0749596X16300286.html}
}

