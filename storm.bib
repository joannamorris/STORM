  author = {Pinker, Steven and Ullman, Michael T},
  year = {2003},
  journal = {Trends in Cognitive Sciences},
  volume = {7},
  number = {3},
  pages = {108--109},
  issn = {1364-6613},
  abstract = {Responds to the comments by Seidenberg and Joanisse (see record 2003-02850-005) and Ramscar (see record 2003-02850-006) on the authors' article (see record 2002-08093-004), which defended the theory that irregular past-tense forms are stored in the lexicon, a division of declarative memory. The reply suggests that the successes of connectionist models in domains of morphology and spelling derive from the sensitivity of superpositional memory to frequency and similarity, and that Ramscar's findings differed from those of Pinker and Ullman because Ramscar stacked the deck against finding an effect of morphology. (PsycINFO Database Record (c) 2006 APA, all rights reserved).},
