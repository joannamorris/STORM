}

@article{baldwin_language_2005,
  title = {Neuroimaging of Syntax and Syntactic Processing},
  author = {Grodzinsky, Yosef and Friederici, Angela D},
  year = {2006},
  month = apr,
  journal = {Current Opinion in Neurobiology},
  series = {Cognitive Neuroscience},
  volume = {16},
  number = {2},
  pages = {240--246},
  issn = {0959-4388},
  doi = {10.1016/j.conb.2006.03.007},
  urldate = {2015-03-18},
  abstract = {Recent results challenge and refine the prevailing view of the way language is represented in the human brain. Syntactic knowledge and processing mechanisms that implement syntax in use are mapped onto neural tissue in experiments that harness both syntactic concepts and imaging technologies to the study of brain mechanisms in healthy and impaired populations. In the emerging picture, syntax is neurologically segregated, and its component parts are housed in several distinct cerebral loci that extend beyond the traditional ones \textemdash{} Broca's and Wernicke's regions in the left hemisphere. In particular, the new brain map for syntax implicates portions of the right cerebral hemisphere.},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/XBANGCZB/S0959438806000328.html}
}

@incollection{baldwin_language_2005-1,
  title = {Language {{Promotes Structural Alignment}} in the {{Acquisition}} of {{Mentalistic Concepts}}},
  booktitle = {Why Language Matters for Theory of Mind.},
  author = {Baldwin, Dare A. and Saylor, Megan M.},
  editor = {Astington, Janet Wilde and Baird, Jodie A.},
  year = {2005},
  pages = {123--143},
  publisher = {{Oxford University Press}},
  address = {{New York, NY US}},
  abstract = {This chapter explores how language promotes structural alignment in the acquisition of mentalistic concepts. The authors advocate viewing mental-state concepts as emerging in the context of social and linguistic interaction. The purpose of this chapter is to pursue this issue as a specific case of Gentner and colleagues' (e.g., Gentner \& Medina, 1998) general proposal that language is a catalyst for abstraction in part because it serves as an aid to analogical reasoning and inductive inference. The authors' hypothesis is that language invites one to compare people's behavior across distinct action scenarios, thereby helping to highlight commonalities, foster attention to relevant differences, and promote inferences about non-obvious sources of commonality and difference. The authors delve into the process of mentalistic abstraction, wherein ideas are formed to explain notions such as belief, desire, and intention. The authors provide a far-from-comprehensive sketch of Gentner et al's conceptualization of analogical reasoning, followed by brief coverage of findings indicating that language facilitates analogical reasoning. With this background established, the chapter turns to applying these ideas to the case of present interest: construction of mentalistic concepts. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
  isbn = {0-19-515991-8},
  keywords = {abstraction,Cognitive Development,commonality,Concept Formation,difference,Language,Language Development,mentalistic concepts,Psychological Development,Reasoning,Semantics,structural alignment}
}

@article{balhorn_rise_2004-1,
  title = {The {{Rise}} of {{Epicene They}}},
  author = {Balhorn, Mark},
  year = {2004},
  month = jun,
  journal = {Journal of English Linguistics},
  volume = {32},
  number = {2},
  pages = {79--104},
  publisher = {{SAGE Publications Inc}},
  issn = {0075-4242},
  doi = {10.1177/0075424204265824},
  urldate = {2022-03-28},
  abstract = {A pronoun pattern that is common in both spoken and written English is some form of they with singular, generic antecedents, as in, ?Everyone was so pleased with themselves.? This article addresses the question of how long such a pattern has been in the language and how a pattern that clearly violates number agreement could become so ubiquitous. An examination of recent scholarship as well as the historical record reveals that the origins of this usage rest in the fourteenth century and that its frequency has increased since then. Using current theories of agreement, the author presents a hypothesis for how and when this pattern could have entered the language. Further examination of the historical record supports the hypothesis that loss of grammatical gender in the thirteenth century was crucial for the introduction of generic they into the language.}
}

@article{balling_probability_2012,
  ids = {rastle_place_2018},
  title = {The Place of Morphology in Learning to Read in {{English}}},
  author = {Rastle, Kathleen},
  year = {2019},
  month = jul,
  journal = {Cortex},
  series = {Structure in Words: The Present and Future of Morphological Processing in a Multidisciplinary Perspective},
  volume = {116},
  pages = {45--54},
  issn = {0010-9452},
  doi = {10.1016/j.cortex.2018.02.008},
  urldate = {2019-12-05},
  abstract = {Morphology is a major organising principle of English and other alphabetic languages, but has been largely neglected in theories of reading acquisition. In this article, I develop the view that learning to appreciate morphological relationships may be a vital part of acquiring a direct mapping between printed words and their meanings, represented in the ventral brain pathway of the reading network. I show that morphology provides an important degree of regularity across this mapping in English, and suggest that this regularity is directly associated with irregularity in the mapping between spelling and sound. I further show that while children in primary school display explicit knowledge of morphological relationships, there is scant evidence they show the rapid morphological analysis of printed words that skilled readers exhibit. These findings suggest that the acquisition of long-term morphological knowledge may be associated with the ongoing development of reading expertise. Implications for reading instruction are discussed.},
  langid = {english},
  keywords = {Form-meaning relationship,Language,Morphology,Reading acquisition,Reading instruction,Skilled reading},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/6QWB28SF/Rastle - 2019 - The place of morphology in learning to read in Eng.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/7HX69WWW/Rastle - 2018 - The place of morphology in learning to read in Eng.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/QMKCL9KN/Rastle - 2019 - The place of morphology in learning to read in Eng.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/833N29A3/S0010945218300571.html;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/ED9Q6C7L/S0010945218300571.html;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/GVA9WV9T/S0010945218300571.html}
}

@article{balling_probability_2012-1,
  title = {Probability and Surprisal in Auditory Comprehension of Morphologically Complex Words.},
  author = {Balling, Laura Winther and Baayen, R. Harald},
  year = {2012},
  month = oct,
  journal = {Cognition},
  volume = {125},
  number = {1},
  pages = {80--106},
  issn = {0010-0277},
  abstract = {Two auditory lexical decision experiments document for morphologically complex words two points at which the probability of a target word given the evidence shifts dramatically. The first point is reached when morphologically unrelated competitors are no longer compatible with the evidence. Adapting terminology from Marslen-Wilson (1984), we refer to this as the word's initial uniqueness point (UP1). The second point is the complex uniqueness point (CUP) introduced by Balling and Baayen (2008), at which morphologically related competitors become incompatible with the input. Later initial as well as complex uniqueness points predict longer response latencies. We argue that the effects of these uniqueness points arise due to the large surprisal (Levy, 2008) carried by the phonemes at these uniqueness points, and provide independent evidence that how cumulative surprisal builds up in the course of the word co-determines response latencies. The presence of effects of surprisal, both at the initial uniqueness point of complex words, and cumulatively throughout the word, challenges the Shortlist B model of Norris and McQueen (2008), and suggests that a Bayesian approach to auditory comprehension requires complementation from information theory in order to do justice to the cognitive cost of updating probability distributions over lexical candidates. (PsycINFO Database Record (c) 2013 APA, all rights reserved) (journal abstract)},
  keywords = {auditory comprehension surprisal,Auditory Perception,Comprehension,Lexical Decision,morphologically complex words,Morphology (Language),Probability,word recognition}
}

