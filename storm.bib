@article{feldman_morphological_1999-1,
  title = {Morphological {{Priming}}: {{The Role}} of {{Prime Duration}}, {{Semantic Transparency}}, and {{Affix Position}}},
  shorttitle = {Morphological {{Priming}}},
  author = {Feldman, Laurie Beth and Soltano, Emily G.},
  year = {1999},
  month = jun,
  journal = {Brain and Language},
  volume = {68},
  number = {1\textendash 2},
  pages = {33--39},
  issn = {0093-934X},
  doi = {10.1006/brln.1999.2077},
  urldate = {2013-11-14},
  abstract = {Patterns of facilitation in three lexical decision studies reveal several properties of morphologically complex words that influence processing. In one study, effects of morphological (VOWED\textendash VOW) similarity are contrasted with effects of either semantic (PLEDGE\textendash VOW) or orthographic (VOWEL\textendash VOW) similarity at two prime durations to show the distinctiveness of morphological processing. In a second study, we compare morphologically complex forms that are semantically transparent (CASUALLY\textendash CASUALNESS) with opaque forms (CASUALTY\textendash CASUALNESS) and show that opaque relatives are more similar to VOWEL\textendash VOW type pairs than to transparent relatives. In the third study, we examine facilitation for complex targets (CALCULATION) preceded by prefixed (MISCALCULATE) and suffixed (CALCULATOR) relatives and show that the position of the base morpheme influences processing under cross-modal but not under purely visual presentation conditions. Taken collectively, under comparable presentation conditions, semantic transparency but not base morpheme position constrains morphological processing.},
  keywords = {(Language),[2340].,*Cognitive,*Cognitive Processes,*Lexical,*Lexical Decision,*Morphology,*Morphology (Language),*Priming,*Stimulus,*Stimulus Parameters,{base morpheme position \& prime characteristics \& morphological vs semantic vs orthographic similarity, morphological processing \& lexical decision latency},Cognitive,Cognitive Processes,Cognitive Processes [2340].,cross-modal priming,Decision,Duration,Human.,Key Words: morphological processing,Lexical Decision,morphological similarity,Morphology (Language),orthographic similarity,Orthography,Parameters,prefixed forms,Priming,Processes,semantic similarity,semantic transparency,Semantics,Stimulus,Stimulus Duration,Stimulus Parameters,suffixed forms},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/CDR6KETK/S0093934X99920771.html}
}

@article{feldman_morphological_2001,
  title = {The Intervenor Effect in Masked Priming: {{How}} Does Masked Priming Survive across an Intervening Word?},
  shorttitle = {The Intervenor Effect in Masked Priming},
  author = {Forster, Kenneth I.},
  year = {2009},
  month = jan,
  journal = {Journal of Memory and Language},
  volume = {60},
  number = {1},
  pages = {36--49},
  issn = {0749-596X},
  doi = {10.1016/j.jml.2008.08.006},
  urldate = {2013-07-16},
  abstract = {Four masked priming experiments are reported investigating the effect of inserting an unrelated word between the masked prime and the target. When the intervening word is visible, identity priming is reduced to the level of one-letter-different form priming, but form priming is largely unaffected. However, when the intervening word is itself masked, form priming is eliminated altogether, while identity priming remains at the level observed with a visible intervenor. Normal priming effects are obtained when the order of the masked prime and the intervenor is reversed. It is suggested that identity priming consists of two effects, one that depends on the prime being adjacent to the target (a semantic effect), and one that is independent of adjacency (a form effect).},
  keywords = {Form priming,Identity priming,Lexical Decision,Masked priming},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/X33ZUPWU/S0749596X08000843.html}
}

@article{feldman_morphological_2001-1,
