@article{kail_verb_1997,
  title = {Constraints on Movement Phenomena in Sentence Processing: {{Evidence}} from Event-Related Brain Potentials},
  author = {McKinnon, Richard and Osterhout, Lee},
  year = {1996},
  journal = {Language \& Cognitive Processes},
  volume = {11},
  number = {5},
  pages = {495--523},
  abstract = {Event-related potentials (ERPs) were recorded from 13 scalp electrodes while 18-32-yr-old Ss read sentences, some of which contained a violation of constraints on constituent movement (subjacency or the empty category principle). In two experiments, the first word that indicated a violation elicited a widely distributed positive-going wave with an onset between 250 and 300 msec and a duration of several hundred milliseconds. This effect was similar to the positive shifts that have been observed in response to other types of syntactic violations (P600), but qualitatively distinct from the N400 response elicited by semantically/pragmatically anomalous words. These findings are taken as an indication that movement constraints can be applied during the earliest stages of sentence processing, perhaps in conjunction with the creation of phrase structure. We evaluate the hypothesis that the P600 is a general electrophysiological marker of syntactic anomaly. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,(30-39,[2340].,*Cognitive,*Cognitive Processes,*Cortical,*Cortical Evoked Potentials,*Sentence,*Sentence Comprehension,*Sentence Structure,\&,Adulthood,Cognitive,Cognitive Processes [2340].,Comprehension,Evoked,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older). Y,Male.,older).,Potentials,Processes,Structure,Thirties,Young,yrs,yrs).},
  annotation = {Taylor \& Francis, US Empirical Study. Journal Article. English}
}

@article{kail_verb_1997-1,
  title = {Verb Agreement Processing in French: {{A}} Study of on-Line Grammaticality Judgments},
  author = {Kail, Michele and Bassano, Dominique},
  year = {1997},
  journal = {Language \& Speech},
  volume = {40},
  number = {1},
  pages = {25--46},
  abstract = {Studied how the syntactic constraint of verb agreement (VAG) in French sentence processing (SP) could be influenced by morphological, structural, and semantic information in SP. Ss were 22 monolingual native French-speaking adults (mean age 31 yrs). An experimental on-line error detection (ED) paradigm was used to examine how variations in linguistic material (LM) interposed between the subject and the verb affected the detection of number-VAG violations. The time needed for the detection of verbal agreement errors was found to depend largely on the structure and nature of the LM interposed between the subject noun and the verb. Also, ED took more time when the local noun in the interposed sequence mismatched the subject noun in number, than when the 2 nouns matched. Finally, ED seemed to be affected mainly by subject head-noun lexical properties rather than by local noun features. Data suggest that structural properties of the LM interposed between the subject head noun and the verb, and morphological information relative to head noun number, can influence VAG processing. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(30-39,(Language),[2720].,*Morphology,*Morphology (Language),*Semantics,*Sentence,*Sentence Structure,*Syntax,*Verbs,\&,Adulthood,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older). T,Language,Linguistics,Linguistics \& Language \& Speech [2720].,Male.,older).,Speech,Structure,Thirties,yrs,yrs).},
  annotation = {Kingston Press, England Empirical Study. Journal Article. English}
}

@article{kambe_parafoveal_2002,
  title = {To {{See}} or Not to {{See}}: {{The Need}} for {{Attention}} to {{Perceive Changes}} in {{Scenes}}},
