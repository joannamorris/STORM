}

@article{peng_how_1999,
  title = {Toward a Brain-Based Componential Semantic Representation},
  author = {Binder, Jeffrey R. and Conant, Lisa L. and Humphries, Colin J. and Fernandino, Leonardo and Simons, Stephen B. and Aguilar, Mario and Desai, Rutvik H.},
  year = {2016},
  month = may,
  journal = {Cognitive Neuropsychology},
  volume = {33},
  number = {3-4},
  pages = {130--174},
  issn = {0264-3294},
  doi = {10.1080/02643294.2016.1147426},
  urldate = {2018-08-31},
  abstract = {Componential theories of lexical semantics assume that concepts can be represented by sets of features or attributes that are in some sense primitive or basic components of meaning. The binary features used in classical category and prototype theories are problematic in that these features are themselves complex concepts, leaving open the question of what constitutes a primitive feature. The present availability of brain imaging tools has enhanced interest in how concepts are represented in brains, and accumulating evidence supports the claim that these representations are at least partly 'embodied' in the perception, action, and other modal neural systems through which concepts are experienced. In this study we explore the possibility of devising a componential model of semantic representation based entirely on such functional divisions in the human brain. We propose a basic set of approximately 65 experiential attributes based on neurobiological considerations, comprising sensory, motor, spatial, temporal, affective, social, and cognitive experiences. We provide normative data on the salience of each attribute for a large set of English nouns, verbs, and adjectives, and show how these attribute vectors distinguish a priori conceptual categories and capture semantic similarity. Robust quantitative differences between concrete object categories were observed across a large number of attribute dimensions. A within- versus between-category similarity metric showed much greater separation between categories than representations derived from distributional (latent semantic) analysis of text. Cluster analyses were used to explore the similarity structure in the data independent of a priori labels, revealing several novel category distinctions. We discuss how such a representation might deal with various longstanding problems in semantic theory, such as feature selection and weighting, representation of abstract concepts, effects of context on semantic retrieval, and conceptual combination. In contrast to componential models based on verbal features, the proposed representation systematically relates semantic content to large-scale brain networks and biologically plausible accounts of concept acquisition. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {Brain,Cognition,cognitive neuroscience,Cognitive Neuroscience,concept representation,embodied cognition,Lexical Access,Semantics},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/ZU2E2JYN/Binder et al. - 2016 - Toward a brain-based componential semantic represe.pdf}
}

@incollection{peng_how_1999-1,
  title = {How Is Access Representation Organized? {{The}} Relation of Polymorphemic Words and Their Morphemes in {{Chinese}}},
  shorttitle = {How Is Access Representation Organized?},
  booktitle = {Reading {{Chinese}} Script:  {{A}} Cognitive Analysis.},
  author = {Peng, Danling and Liu, Ying and Wang, Chunmao},
  editor = {Wang, Jian and Inhoff, Albrecht W. and Chen, Hsuan-Chih},
  year = {1999},
  pages = {65--89},
  publisher = {{Lawrence Erlbaum Associates Publishers}},
  address = {{Mahwah, NJ US}},
  abstract = {(from the chapter) A number of models of lexical access were developed in the past years. The major disagreement between models focuses on the issue of how lexical information is accessed. Does it use a strategy involving decomposing of elements of speech, or are they processed as a unit? No single model has yet been devised to settle such disagreement because both decomposed morphemes and whole words appear to exist in the access to word representation. The conflicting evidence in the literature suggests that hidden factors (e.g., surface frequency and transparency) influence the organization of access representation.  This chapter describes three experiments with 68 college students that were designed to test for factors that influence the organization of access representation in Chinese two-morpheme words. Results confirmed the presence of two factors. However, both interactive-activation models (M. Taft, 1994) and augmented addressed morpheme models (A. Caramazza et al, 1988) as well as other empirical research failed to account for the results. An Inter/Intra Connection model was proposed and adapted to the problem of word processing. (PsycINFO Database Record (c) 2012 APA, all rights reserved) (chapter)},
  isbn = {0-8058-2478-2},
  keywords = {(18,(Language),[2340].,[2720].,*Lexical,*Morphemes,*Morphology,*Psycholinguistics,*Written,\&,Access,Adulthood,Cognitive,{factors that influence organization of access representation in Chinese two-morpheme words, college students},Human.,Language,lexical access,Linguistics,Morphemes,Morphology (Language),older).,Processes,PSYCHOLINGUISTICS,Speech,Written Language,yrs}
}

