@article{taft_lexical_1998-1,
  title = {Lexical Representation of Gender: {{A}} Quasiregular Domain},
  author = {Taft, Marcus and Meunier, Fanny},
  year = {1998},
  journal = {Journal of Psycholinguistic Research},
  volume = {27},
  number = {1},
  pages = {23--45},
  abstract = {Hypothesized that the gender of low-frequency irregular French word forms would take longer to classify than low-frequency regular forms, according to neural network models. 16 native French speaking Ss participated in Exps 1 and 2. Exp 1 looked for a gender regularity effect in visually presented French nouns of high and low frequency. In Exp 2, place-names with highly predictive endings were presented for gender decision in order to determine the relative importance of article information and orthographic information in generating the gender of a word. In Exp 3, 30 different native French speakers were shown an adjective paired with the place-name and asked whether the form of the adjective was appropriate. A regularity effect was found in Exp 1, but it did not interact with word frequency. It was further revealed that there was difficulty in making gender decisions (Exp 2) and gender verification responses (Exp 3) to words whose endings were highly informative of gender, but whose associated article was not. How a neural network might handle these results is discussed, and an alternative account is considered whereby there are 2 potential sources of gender information, lexical and nonlexical, with the latter being used to confirm the former. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,[2720].,*Language,*Lexical,*Lexical Decision,*Word,*Word Frequency,*Word Recognition,\&,Adulthood,Decision,Frequency,Human.,Human. Adulthood (18 yrs \& older).,Language,Linguistics,Linguistics \& Language \& Speech [2720].,older).,Recognition,Speech,yrs},
  annotation = {Kluwer Academic, US Empirical Study. Journal Article. English}
}

@article{taft_morphemic_1999,
  title = {What Animals Can Teach Us about Human Language: The Phonological Continuity Hypothesis},
