@article{kempley_effects_1982-1,
  title = {The Effects of Priming with Regularly and Irregularly Related Words in Auditory Word Recognition},
  author = {Kempley, S. and Morton, John},
  year = {1982},
  journal = {British Journal of Psychology},
  volume = {73},
  number = {4},
  pages = {441--454},
  abstract = {Examined the effects of relatively long-term (10-40 min) priming on the recognition of spoken words presented in noise to 32 20-50 yr olds to determine the extent to which the priming was lexical (i.e., whole word), morphological, or physical. No priming was found for physically related words. Thus, "deflecting" spoken clearly in pretraining had no effect on identification of "reflecting" in the test phase. There was strong facilitation from a word with a regular inflectional relationship to the test word. Thus, "reflected" facilitated recognition of "reflecting" almost as much as prior experience of "reflecting" itself, and when responses were scored in terms of the root morpheme, the morphemic effect was the same in the 2 cases. That this was a structural and not a semantic effect was shown by the lack of facilitation between irregularly related words such as "held/holding," "man/men," and "lost/loses." Data suggest that there is a morphological/structural level of analysis that is presemantic, at which these long-term facilitation effects take place. They also challenge the cohort theory of word recognition. (22 ref) (PsycINFO Database Record (c) 2006 APA, all rights reserved).}
}

@article{kerr_harking_1998,
  title = {{{HARKing}}: {{Hypothesizing After}} the {{Results}} Are {{Known}}},
  shorttitle = {{{HARKing}}},
  author = {Kerr, Norbert L.},
  year = {1998},
  month = aug,
  journal = {Personality and Social Psychology Review},
  volume = {2},
  number = {3},
  pages = {196--217},
  issn = {1088-8683},
  doi = {10.1207/s15327957pspr0203_4},
  urldate = {2020-02-25},
  abstract = {This article considers a practice in scientific communication termed HARKing (Hypothesizing After the Results are Known). HARKing is defined as presenting a post hoc hypothesis (i.e., one based on or informed by one's results) in one's research report as if it were, in fact, an a priori hypotheses. Several forms of HARKing are identified and survey data are presented that suggests that at least some forms of HARKing are widely practiced and widely seen as inappropriate. I identify several reasons why scientists might HARK. Then I discuss several reasons why scientists ought not to HARK. It is conceded that the question of whether HARKing's costs exceed its benefits is a complex one that ought to be addressed through research, open discussion, and debate. To help stimulate such discussion (and for those such as myself who suspect that HARKing's costs do exceed its benefits), I conclude the article with some suggestions for deterring HARKing.},
  langid = {english},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/ZSP4I9UK/Kerr - 1998 - HARKing Hypothesizing After the Results are Known.pdf}
}

@article{keuleers_vwr:_2013,
  title = {Time Course and Functional Neuroanatomy of Speech Segmentation in Adults},
  author = {Cunillera, Toni and C{\`a}mara, Estela and Toro, Juan M. and {Marco-Pallares}, Josep and {Sebasti{\'a}n-Galles}, Nuria and Ortiz, Hector and Pujol, Jes{\'u}s and {Rodr{\'i}guez-Fornells}, Antoni},
  year = {2009},
  month = nov,
  journal = {NeuroImage},
  volume = {48},
  number = {3},
  pages = {541--553},
  issn = {1053-8119},
  doi = {10.1016/j.neuroimage.2009.06.069},
  urldate = {2022-01-03},
  abstract = {The present investigation was devoted to unraveling the time-course and brain regions involved in speech segmentation, which is one of the first processes necessary for learning a new language in adults and infants. A specific brain electrical pattern resembling the N400 language component was identified as an indicator of speech segmentation of candidate words. This N400 trace was clearly elicited after a short exposure to the words of the new language and showed a decrease in amplitude with longer exposure. Two brain regions were observed to be active during this process: the posterior superior temporal gyrus and the superior part of the ventral premotor cortex. We interpret these findings as evidence for the existence of an auditory\textendash motor interface that is responsible for isolating possible candidate words when learning a new language in adults.},
  langid = {english}
}

