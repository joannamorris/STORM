  annotation = {Cambridge Univ Press, US Empirical Study. Journal Article. English}
}

@article{doignon_illusory_2005,
  title = {Children's Overregularization of {{English}} Plurals: A Quantitative Analysis*},
  shorttitle = {Children's Overregularization of {{English}} Plurals},
  author = {Marcus, Gary F.},
  year = {1995},
