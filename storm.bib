@article{winkler_brain_1999-1,
  title = {Brain Responses Reveal the Learning of Foreign Language Phonemes.},
  author = {Winkler, I and Kujala, T and Tiitinen, H and Sivonen, P and Alku, P and Lehtokoski, a and Czigler, I and Cs{\'e}pe, V and Ilmoniemi, R J and N{\"a}{\"a}t{\"a}nen, R},
