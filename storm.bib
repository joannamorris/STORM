@article{bertram_effects_2000,
  title = {Positional Specificity of Radicals in {{Chinese}} Character Recognition},
  author = {Taft, Marcus and Zhu, Xiaoping and Peng, Danling},
  year = {1999},
  journal = {Journal of Memory \& Language},
  volume = {40},
  number = {4},
  pages = {498--519},
  abstract = {A total of 5 experiments with 154 native Mandarin Chinese speakers were carried out to examine the effects of changing the components of Chinese characters on the subsequent recognition of the characters. The first 2 experiments took 2-radical Chinese characters and transposed their radicals to create another character. Character decision and naming responses to these transposed stimuli were then compared to control items that were not created via transposition, and no difference was found. Nor was a transposition effect found in a 3rd experiment examining noncharacters. These results were taken to mean that positional information is crucial in activating radical information during character recognition. A further experiment revealed a radical-transposition effect, but only when a 4-radical character had 2 of its radicals transposed. In Exp 4 the transposition of characters within 2-character words revealed considerable interference, which confirmed the expectation that positional information is not so important in character-level representations. Results support a hierarchical framework for considering the recognition of Chinese words whereby there is both a radical and a character level of representation, with the former being directly activated by featural information. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Cognition),(Learning),[2720].,*Chinese,*Chinese Cultural Groups,*Recognition,*Recognition (Learning),*Transposition,*Transposition (Cognition),*Written,*Written Language,\&,Adulthood,Cultural,Groups,Human.,Human. Adulthood (18 yrs \& older).,Language,Linguistics,Linguistics \& Language \& Speech [2720].,older).,Speech,yrs},
  annotation = {Elsevier Science, US Empirical Study. Journal Article. English}
}

@article{bertram_interplay_1999,
  title = {The Rapid Use of Gender Information: Evidence of the Time Course of Pronoun Resolution from Eyetracking},
  shorttitle = {The Rapid Use of Gender Information},
  author = {Arnold, Jennifer E and Eisenband, Janet G and {Brown-Schmidt}, Sarah and Trueswell, John C},
  year = {2000},
  month = jul,
  journal = {Cognition},
  volume = {76},
  number = {1},
  pages = {B13-B26},
  issn = {0010-0277},
  doi = {10.1016/S0010-0277(00)00073-1},
  urldate = {2020-08-29},
  abstract = {Eye movements of listeners were monitored to investigate how gender information and accessibility influence the initial processes of pronoun interpretation. Previous studies on this issue have produced mixed results, and several studies have concluded that gender cues are not automatically used during the early processes of pronoun interpretation (e.g. Garnham, A., Oakhill, J. \& Cruttenden, H. (1992). The role of implicit causality and gender cue in the interpretation of pronouns. Language and Cognitive Processes, 73 (4), 231\textendash 255; Greene, S. B., McKoon, G. \& Ratcliff, R. (1992). Pronoun resolution and discourse models. Journal of Experimental Psychology: Learning, Memory, and Cognition, 182, 266\textendash 283). In the two experiments presented here, participants viewed a picture with two familiar cartoon characters of either same or different gender. They listened to a text describing the picture, in which a pronoun referred to either the first, more accessible, character, or the second. (For example, Donald is bringing some mail to \{Mickey/Minnie\} while a violent storm is beginning. He's carrying an umbrella\ldots.) The results of both experiments show rapid use of both gender and accessibility at approximately 200 ms after the pronoun offset.},
  langid = {english},
  keywords = {Accessibility,Gender cues,Language comprehension,Pronoun resolution,Referent processing},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/JJESNJJG/S0010027700000731.html}
}

@article{bertram_length_2003,
  title = {The Influence of Age of Acquisition, Root, Frequency, and Context Availability in Processing Nouns and Verbs},
  author = {Colombo, Lucia and Burani, Cristina},
  year = {2002},
  journal = {Brain \& Language},
  volume = {81},
  number = {1-3},
  pages = {398--411},
  abstract = {In the Italian language there is a higher number of inflectional suffixes in verbs than in nouns, and this might imply that verbs are more likely to undergo a morphological analysis in terms of root and suffix as compared to nouns. Moreover, verbs tend to be more abstract than nouns, and this aspect might make verb processing more difficult. Finally, the developmental gap in the production of nouns and verbs suggests that age of acquisition might affect noun and verbs differently. Nouns and verbs were presented in a lexical decision and in a naming task. The morphological variable root frequency in addition to word frequency, length and word age of acquisition, and the semantic variables concreteness and context availability (P. J. Schwanenflugel, K. K. K. Harnishfeger, and R. W. Stowe, 1988) were used as predictors in multiple-regression analyses in which lexical decision and naming latencies were the dependent variables. The results showed that age of acquisition, context availability, and root frequency are all important in predicting both lexical decision and naming latencies for nouns and verbs, but age of acquisition and root frequency are better predictors of the differences in processing Italian nouns and verbs. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),(Phonetic,[2820].,*Age,*Age Differences,*Language,*Language Development,*Lexical,*Lexical Decision,*Morphology,*Morphology (Language),*Speech,*Speech Characteristics,\&,Adulthood,Characteristics,Cognitive,Cognitive \& Perceptual Development [2820].,Decision,Development,Differences,Human.,Human. Adulthood (18 yrs \& older).,Naming,Nouns,older).,Perceptual,Units),Verbs,Words,Words (Phonetic Units),yrs},
  annotation = {Academic Press, US Empirical Study. Journal Article. English}
}

@article{bertram_role_2000,
  ids = {longbottom_why_1999-1},
  title = {Why Teach Science? {{Setting}} Rational Goals for Science Education},
  shorttitle = {Why Teach Science?},
  author = {Longbottom, John E. and Butler, Philip H.},
  year = {1999},
  journal = {Science Education},
  volume = {83},
  number = {4},
  pages = {473--492},
  issn = {1098-237X},
  doi = {10.1002/(SICI)1098-237X(199907)83:4<473::AID-SCE5>3.0.CO;2-Z},
  urldate = {2020-02-24},
  abstract = {In this article we develop a fundamental rationale for teaching science to all children. We justify the teaching of science by linking scientific ways of thinking with the advancement of democratic society. Rather than simply treating science education as a civics course, we take the strong view that science education should produce a population with the skills to critically analyze and change society. Science students must learn science, but they must also learn about science, they must develop a scientific view of the world, and they must adopt some of the creative and critical attributes of scientists. To achieve these outcomes, science educators must be clear about what view of science they reflect and, in particular, they must reject both positivist and postmodernist elements. Science education must reflect the way in which the theories and practice of science are constrained by the real world. In setting out a robust answer to the question of why teach science we provide three aims for science education. These aims can assist science educators to decide what and how they teach, and help us all to defend science education from antiscientific criticisms. \textcopyright{} 1999 John Wiley \& Sons, Inc. Sci Ed 83:473\textendash 492, 1999.},
  copyright = {Copyright \textcopyright{} 1999 John Wiley \& Sons, Inc.},
  langid = {english},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/6WLN9VIG/Longbottom and Butler - 1999 - Why teach science Setting rational goals for scie.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/8NIH6U9I/(SICI)1098-237X(199907)834473AID-SCE53.0.html;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/ATUAW7M4/(SICI)1098-237X(199907)834473AID-SCE53.0.html}
}

@article{bertram_role_2000-1,
  title = {The Role of Context in Morphological Processing: {{Evidence}} from {{Finnish}}},
  author = {Bertram, Raymond and Hy{\"o}n{\"a}, Jukka and Laine, Matti},
  year = {2000},
  journal = {Language \& Cognitive Processes},
  volume = {15},
  number = {4-5},
  pages = {367--388},
  abstract = {This paper is concerned with the role of context on the processing of inflected nouns in Finnish. Identification of partitive plurals with the homonymic suffix -jA was studied by presenting the target nouns in a sentence context and by recording durations of readers' eye fixations and self-paced reading times for these targets. A recent visual lexical decision study indicated that the same inflected words with -jA were sensitive to surface frequency manipulations, but not to base frequency manipulations. The authors interpreted these results to suggest that these inflectional forms are stored and processed by means of their whole-word representations. In contrast, the present context study shows both a surface frequency effect and a lagged base frequency effect. The authors argue that syntactic cues prior to the target word prime the inflectional reading of the -jA suffix, and as a consequence the base is reinstated as an effective unit in processing these nouns with a homonymic suffix. Ss were 100 undergraduate students. (PsycINFO Database Record (c) 2002 APA, all rights reserved) (journal abstract).},
  keywords = {(18,(Language),[2720].,*Contextual,*Contextual Associations,*Inflection,*Language,*Morphology,*Morphology (Language),\&,Adulthood,Associations,Human.,Human. Adulthood (18 yrs \& older).,Language,Linguistics,Linguistics \& Language \& Speech [2720].,older).,Speech,yrs},
  annotation = {Taylor \& Francis, US Empirical Study. Journal Article. English}
}

@article{beyersmann_language_2015,
  title = {Language Proficiency and Morpho-Orthographic Segmentation},
  author = {Beyersmann, Elisabeth and Casalis, S{\'e}verine and Ziegler, Johannes C. and Grainger, Jonathan},
  year = {2015},
  month = aug,
  journal = {Psychonomic Bulletin \& Review},
  volume = {22},
  number = {4},
  pages = {1054--1061},
  issn = {1069-9384, 1531-5320},
  doi = {10.3758/s13423-014-0752-9},
  urldate = {2017-01-11},
  abstract = {One key finding in support of the hypothesis that written words are automatically parsed into component morphemes independently of the true morphological structure of the stimuli, so-called morpho-orthographic segmentation, is that suffixed nonword primes facilitate the visual recognition of a stem target (rapidifier-RAPIDE) whereas non-suffixed primes (rapiduit-RAPIDE) do not. However, Morris, Porter, Grainger, and Holcomb (Language \& Cognitive Processes, 26(4\textendash 6), 558\textendash 599, 2011)reported equivalent priming from suffixed and non-suffixed nonword primes, hence questioning the morphological nature of prior findings. Here we provide a further investigation of masked priming with morphologically complex nonword primes with an aim to isolate factors that modulate the size of these priming effects. We conducted a masked primed lexical decision experiment in French, in which the same target (TRISTE) was preceded by a suffixed word (tristesse), a suffixed nonword (tristerie), a non-suffixed nonword (tristald), or an unrelated prime word (direction). Participants were split into two groups, based on their language proficiency. The results show that in the high proficiency group, comparable magnitudes of priming were obtained in all three related prime conditions (including the non-suffixed condition) relative to unrelated primes, whereas in the low proficiency group, priming was significantly reduced in the non-suffixed condition compared to the two suffixed conditions. These findings provide further evidence that individual differences in language proficiency can modulate the impact of morphological factors during reading, and an explanation for the discrepant findings in prior research.},
  langid = {english},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/XNPV9KF7/s13423-014-0752-9.html}
}

@article{beyersmann_morpho-orthographic_2016,
  title = {Morpho-Orthographic Segmentation without Semantics},
  author = {Beyersmann, Elisabeth and Ziegler, Johannes C. and Castles, Anne and Coltheart, Max and Kezilas, Yvette and Grainger, Jonathan},
  year = {2016},
  month = apr,
  journal = {Psychonomic Bulletin \& Review},
  volume = {23},
  number = {2},
  pages = {533--539},
  issn = {1069-9384, 1531-5320},
  doi = {10.3758/s13423-015-0927-z},
  urldate = {2017-02-08},
  abstract = {Masked priming studies have repeatedly provided evidence for a form-based morpho-orthographic segmentation mechanism that blindly decomposes any word with the mere appearance of morphological complexity (e.g., corn + er). This account has been called into question by Baayen et al. Psychological Review, 118, 438\textendash 482 (2011), who pointed out that the prime words previously tested in the morpho-orthographic condition vary in the extent to which the suffix conveys regular meaning. In the present study, we investigated whether evidence for morpho-orthographic segmentation can be obtained with a set of tightly controlled prime words that are entirely semantically opaque. Using a visual lexical decision task, we compared priming from truly suffixed primes (hunter-HUNT), completely opaque pseudo-suffixed primes (corner-CORN), and non-suffixed primes (cashew-CASH). The results show comparable magnitudes of priming for the truly suffixed and pseudo-suffixed primes, and no priming from non-suffixed primes, and therefore provide further important evidence in support of morpho-orthographic segmentation processes operating in the absence of any possible role for semantics.},
  langid = {english},
  keywords = {Cognitive Psychology,Masked priming,Morpho-orthographic priming,morphological processing,Pseudo-affixes},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/BPTW67SG/Beyersmann et al. - 2016 - Morpho-orthographic segmentation without semantics.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/A265QJTD/s13423-015-0927-z.html;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/U95TTDHD/s13423-015-0927-z.html}
}

