@article{berent_phonological_2001-1,
  title = {Phonological Constraints on Reading: {{Evidence}} from the Obligatory Contour Principle},
  author = {Berent, Iris and Shimron, Joseph and Vaknin, Vared},
  year = {2001},
  journal = {Journal of Memory \& Language},
  volume = {44},
  number = {4},
  pages = {644--665},
  abstract = {Hebrew frequently manifests gemination in its roots, but strictly constrains its position: root-final germination is extremely frequency, whereas root-initial gemination is rare. This asymmetry is explained by a universal constraint on phonological representations, the Obligatory Contour Principle. Three experiments with 60 Ss examined whether this phonological constraint affects performance in a lexical decision task. The rejection of nonwords generated from novel roots with root-initial gemination was significantly faster than roots with final gemination controls. The emergence of this asymmetry regardless of the position of geminates in the word implicates a constraint on root, rather than simply word structure. Results further indicate that speakers are sensitive to the structure of geminate bigrams. Nonwords formed from roots with final gemination were significantly more difficult to reject than foils generated from frequency-matched no gemination controls. Speakers are thus sensitive to the identity of geminates and constrain their location in the root. These findings suggest that the representations assembled in reading a deep orthography are structured linguistic entities, constrained by phonological competence. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),(Phonetic,[2720].,*Foreign,*Foreign Languages,*Lexical,*Lexical Decision,*Morphology,*Morphology (Language),*Phonology,*Written,*Written Language,\&,Adulthood,Arts,Decision,Education,Human.,Human. Adulthood (18 yrs \& older).,Language,Language Arts Education,Languages,Linguistics,Linguistics \& Language \& Speech [2720].,older).,Reading,Speech,Units),Words,Words (Phonetic Units),yrs},
  annotation = {Elsevier Science, US Empirical Study. Journal Article. English}
}

@article{berent_representation_1997,
  title = {Tense and Aspect in Second Language Acquisition: {{Form}}, Meaning, and Use},
  author = {{Bardovi-Harlig}, Kathleen},
  year = {2000},
  journal = {Language Learning},
  volume = {50},
  number = {Suppl,1},
  pages = {491p},
  abstract = {This book is about the acquisition of temporal expression in second language. Temporal expression, or what C. Smith (1980) has called "time talk," has come into its own as an area of research in adult second language acquisition. The investigation of temporal expression includes all linguistic means of reference to time, although it most familiar in its morphological instantiation. The study of tense-aspect morphology has been the focus of many descriptive and pedagogical accounts of language. This book has 3 goals: to describe the acquisition of temporal expression, to examine how we study the acquisition of temporal expression in second language, and to consider research methods and analysis as they have been used in the study of second language temporality. The development of temporality is amenable to study through production data. This field of inquiry is dominated by learner-generated texts. There have been some experimental studies done that balance the production of learners among different types of verbs, and these have confirmed the patterns that have been observed in freer learner production. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(13-17,(18,(birth-12,(Language),[2720].,*Discourse,*Discourse Analysis,*Foreign,*Foreign Language Learning,*Morphology,*Morphology (Language),*Time,*Time Perspective,*Verbal,*Verbal Communication,\&,Adolescence,Adulthood,Analysis,Childhood,Communication,Human.,Human. Childhood (birth-12 yrs). Adolescence (13-1,Language,Learning,Linguistics,Linguistics \& Language \& Speech [2720].,older).,Perspective,Speech,yrs,yrs).},
  annotation = {Blackwell Publishers, US Journal Article. English}
}

@article{berent_scope_2002-1,
  title = {The Scope of Linguistic Generalizations: {{Evidence}} from {{Hebrew}} Word Formation},
  author = {Berent, Iris and Marcus, Gary F. and Shimron, Joseph and Gafos, Adamantios I.},
  year = {2002},
  journal = {Cognition},
  volume = {83},
  number = {2},
  pages = {113--139},
  abstract = {Does the productive use of language stem from the manipulation of mental variables? If linguistic constraints appeal to variables, rather than instances, then they should generalize to any representable novel instance, including instances that fall beyond the phonological space of a language. This prediction was tested by investigating a constraint on the structure of Hebrew roots. Hebrew frequently exhibits geminates in its roots, but it strictly constraints their location: geminates are frequent at the end of the root, but rare at its beginning. Symbolic accounts capture the ban on root-initial geminates as XXY, where X and Y are variables that stand for any two distinct consonants. If the constraint on root structure appeals to the identity of abstract variables, then speakers should be able to extend it to root geminates with foreign phonemes, including phonemes with foreign feature values. Findings are presented from three experiments supporting this prediction. These results suggest that a complete account of linguistic processing must incorporate mechanisms for generalization outside the representational space of trained items. Mentally-represented variables would allow speakers to make such generalizations. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),[2340].,[2720].,*Cognitive,*Cognitive Generalization,*Language,*Linguistics,*Morphology,*Morphology (Language),*Phonemes,\&,Adulthood,Cognitive,Cognitive Processes [2340]. Linguistics \& Language,Consonants,Generalization,Human.,Human. Adulthood (18 yrs \& older).,Language,Linguistics,older).,Processes,Speech,yrs},
  annotation = {Elsevier Science, Netherlands Empirical Study. Journal Article. English}
}

@article{berg_self-organization_2017,
  title = {Self-Organization in the Spelling of {{English}} Suffixes: {{The}} Emergence of Culture out of Anarchy},
  shorttitle = {Self-Organization in the Spelling of {{English}} Suffixes},
  author = {Berg, Kristian and Aronoff, Mark},
  year = {2017},
  month = mar,
  journal = {Language},
  volume = {93},
  number = {1},
  pages = {37--64},
  issn = {1535-0665},
  doi = {10.1353/lan.2017.0000},
  urldate = {2018-05-09},
  abstract = {{$<$}p{$>$} English spelling is unphonetic: the same sounds can be spelled in various and often idiosyncratic ways. It is also ungoverned: there is no authority guiding its development. However, it is not as arbitrary as one might conclude. Investigating the spelling of four derivational suffixes, we show that the spelling of English suffixes is quite consistent. Homophonous endings of morphologically simple words are spelled differently, keeping the suffix spelling distinct (cf. e.g. \&lt;nervous vs. \&lt;service/*\&lt;servous). English spelling thus provides morphological cues for the reader. Diachronically, we show that this system emerged without explicit regulation, but as a result of self-organization. We use the Helsinki corpus to show how variation was gradually reduced for each of the suffixes. The regular spellings of today emerged gradually, through a sorting-out process of competition between alternate spellings.* {$<$}/p{$>$}},
  langid = {english}
}

@article{bergen_psychological_2004,
