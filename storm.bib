@article{landau_importance_1988,
  title = {The Importance of Shape in Early Lexical Learning.},
  author = {Landau, Barbara and Smith, Linda B. and Jones, Susan S.},
  year = {1988},
  journal = {Cognitive Development},
  volume = {3},
  number = {3},
  pages = {299--321},
  issn = {1879-226X(Electronic),0885-2014(Print)},
  doi = {10.1016/0885-2014(88)90014-7},
  abstract = {Examined whether dimensions of perceptual similarity (shape, size, and texture) were weighted more heavily than others in determining word extension in 4 experiments with 88 2-yr-olds, 88 3-yr-olds, and 68 undergraduates. Results indicate that 2- and 3-yr-olds and adults all weighted shape more heavily than size or texture. The shape bias increased in strength and generality from 2 to 3 yrs of age and more markedly from early childhood to adulthood. In young Ss, the shape bias was much stronger in word extension than in nonword classification tasks. Results suggest that the development of the shape bias originated in language learning and did not stem from general perceptual processes. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
