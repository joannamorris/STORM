  author = {Taft, Marcus},
  year = {1990},
  journal = {Journal of Memory \& Language},
  volume = {29},
  number = {2},
  pages = {245--257},
  abstract = {5 experiments with 100 Ss examined lexical decision (LXD) and naming (NM) times to single words that, when used in a sentence, are functionally constrained. Closed class words (CCWs) that cannot meaningfully stand alone (e.g., than) took longer to respond to than open class words (OCWs) in an LXD task but not an NM task. OCWs that cannot stand alone (e.g., rely) were associated with longer LXD times than those that can (e.g., rail), but not with longer NM times. Thus, the effect of functional constraint was not restricted to CCWs and seemed to occur at a postaccess decision stage of processing rather than influencing lexical access itself. Not all functional constraints delayed LXD times, since obligatorily transitive verbs (e.g., borrow) did not differ from unconstrained words. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Phonetic,[2340].,*Lexical,*Lexical Decision,*Naming,*Stimulus,*Stimulus Parameters,*Syntax,*Words,*Words (Phonetic Units),\&,Adulthood,Cognitive,Cognitive Processes [2340].,Decision,Human.,Human. Adulthood (18 yrs \& older).,older).,Parameters,Processes,Reaction,reaction time,Time,Units),yrs},
  annotation = {Elsevier Science, US Empirical Study. Journal Article. English}
}

