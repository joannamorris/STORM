@article{stanners_memory_1979-1,
  title = {{{COVID}} 19-{{Induced Smell}} and {{Taste Impairments}}: {{Putative Impact}} on {{Physiology}}},
  shorttitle = {{{COVID}} 19-{{Induced Smell}} and {{Taste Impairments}}},
  author = {Meunier, Nicolas and Briand, Lo{\"i}c and {Jacquin-Piques}, Agn{\`e}s and Brondel, Laurent and P{\'e}nicaud, Luc},
