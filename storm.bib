}

@article{gupta_road_2012,
  title = {The {{Road Not Taken Creative Solutions Require Avoidance}} of {{High-Frequency Responses}}},
  author = {Gupta, Nitin and Jang, Yoonhee and Mednick, Sara C. and Huber, David E.},
  year = {2012},
  month = feb,
  journal = {Psychological Science},
  pages = {0956797611429710},
  issn = {0956-7976, 1467-9280},
  doi = {10.1177/0956797611429710},
  urldate = {2015-08-30},
  abstract = {To investigate individual differences in creativity as measured with a complex problem-solving task, we developed a computational model of the remote associates test (RAT). For 50 years, the RAT has been used to measure creativity. Each RAT question presents three cue words that are linked by a fourth word, which is the correct answer. We hypothesized that individuals perform poorly on the RAT when they are biased to consider high-frequency candidate answers. To assess this hypothesis, we tested individuals with 48 RAT questions and required speeded responding to encourage guessing. Results supported our hypothesis. We generated a norm-based model of the RAT using a high-dimensional semantic space, and this model accurately identified correct answers. A frequency-biased model that included different levels of bias for high-frequency candidate answers explained variance for both correct and incorrect responses. Providing new insight into the nature of creativity, the model explains why some RAT questions are more difficult than others, and why some people perform better than others on the RAT.},
  langid = {english},
  pmid = {22344387},
  keywords = {associative processes,creativity,Individual differences,Language,remote associates test,Semantic Memory,word association,word frequency},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/JG8B8NMA/0956797611429710.html}
}

@article{hackley_cross-modal_1990,
  title = {Compound Words and Structure in the Lexicon},
