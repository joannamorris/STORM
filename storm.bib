  publisher = {{American Psychological Association}},
  issn = {0278-7393},
  doi = {10.1037/a0040332},
  urldate = {2021-03-13},
  abstract = {Recent studies have documented frequency effects for word n-grams, independently of word unigram frequency. Further studies have revealed constructional prototype effects, both at the word level as well as for phrases. The present speech production study investigates the time course of these effects for the production of prepositional phrases in English, using event related potentials (ERPs). For word frequency, oscillations in the theta range emerged. By contrast, phrase frequency showed a persistent effect over time. Furthermore, independent effects with different temporal and topographical signatures characterized phrasal prototypicality. In a simulation study we demonstrate that naive discrimination learning provides an alternative account of the data that is as least as powerful as a standard lexical predictor analysis. The implications of the current findings for models of language processing are discussed. (PsycInfo Database Record (c) 2020 APA, all rights reserved)},
  keywords = {discrimination learning,Discrimination Learning,ERP,Evoked Potentials,Naming,Oral Communication,phrase frequency,Phrases,Pictorial Stimuli,picture naming,relative entropy,Word Frequency},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/2ZSFU2V8/Hendrix et al. - 2017 - Distinct ERP signatures of word frequency, phrase .pdf}
}

@article{henson_what_2005,
  title = {What Can Functional Neuroimaging Tell the Experimental Psychologist?},
  author = {Henson, Richard},
  year = {2005},
  month = feb,
  journal = {The Quarterly Journal of Experimental Psychology Section A},
  volume = {58},
  number = {2},
  pages = {193--233},
  issn = {0272-4987},
  doi = {10.1080/02724980443000502},
  urldate = {2015-08-29},
  abstract = {I argue here that functional neuroimaging data\textemdash which I restrict to the haemodynamic techniques of fMRI and PET\textemdash can inform psychological theorizing, provided one assumes a ``systematic'' function\textendash structure mapping in the brain. In this case, imaging data simply comprise another dependent variable, along with behavioural data, that can be used to test competing theories. In particular, I distinguish two types of inference: function-to-structure deduction and structure-to-function induction. With the former inference, a qualitatively different pattern of activity over the brain under two experimental conditions implies at least one different function associated with changes in the independent variable. With the second type of inference, activity of the same brain region(s) under two conditions implies a common function, possibly not predicted a priori. I illustrate these inferences with imaging studies of recognition memory, short-term memory, and repetition priming. I then consider in greater detail what is meant by a ``systematic'' function\textendash structure mapping and argue that, particularly for structure-to-function induction, this entails a one-to-one mapping between functional and structural units, although the structural unit may be a network of interacting regions and care must be taken over the appropriate level of functional/structural abstraction. Nonetheless, the assumption of a systematic function\textendash structure mapping is a ``working hypothesis'' that, in common with other scientific fields, cannot be proved on independent grounds and is probably best evaluated by the success of the enterprise as a whole. I also consider statistical issues such as the definition of a qualitative difference and methodological issues such as the relationship between imaging and behavioural data. I finish by reviewing various objections to neuroimaging, including neophrenology, functionalism, and equipotentiality, and by observing some criticisms of current practice in the imaging literature.},
  pmid = {15903115},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/GM49WS8Z/Henson - 2005 - What can functional neuroimaging tell the experime.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/H2VM3H8U/02724980443000502.html;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/WMFHGNZR/02724980443000502.html}
}

@article{herculanohouzel_glia/neuron_2014,
  title = {The Glia/Neuron Ratio: {{How}} It Varies Uniformly across Brain Structures and Species and What That Means for Brain Physiology and Evolution},
  shorttitle = {The Glia/Neuron Ratio},
  author = {Herculano-Houzel, Suzana},
  year = {2014},
  month = sep,
  journal = {Glia},
  volume = {62},
  number = {9},
  pages = {1377--1391},
  issn = {1098-1136},
  doi = {10.1002/glia.22683},
  urldate = {2018-09-09},
  abstract = {It is a widespread notion that the proportion of glial to neuronal cells in the brain increases with brain size, to the point that glial cells represent ``about 90\% of all cells in the human brain.'' This notion, however, is wrong on both counts: neither does the glia/neuron ratio increase uniformly with brain size, nor do glial cells represent the majority of cells in the human brain. This review examines the origin of interest in the glia/neuron ratio; the original evidence that led to the notion that it increases with brain size; the extent to which this concept can be applied to white matter and whole brains and the recent supporting evidence that the glia/neuron ratio does not increase with brain size, but rather, and in surprisingly uniform fashion, with decreasing neuronal density due to increasing average neuronal cell size, across brain structures and species. Variations in the glia/neuron ratio are proposed to be related not to the supposed larger metabolic cost of larger neurons (given that this cost is not found to vary with neuronal density), but simply to the large variation in neuronal sizes across brain structures and species in the face of less overall variation in glial cell sizes, with interesting implications for brain physiology. The emerging evidence that the glia/neuron ratio varies uniformly across the different brain structures of mammalian species that diverged as early as 90 million years ago in evolution highlights how fundamental for brain function must be the interaction between glial cells and neurons. GLIA 2014;62:1377\textendash 1391},
  copyright = {\textcopyright{} 2014 Wiley Periodicals, Inc.},
  langid = {english},
  keywords = {brain metabolism,brain size,cell size},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/J696HBWP/glia.html}
}

@article{hermans_lexical_2011,
