@article{luck_introduction_2014,
  title = {A {{Statistical Learning Perspective}} on {{Children}}'s {{Learning About Graphotactic}} and {{Morphological Regularities}} in {{Spelling}}},
  author = {Deacon, S. H{\'e}l{\`e}ne and Conrad, Nicole and Pacton, S{\'e}bastien},
  year = {2008},
  month = may,
  journal = {Canadian Psychology; Ottawa},
  volume = {49},
  number = {2},
  pages = {118--124},
  issn = {07085591},
  urldate = {2019-05-12},
  abstract = {The first point of evidence opposing rule-based learning is the disconnect between the idea of children's initial and exclusive reliance on a phonetic rule in spelling and the growing amount of evidence showing that children young enough to be in this stage demonstrate sensitivity to morphological regularities in their spelling. This is the case for the 5- and 6-year-olds in Treiman and Cassar's (1996) study, and the 6-year-olds in Deacon and [Bryant]'s (2006) study. (Such a concern also exists in relation to the early findings of sensitivity to graphotactic regularities; e.g., [Pacton] et al., 2001). Certainly, some of these discrepancies can be accounted for by the context of assessment. For example, there is some evidence that increasing task demands (e.g., introducing a secondary task or varying the quantity spelt) increases agreement errors for French spellers (e.g., Hupet, Fayol, \& Schelstraete, 1998). For English, a similar pattern emerges based on the metric of analysis. The spelling tund (instead of tuned) is categorized as a phonetic spelling not influenced by morphology by [Nunes] et al. (1997b) who focused on the correct use of -ed, but [Treiman] and Cassar ( 1996) considered such a spelling as influenced by morphology if /n/ was transcribed accurately more often in two- than in one-morpheme words (e.g., tuned vs. brand). More important, not all findings can be discounted on this basis. Deacon and Bryant (2005; 2006) recently demonstrated that young children demonstrate an appreciation of the relationship between morphemes and spelling even though the coding required correct spellings of whole morphemes and not just single letters (see also Deacon, 2008). Young children's appreciation of the role of morphemes in spelling goes directly against the predictions made for Nunes et al.'s stage of initially phonetic spelling. Nous pr\'esentons notre point de vue sur la mani\`ere dont les enfants apprennent l'orthographe en nous appuyant sur une \'evaluation de la recherche r\'ecente en mati\`ere de d\'eveloppement des comp\'etences en lecture et en \'ecriture des enfants francophones et anglophones. Nous nous penchons sur la question de l'apprentissage des codes graphotactiques (combinaisons de lettres admises), apr\`es quoi nous examinons plus \`a fond la mani\`ere dont les enfants apprennent la position des morph\`emes (les plus petites unit\'es de sens de la langue). Nous mettons en lumi\`ere les diff\'erences entre les r\'esultats de la recherche r\'ecente et les mod\`eles traditionnels voulant que les enfants se servent de l'information graphotactique et morphologique relativement tard dans leur apprentissage de l'orthographe, et selon lesquels le d\'eveloppement des comp\'etences, en particulier la reconnaissance des codes morphologiques, s'appuie essentiellement sur l'apprentissage des r\`egles d'orthographe. En fait, il semble que les enfants en tr\`es bas \^age soient influenc\'es, dans leur apprentissage de l'orthographe, par les structures graphotactiques comme par les structures morphologiques et qu'en d\'efinitive, ils ne d\'ependent pas exclusivement des r\`egles pour \'ecrire. C'est pourquoi nous formulons la possibilit\'e que les enfants pourraient apprendre \`a reconna\^itre les codes graphotactiques et morphologiques de l'orthographe par apprentissage statistique.},
  copyright = {Copyright Canadian Psychological Association May 2008},
  langid = {english},
  keywords = {Children \& youth,Learning,Psychology,Spelling,Vocabulary development},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/CB5W3KSZ/Deacon et al. - 2008 - A Statistical Learning Perspective on Children's L.pdf}
}

@article{lukatela_grammatical_1983,
  title = {Phases of Inquiry-Based Learning: {{Definitions}} and the Inquiry Cycle},
  shorttitle = {Phases of Inquiry-Based Learning},
  author = {Pedaste, Margus and M{\"a}eots, Mario and Siiman, Leo A. and {de Jong}, Ton and {van Riesen}, Siswa A. N. and Kamp, Ellen T. and Manoli, Constantinos C. and Zacharia, Zacharias C. and Tsourlidaki, Eleftheria},
  year = {2015},
  month = feb,
  journal = {Educational Research Review},
  volume = {14},
  pages = {47--61},
  issn = {1747-938X},
  doi = {10.1016/j.edurev.2015.02.003},
  urldate = {2020-06-17},
  abstract = {Inquiry-based learning is gaining popularity in science curricula, international research and development projects as well as teaching. One of the underlying reasons is that its success can be significantly improved due to the recent technical developments that allow the inquiry process to be supported by electronic learning environments. Inquiry-based learning is often organized into inquiry phases that together form an inquiry cycle. However, different variations on what is called the inquiry cycle can be found throughout the literature. The current article focuses on identifying and summarizing the core features of inquiry-based learning by means of a systematic literature review and develops a synthesized inquiry cycle that combines the strengths of existing inquiry-based learning frameworks. The review was conducted using the EBSCO host Library; a total of 32 articles describing inquiry phases or whole inquiry cycles were selected based on specific search criteria. An analysis of the articles resulted in the identification of five distinct general inquiry phases: Orientation, Conceptualization, Investigation, Conclusion, and Discussion. Some of these phases are divided into sub-phases. In particular, the Conceptualization phase is divided into two (alternative) sub-phases, Questioning and Hypothesis Generation; the Investigation phase is divided into three sub-phases, Exploration or Experimentation leading to Data Interpretation; and the Discussion phase is divided into two sub-phases, Reflection and Communication. No framework bringing together all of these phases and sub-phases was found in the literature. Thus, a synthesized framework was developed to describe an inquiry cycle in which all of these phases and sub-phases would be present. In this framework, inquiry-based learning begins with Orientation and flows through Conceptualization to Investigation, where several cycles are possible. Inquiry-based learning usually ends with the Conclusion phase. The Discussion phase (which includes Communication and Reflection) is potentially present at every point during inquiry-based learning and connects to all the other phases, because it can occur at any time during (discussion in-action) or after inquiry-based learning when looking back (discussion on-action).},
  langid = {english},
  keywords = {Inquiry cycle,Inquiry phases,Inquiry-based learning,Inquiry-based learning framework},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/BDX7SQ8B/Pedaste et al. - 2015 - Phases of inquiry-based learning Definitions and .pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/8I32HPK4/S1747938X15000068.html}
}

@article{lukatela_identity_1999,
  title = {Can Orthographic Rimes Facilitate Naming?},
  author = {Montant, Marie and Ziegler, Johannes C.},
  year = {2001},
  journal = {Psychonomic Bulletin \& Review},
  volume = {8},
  number = {2},
  pages = {351--356},
  abstract = {(from the journal abstract) Although the orthographic rime (body) is thought to play an important role in reading English, previous priming experiments found little or no evidence for facilitatory body-priming effects in the naming task. That is, hose primes NOSE no better than does a completely unrelated prime. In the present study, the hypothesis that facilitatory body-priming effects are typically masked by strong inhibitory onset effects was investigated. It was shown that when the onset of a prime was removed, facilitatory body priming could be obtained with stimuli that previously had produced no evidence of facilitation. The present study thus reconciles conflicting patterns concerning facilitation versus inhibition in body priming. (PsycINFO Database Record (c) 2004 APA, all rights reserved).},
  keywords = {(18,[2340].,*Naming,*Orthography,*Phonology,*Priming,*Reading,\&,Adulthood,Cognitive,Cognitive Processes [2340].,Human.,Human. Adulthood (18 yrs \& older).,older).,Processes,yrs},
  annotation = {References . Psychonomic Society, US English Empirical Study Peer Reviewed Journal}
}

@article{lukatela_identity_1999-1,
  title = {Identity Priming in {{English}} Is Compromised by Phonological Ambiguity},
  author = {Lukatela, Georgije and Frost, Stephen J and Turvey, M},
  year = {1999},
  journal = {Journal of Experimental Psychology: Human Perception \& Performance},
  volume = {25},
  number = {3},
  pages = {775--790},
  issn = {0096-1523},
  abstract = {If it takes longer to achieve a single phonological representation for inconsistent words (e.g., BOWL) than for consistent words (e.g., BENT), and if phonological coherence is pivotal to visual word recognition, then identity priming should depend on consistency. This hypothesis was evaluated in naming and lexical decision within a 4-field presentation sequence of mask-prime-mask-target. The prime-target stimulus onset asynchrony (SOA) was either 114 or 244 ms (with prime durations, respectively, of 43 and 129 ms). Four experiments compared identity primes such as BOWL and BENT, which were equated, on average, for total number of friendly and unfriendly neighbors, bigram frequency, and number of 1-letter-different neighbors. In both tasks, BENT primed itself better than BOWL primed itself, with the difference being larger at the shorter SOA. Word processing is constrained primarily by the rate of achieving a coherent phonological code. (PsycINFO Database Record (c) 2005 APA, all rights reserved) (journal abstract).},
  keywords = {(18,(18-29,[2326].,*Phonology,*Priming,*Speech,*Speech Perception,*Word,*Word Recognition,\&,Adulthood,Auditory,Auditory \& Speech Perception [2326].,Human.,Human. Adulthood (18 yrs \& older). Young Adulthood,older).,Perception,Recognition,Speech,us.,Young,yrs,yrs)}
}

@article{luzzatti_lexical_2001-1,
  title = {Lexical Representation and Processing of Morphologically Complex Words: {{Evidence}} from the Reading Performance of an {{Italian}} Agrammatic Patient},
  author = {Luzzatti, Claudio and Mondini, Sara and Semenza, Carlo},
  year = {2001},
  journal = {Brain \& Language},
  volume = {79},
  number = {3},
  pages = {345--359},
  abstract = {The study of patients with acquired language disorders has provided crucial evidence for contemporary theories on mental lexical representation. This is particularly true for the representation of morphologically complex words. In this paper the authors analyzed the performance of a patient (M.B.) affected by agrammatism and dyslexia. M.B. was required to read aloud simple and morphologically complex words. The patient's pattern of errors was interpreted as the result of a predominant use of the lexical routine (phonological dyslexia). Three reading tasks were developed which allowed testing of M.B.'s ability to read morphologically complex words (reading of regular and irregular plurals; reading of high- and low-frequency singular and plural nouns; reading of evaluative suffixes). Errors were determined by frequency effect rather than by type of suffix (i.e., inflectional or derivational). High-frequency morphologically complex items seemed to meet stored representations, thus avoiding the parsing procedures that are required for less frequent items. These results are in keeping with dual route models of lexical representation of morphologically complex words. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,(Language),(Phonetic,[3297].,*Aphasia,*Lexical,*Lexical Access,*Morphology,*Morphology (Language),*Reading,*Reading Ability,*Words,*Words (Phonetic Units),\&,Ability,Access,Adulthood,Brain,Damage,Disorders,Dyslexia,Frequency,Human.,Human. Male. Adulthood (18 yrs \& older). Young Adu,Male.,Neurological,Neurological Disorders \& Brain Damage [3297].,older).,Units),Word,Word Frequency,Young,yrs,yrs).},
  annotation = {Academic Press, US Conference Proceedings/Symposia. Empirical Study. Clinical Case Report. A preliminary version of this paper was presented at the aforementioned meeting and at the 16th European Workshop on Cognitive Neuropsychology (Bressanone, Jan, 1998) Journal Article. English},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/XNJDSS79/Luzzatti, Mondini, Semenza. 2001.pdf}
}

@article{luzzatti_morphological_1996,
  title = {Less Well-Behaved Pronouns: {{Singular}} They in {{English}} and Plural Ta `It/He/She' in {{Chinese}}},
  shorttitle = {Less Well-Behaved Pronouns},
  author = {Chen, Jing and Wu, Yicheng},
