@article{alonzo_sonority_2002-1,
  title = {Sonority Constraints on Onset-Rime Cohesion: {{Evidence}} from Native and Bilingual {{Filipino}} Readers of {{English}}},
  author = {Alonzo, Angelo and Taft, Marcus},
  year = {2002},
  journal = {Brain \& Language},
  volume = {81},
  number = {1-3},
  pages = {368--383},
  abstract = {Research in English suggests that syllables can be analyzed in terms of two subunits\textendash the onset and the rime. This study investigated whether 30 nonnative readers of English (compared to 30 monolingual English speakers), which in the case of the present study were native Filipino speakers, also make use of onset-rime units, particularly when some features of their native language appear to foster no awareness of such units. In two lexical decision experiments (n = 91), monosyllabic English words were presented, divided in between their first and second consonants (e.g., B LIND), at their onset-rime boundary (e.g., BL IND), or at their antibody boundary (e.g., BLI ND). Results indicated that the processes of infixation and reduplication did not affect the English word processing of native Filipino speakers. Rather, results for both native Filipino and native English speakers suggest that onsets composed of an "s /+ consonant' sequence (e.g., STAMP) are less cohesive than onsets comprised of a stop-liquid sequence (e.g., BLIND). It was concluded that not only may sonority constraints underlie onset cohesiveness, but that such phonetic properties may also be involved in visual word recognition. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Phonetic,[2340].,*Lexical,*Lexical Decision,*Speech,*Speech Characteristics,*Syllables,*Word,*Word Recognition,\&,Adulthood,Characteristics,Cognitive,Cognitive Processes [2340].,Consonants,Decision,Human.,Human. Adulthood (18 yrs \& older).,older).,Perception,Phonology,Processes,Recognition,Units),Visual,Visual Perception,Vowels,Words,Words (Phonetic Units),yrs},
  annotation = {Academic Press, US Empirical Study. Journal Article. English}
}

