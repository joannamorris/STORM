}

@article{berger_developmental_1996,
  title = {Developmental Changes in Real-Time Sentence Processing},
  author = {Berger, Erich Von and Wulfeck, Beverly and Bates, Elizabeth and Fink, Nancy},
  year = {1996},
  journal = {First Language},
  volume = {16},
  number = {47, Pt 2},
  pages = {193--222},
  abstract = {Examined syntactic, semantic and morphologic cues that 55 English-speaking children (aged 7-12 yrs) and 24 young adults used to determine agent roles during sentence interpretation. Processing times associated with these cues were also examined. Both agent choice and reaction times indicated a developmental progression in agent role assignment, with earliest reliance on canonical word order (SVO) followed by later emerging non-canonical strategies (VOS and OSV). Converging information from subject-verb agreement and animacy cues increased use of these non-canonical word orders; however, weaker agreement and animacy cues also appeared to slow down reaction times. Non-canonical VOS, followed by OSV strategies, stabilized between 7 and 12 yrs of age, when adult-like patterns of competition and convergence emerged in the interaction between strong and weak cues. Results of this study are considered within the framework of the Competition Model (E. Bates and B. MacWhinney 1989). (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,(6-12,(birth-12,(Language),[2820].,*Cues,*Language,*Language Development,*Morphology,*Morphology (Language),*Sentence,*Sentence Comprehension,*Syntax,\&,Adulthood,Age,Childhood,Cognitive,Cognitive \& Perceptual Development [2820].,Comprehension,Development,Female.,Human.,Human. Male. Female. Childhood (birth-12 yrs). Sch,Male.,older).,Perceptual,School,Semantics,Strategies,Young,yrs,yrs).},
  annotation = {Alpha Academic, England Empirical Study. Journal Article. English}
}

