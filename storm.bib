@article{grossi_phonological_2001,
  title = {Unconscious {{Masked Priming Depends}} on {{Temporal Attention}}},
  author = {Naccache, L. and Blandin, E. and Dehaene, S.},
  year = {2002},
  month = sep,
  journal = {Psychological Science (Wiley-Blackwell)},
  volume = {13},
  number = {5},
  pages = {416--424},
  issn = {09567976},
  abstract = {The cognitive processes at work in masked priming experiments are usually considered automatic and independent of attention. We provide evidence against this view. Three behavioral experiments demonstrate that the occurrence of unconscious priming in a number\textendash comparison task is determined by the allocation of temporal attention to the time window during which the prime\textendash target pair is presented. Both response\textendash congruity priming and physical repetition priming vanish when temporal attention is focused away from this time window. These findings are inconsistent with the concept of a purely automatic spreading of activation during masked priming.},
  lccn = {7225331},
  keywords = {ATTENTION,Cognition,PRIMING (Psychology)}
}

@article{grossi_phonological_2001-1,
  title = {Phonological Processing in Visual Rhyming: {{A}} Developmental {{ERP}} Study},
