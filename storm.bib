@article{bortolini_clinical_2006,
  title = {Clinical Markers for Specific Language Impairment in {{Italian}}: The Contribution of Clitics and Non-word Repetition},
  shorttitle = {Clinical Markers for Specific Language Impairment in {{Italian}}},
  author = {Bortolini, Umberta and Arf{\'e}, Barbara and Caselli, Cristina M. and Degasperi, Luisa and Deevy, Patricia and Leonard, Laurence B.},
  year = {2006},
  month = jan,
  journal = {International Journal of Language \& Communication Disorders},
  volume = {41},
  number = {6},
  pages = {695--712},
  issn = {1368-2822},
  doi = {10.1080/13682820600570831},
  urldate = {2019-08-13},
  abstract = {Background: The discovery of clinical markers for specific language impairment (SLI) in children can assist in the accurate identification of children with this disorder, and in a description of the disorder's phenotype for genetic study. One challenge to this type of research is the fact that languages vary in the most salient symptoms of SLI. This study focuses on Italian.Aims: To determine whether three measures \textemdash{} the use of third-person plural inflections, the use of direct-object clitics and non-word repetition \textemdash{} are successful in distinguishing Italian-speaking children with SLI from their typically developing peers.Methods \& Procedures: Eleven preschool-aged children with SLI, 11 same-age typically developing peers and 11 younger typically developing children participated in the study. The third-person plural inflection and direct-object clitic tasks required the children to describe drawings in response to prompts provided by the examiner. In the non-word repetition task, the children repeated non-words ranging from one to four syllables in length.Outcomes \& Results: All three measures proved successful either singly or in combination, with direct-object clitics and non-word repetition showing the highest sensitivity and specificity.Conclusions: Additional research should be pursued to replicate and extend these findings. Along with the potential clinical value of the findings, the results suggest that difficulties with non-final weak syllables \textemdash{} a problem that would adversely affect all three measures \textemdash{} may be an important part of the SLI profile in Italian.},
  pmid = {17079223},
  keywords = {Italian,Language assessment,Specific language impairment},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/EUA4XZSB/13682820600570831.html}
}

