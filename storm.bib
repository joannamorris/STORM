}

@article{quemart_role_2011,
  title = {Word Order and {{Broca}}'s Region: {{Evidence}} for a Supra-Syntactic Perspective},
  shorttitle = {Word Order and {{Broca}}'s Region},
  author = {{Bornkessel-Schlesewsky}, Ina and Schlesewsky, Matthias and von Cramon, D. Yves},
  year = {2009},
  month = dec,
  journal = {Brain and Language},
  volume = {111},
  number = {3},
  pages = {125--139},
  issn = {0093-934X},
  doi = {10.1016/j.bandl.2009.09.004},
  urldate = {2020-08-28},
  abstract = {It has often been suggested that the role of Broca's region in sentence comprehension can be explained with reference to general cognitive mechanisms (e.g. working memory, cognitive control). However, the (language-related) basis for such proposals is often restricted to findings on English. Here, we argue that an extension of the database to other languages can shed new light on the types of mechanisms that an adequate account of Broca's region should be equipped to deal with. This becomes most readily apparent in the domain of word order variations, which we examined in German verb-final sentences using event-related fMRI. Our results showed that activation in the pars opercularis \textendash{} a core subregion of Broca's area \textendash{} was not only modulated by the relative ordering of subject and object, but also by a further factor known to affect word order in a number of languages, namely referentiality. Notably, the finding provides the first demonstration of a word order-related activation difference within subject-initial sentences in this region. Additional parametric analyses using individual behavioral data as predictors further attest to the independence of the pars opercularis activation from: (a) sentence acceptability, and (b) difficulty in performing the experimental (judgment) task. We argue that these and related findings attest to the need for a processing mechanism that can manipulate predicate-independent, interacting and hierarchically structured relational representations during real time comprehension. These properties pose a challenge to existing accounts of pars opercularis function.},
  langid = {english},
  keywords = {Broca's area,Definiteness,Inferior frontal gyrus,Linearization,Pars opercularis,Prominence,Referentiality,Word order},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/RUF53LMF/S0093934X09001333.html}
}

@article{quemart_role_2011-1,
  title = {The Role of Form and Meaning in the Processing of Written Morphology: {{A}} Priming Study in {{French}} Developing Readers},
  shorttitle = {The Role of Form and Meaning in the Processing of Written Morphology},
  author = {Qu{\'e}mart, Pauline and Casalis, S{\'e}verine and Col{\'e}, Pascale},
  year = {2011},
  month = aug,
  journal = {Journal of Experimental Child Psychology},
  volume = {109},
  number = {4},
  pages = {478--496},
  issn = {0022-0965},
  doi = {10.1016/j.jecp.2011.02.008},
  urldate = {2015-03-12},
  abstract = {Three visual priming experiments using three different prime durations (60 ms in Experiment 1, 250 ms in Experiment 2, and 800 ms in Experiment 3) were conducted to examine which properties of morphemes (form and/or meaning) drive developing readers' processing of written morphology. French third, fifth, and seventh graders and adults (the latter as a control group) performed lexical decision tasks in which targets were preceded by morphological (e.g., tablette\textendash TABLE, ``little table\textendash table''), pseudoderived (e.g., baguette\textendash BAGUE, ``little stick\textendash ring''), orthographic control (e.g., abricot\textendash ABRI, ``apricot\textendash shelter''), and semantic control (e.g., Tulipe\textendash FLEUR, ``tulip\textendash flower'') primes. Across all groups, different patterns of priming were observed in both morphological and orthographic/semantic control conditions, suggesting that they all process morphemes as units when reading. In developing readers, the processing of written morphology is triggered by the form properties of morphemes, and their semantic properties are activated later in the time course of word recognition. In adults, patterns of priming were similar except that the activation of the form properties of morphemes decreased earlier in the time course of word recognition. Taken together, these findings indicate that French developing readers process both the form and meaning properties of morphemes when reading and support a progressive quantitative change in the development of morphological processing over the course of reading development.},
  keywords = {Lexical Decision,Morphology,Pseudoderivation,Reading development,Semantics,Visual priming},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/6SFFTFFT/Quémart et al. - 2011 - The role of form and meaning in the processing of .html}
}

@article{r_core_team_r:_2021,
  title = {The {{N400}} Potential Could Index a Semantic Inhibition},
  author = {Debruille, J. Bruno},
  year = {2007},
  month = dec,
  journal = {Brain Research Reviews},
  volume = {56},
  number = {2},
  pages = {472--477},
  issn = {0165-0173},
  doi = {10.1016/j.brainresrev.2007.10.001},
  urldate = {2017-01-10},
  abstract = {In this theoretical study, I briefly review some of the first hypotheses as to the functional significance of the N400 potential. I then summarize the view that the amplitude of the N400 is proportional to difficulty and indexes the efforts deployed by the brain to integrate the meaning of a stimulus in its context. While this view accounts for many literature data, its shortcomings are also pointed out. The need for various inhibition processes is then emphasized together with the idea that integration difficulty may vary with the amount of inhibition to be performed. Consequently, I stress the view that N400 amplitude could index inhibition rather than integration. The fact that, when presented with incongruous sentence endings, subjects are aware of the meanings of both the sentence frame and the ending has an important consequence on this N400 inhibition view. It means that disconfirmed contexts are not inhibited in some conditions, such as when incongruency is relevant in the situation. To account for this fact, the inhibition the N400 could index has to start only once this relevance is coded, that is, at or after the activation of situational representations. We then show that the idea that the N400 reflects such a late inhibition process may account for literature data that are difficult to account for by the integration view. Further studies are thus needed to test this inhibition idea.},
  keywords = {Activation,Distractor,event-related potential,Inhibition,Integration,Knowledge,N400},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/3J4J4P8Z/S0165017307002202.html}
}

@article{rabovsky_modelling_2018,
  title = {Modelling the {{N400}} Brain Potential as Change in a Probabilistic Representation of Meaning},
  author = {Rabovsky, Milena and Hansen, Steven S. and McClelland, James L.},
  year = {2018},
  month = sep,
