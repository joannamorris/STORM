  pages = {340--347},
  issn = {0033-2909},
  doi = {10.1037/0033-2909.109.2.340},
  urldate = {2017-01-23},
  abstract = {The shape of a reaction time (RT) distribution can be described by a 3-parameter model consisting of the convolution of the normal and exponential distributions, the ex-Gaussian. Analyses based on mean RT do not take the distribution's shape into account and, for that reason, may obscure aspects of performance. To illustrate the point, the ex-Gaussian model was applied to data obtained from a Stroop task. Mean RT revealed strong interference but no facilitation, whereas the analysis based on the ex-Gaussian model showed both interference and facilitation. In short, analyses that do not take the shape of RT distributions into account can mislead and, therefore, should be avoided. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
  keywords = {1991,Reaction Time,shape of RT distribution in Stroop task,Stroop Effect},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/ZVDVTK4I/Heathcote et al. - 1991 - Analysis of response time distributions An exampl.pdf}
}

@article{hedrick_justifications_1988,
  title = {Justifications for the Sharing of Social Science Data},
  author = {Hedrick, Terry E.},
  year = {1988},
  journal = {Law and Human Behavior},
  volume = {12},
  number = {2},
  pages = {163--171},
  issn = {1573-661X(Electronic);0147-7307(Print)},
  doi = {10.1007/BF01073124},
  abstract = {Discusses justifications for data sharing, starting with the premise that voluntary sharing of social science data is a desirable professional behavior. These justifications are grounded in the need for science that is more accurate, more open, more knowledgeable, and has more resources at its disposal. Psychology's involvement in data sharing is examined. It is argued that the costs and benefits of data sharing are now unevenly distributed and that primary researchers bear most of the burden and risks. Adoption of data-sharing norm in the social sciences would help to remedy this imbalance. The author supports the 1985 data-sharing recommendations put forth by the Committee on National Statistics.},
