  keywords = {Design,Graphics,Iconic displays,Relational displays,Visual displays,Visualization},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/6U7VUVQD/j.1756-8765.2011.01150.html}
}

