@article{rueckl_universal_2015,
  title = {Universal Brain Signature of Proficient Reading: {{Evidence}} from Four Contrasting Languages},
  shorttitle = {Universal Brain Signature of Proficient Reading},
  author = {Rueckl, Jay G. and {Paz-Alonso}, Pedro M. and Molfese, Peter J. and Kuo, Wen-Jui and Bick, Atira and Frost, Stephen J. and Hancock, Roeland and Wu, Denise H. and Mencl, William Einar and Du{\~n}abeitia, Jon Andoni and Lee, Jun-Ren and Oliver, Myriam and Zevin, Jason D. and Hoeft, Fumiko and Carreiras, Manuel and Tzeng, Ovid J. L. and Pugh, Kenneth R. and Frost, Ram},
  year = {2015},
