@article{prasada_conceiving_2002-1,
  title = {Conceiving of Entities as Objects and as Stuff},
  author = {Prasada, Sandeep and Ferenz, Krag and Haskell, Todd},
  year = {2002},
  journal = {Cognition},
  volume = {83},
  number = {2},
  pages = {141--165},
  abstract = {What is the difference between conceiving of an entity as an object of some kind and conceiving of it as an amount of solid stuff? This article proposes that the difference lies in how one thinks about the entity's structure. Object construals require thinking of the structure as being nonarbitrary, whereas substance construals require thinking of the structure as being arbitrary. Six experiments are reported on, providing empirical support for this proposal. Regularity of structure, repetition of structure, and the existence of structure-dependent functions, all of which provide reasons to consider the structure of an entity to be nonarbitrary, were shown to bias participants towards object construals. This paper also discusses how the proposed account of what it means to construe an entity as an object or as some stuff can account for a range of findings in the literature on lexical development. These include the relation between cohesiveness and ontological category, shape and ontological category, and complexity of shape and ontological category. Finally, the nature of construals and the relation of object and substance construals to the physical, design, and intentional stances are discussed. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Cognitive,(Language),[2340].,[2720].,*Classification,*Classification (Cognitive Process),*Concept,*Concept Formation,*Concepts,*Morphology,*Morphology (Language),*Semantics,\&,Adulthood,Cognitive,Cognitive Processes,Cognitive Processes [2340]. Linguistics \& Language,Formation,Human.,Human. Adulthood (18 yrs \& older).,Language,Linguistics,older).,Process),Processes,Speech,yrs},
  annotation = {Elsevier Science, Netherlands Empirical Study. Journal Article. English}
}

@article{prasada_generalisation_1993,
  title = {Kanji Recognition and Semantic Priming at Morphological and Semantic Levels: {{An}} Event-Related Brain Potential Study},
  author = {Yao, Peng-peng and Morotomi, Takashi and Okita, Tsunetaka},
  year = {2000},
  journal = {Japanese Journal of Physiological Psychology and Psychophysiology},
  volume = {18},
  number = {3},
  pages = {231--246},
  issn = {0289-2405},
  abstract = {To investigate the relation between levels of processing in Kanji recognition and the loci where the semantic priming occurs, event-related brain potentials (ERPs) to Kanji characters were recorded in two task conditions of semantic matching and non-task. The stimuli were presented in AAAAABAA form (habituation paradigm), and the repetition Kanji A and test Kanji B were semantically related, unrelated, or identical. In the non-task condition, the semantic priming effect was observed in a positive wave with a 200-ms peak latency, P200 (P2). In the semantic matching task, where the subjects were required to decide whether or not the repetition and test Kanji characters were semantically related, the priming effect was obtained as the attenuation of the later negative wave N310 (corresponding to N400) for the related test Kanji, instead of P200. The P200 priming effect was interpreted as the semantically related response at the morphological level, whereas the N310 effect was taken as reflection of the semantic-level activation of the semantic associates with the preceding Kanji. The task dependency of semantic priming levels indicated the top-down control in Kanji recognition processes. (PsycINFO Database Record (c) 2006 APA, all rights reserved).},
  keywords = {[2530].,*Brain,*Evoked,*Evoked Potentials,*Semantic,*Semantic Priming,*Semantics,Electrophysiology,Electrophysiology [2530].,Human,Potentials,Priming}
}

@article{prat_individual_2007,
