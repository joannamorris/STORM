@article{orourke_electrophysiological_2002-1,
  title = {Electrophysiological Evidence for the Efficiency of Spoken Word Processing},
  author = {O'Rourke, Timothy B. and Holcomb, Philip J.},
  year = {2002},
  journal = {Biological Psychology},
  volume = {60},
  number = {2-3},
  pages = {121--150},
  abstract = {The Cohort model (W. D. Marslen-Wilson, 1987) proposes that spoken words are frequently recognized at the first point in the acoustic signal where a unique lexical representation is specified. This proposal was tested in two experiments. In experiment 1, 30 participants (18 females and 12 males, aged 18-22 yrs) made speeded lexical decisions to spoken words and pseudowords. In experiment 2, 30 participants (13 females and 17 males, aged 18-22 yrs) passively listened without making overt responses. In both experiments the recognition points for words (the point past which no other lexical item was consistent with the acoustic signal) and deviation points for pseudowords (the point past which no real word is compatible with the acoustic signal) were manipulated. An event-related potentials negativity in the region of the N400 component and reaction time (RT) occurred sooner for items with early than late recognition/deviation points. In experiment 1, early and late words produced N400s and RTs with indistinguishable latencies, while late pseudowords produced faster RTs and earlier N400s than early pseudowords. Experiment 2 replicated the N400 effects for words, but only produced a trend in the same direction as experiment 1 for pseudowords. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,(Phonetic,[2326].,*Auditory,*Auditory Evoked Potentials,*Lexical,*Lexical Decision,*Speech,*Speech Perception,*Word,*Word Recognition,*Words,*Words (Phonetic Units),\&,Adulthood,Auditory,Auditory \& Speech Perception [2326].,Decision,Electrophysiology,Evoked,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older). Y,Male.,older).,Perception,Potentials,Recognition,Speech,Units),Young,yrs,yrs).},
  annotation = {Elsevier Science, Netherlands Empirical Study. Journal Article. English}
}

@article{orsolini_universals_1997,
  ids = {goodman_inseparability_1997-1},
  title = {On the {{Inseparability}} of {{Grammar}} and the {{Lexicon}}: {{Evidence}} from {{Acquisition}}, {{Aphasia}} and {{Real-time Processing}}},
  shorttitle = {On the {{Inseparability}} of {{Grammar}} and the {{Lexicon}}},
  author = {Goodman, Elizabeth Bates Judith C.},
  year = {1997},
  month = oct,
  journal = {Language and Cognitive Processes},
  volume = {12},
  number = {5-6},
  pages = {507--584},
  publisher = {{Routledge}},
  issn = {0169-0965},
  doi = {10.1080/016909697386628},
  urldate = {2020-08-28},
  abstract = {Within linguistic theory, many phenomena that were previously handled by a separate grammatical component have been moved into the lexicon; in some theories, the contrast between grammar and the lexicon has disappeared altogether. In a review of findings from language development, language breakdown and real-time processing, we conclude that the case for a modular distinction between grammar and the lexicon has been overstated, and that the evidence to date is compatible with a unified lexicalist account. Studies of normal children show that the emergence of grammar is highly dependent upon vocabulary size, a finding confirmed and extended in atypical populations. Studies of language breakdown in older children and adults provide no evidence for a modular dissociation between grammar and the lexicon; some structures are especially vulnerable to brain damage (e.g. function words, non-canonical word orders), but this vulnerability is also observed in neurologically intact individuals under perceptual degradation or cognitive overload. Finally, on-line studies provide evidence for early and intricate interactions between lexical and grammatical information in normal adults. A possible characterization of this unified lexical processor is offered, based on distributed representations in recurrent neural networks.},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/H8C2Y62J/Goodman - 1997 - On the Inseparability of Grammar and the Lexicon .pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/J5ZHTIBM/Goodman - 1997 - On the Inseparability of Grammar and the Lexicon .pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/AI3AZMYG/016909697386628.html;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/EHH34DB3/016909697386628.html}
}

@article{ortony_beyond_1979,
  title = {{Beyond literal similarity}},
  author = {Ortony, Andrew},
  year = {1979},
  month = may,
  journal = {Psychological Review},
  volume = {86},
  number = {3},
  pages = {161--180},
  issn = {0033-295X},
  doi = {10.1037/0033-295X.86.3.161},
  urldate = {2018-04-20},
  abstract = {Previous theories of similarity have restricted themselves to judgments of what might be called literal similarity. The argument presented here is that a complete account of similarity needs also to be sensitive to nonliteralness, or metaphoricity, an aspect of similarity statements that is most evident in similes but that actually underlies metaphorical language in general. The claim that metaphoricity can be represented in terms of the relative degrees of salience of matching (or matchable) attributes of the 2 terms in a comparison is advanced. A modification of A. Tversky's (1977) account of similarity is proposed, and its implications for similarity statements and for the psychological processes involved in their comprehension are discussed. It is argued that the general account of similarity proposed, including as it does nonliteral similarity, can form not only the basis of a theory of metaphor but can also give a credible account of the relationship between metaphor, analogy, and similarity. (39 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  langid = {English (US)},
  keywords = {Language,Theories,Verbal Meaning},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/A3NJ2DV6/Ortony - 1979 - Beyond literal similarity.pdf;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/ICRZSJAK/beyond-literal-similarity.html}
}

