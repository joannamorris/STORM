@article{osterhout_language_1996-1,
  title = {On the Language Specificity of the Brain Response to Syntactic Anomalies: {{Is}} the Syntactic Positive Shift a Member of the {{P300}} Family?},
  author = {Osterhout, Lee and McKinnon, Richard and Bersick, Michael and Corey, Vicka},
  year = {1996},
  journal = {Journal of Cognitive Neuroscience},
  volume = {8},
  number = {6},
  pages = {507--526},
  abstract = {Examined whether the event related potential response to syntactic violations is distinct from that elicited by unexpected, task-relevant anomalies that do not involve grammatical violation. In 3 experiments, ERPs were recorded from the scalp electrodes of 52 18-42 yr old Subjects reading sentences, some of which contained either a verb disagreeing in number from the subject noun (syntactic anomaly) or a word in uppercase letters (physical anomaly). Uppercase words elicited a P300 complex of positivities, whereas agreement violations elicited a late positive shift. These effects differed in their morphology, temporal course, amplitude, and scalp distribution. Manipulations of the probability-of-occurrence and task relevance of the anomalies had robust effects on the response to uppercase words, but not on the response to agreement violations. These anomalies had additive effects when agreement-violating uppercase (doubly anomalous) words were presented. These results indicate that the positive shift elicited by agreement violations is distinct from the P300 response to unexpected, task-relevant anomalies that do not involve the violation of a grammatical rule. ((c) 1997 APA/PsycINFO, all rights reserved)},
  keywords = {Adulthood,Evoked,Evoked Potentials,Potentials,Syntax},
  annotation = {ARTICLE U Washington, Dept of Psychology, Seattle, WA, USA 1996 Nov}
}

@article{osterhout_role_1989,
  title = {Changes in the Dendritic Branching of Adult Mammalian Neurones Revealed by Repeated Imaging in Situ},
  author = {Purves, Dale and Hadley, Robert D.},
  year = {1985},
  month = may,
  journal = {Nature},
  volume = {315},
  number = {6018},
  pages = {404--406},
  publisher = {{Nature Publishing Group}},
  issn = {1476-4687},
  doi = {10.1038/315404a0},
  urldate = {2021-11-04},
  abstract = {A major obstacle to understanding the mechanism of long-term change in the vertebrate nervous system has been the inability to observe the same nerve cell at different times during the life of an animal. The possibility that changes in neural connectivity underlie the remarkable flexibility of the nervous systems of mammals has therefore not been tested by direct observation. Here, we report studies in which we have visualized the same neurone in the superior cervical ganglion of young adult mice at intervals of up to 33 days. This collection of nerve cells is particularly accessible and therefore well suited to our approach. We find that the dendritic branches of the neurones examined change appreciably over intervals, of 2 weeks or more; some branches retract, others elongate and others seem to form de novo. The apparent remodelling of these postsynaptic elements implies that the synaptic connections of these cells normally undergo significant rearrangement beyond what is usually considered to be the developmental period.},
  copyright = {1985 Nature Publishing Group},
  langid = {english},
  keywords = {Humanities and Social Sciences,multidisciplinary,Science},
